    <method name="initialize"><![CDATA[importPackage(Packages.com.ibm.tivoli.maximo.report.script);

mxReportScriptContext = MXReportScriptContext.initialize(reportContext);

isContLabor = false;
isContLease = false;
isContMaster = false;
isContPurch = false;
isContWarranty = false;

if (!params["paramdelimiter"].value)
{
	params["paramdelimiter"] = "||";
}
if (!params["paramstring"].value &amp;&amp; params["appname"].value)
{
	params["paramstring"] = "appHierarchy="+params["appname"];
}
if (params["paramstring"].value)
{
 	if (MXReportUtil.isInAppHierarchy("CONTLABOR", params["paramstring"], params["paramdelimiter"]))
	{
		isContLabor = true;
	}
	 else if (MXReportUtil.isInAppHierarchy("CONTLEASE", params["paramstring"], params["paramdelimiter"]))
	{
		isContLease = true;
	}
	 else if (MXReportUtil.isInAppHierarchy("CONTMASTER", params["paramstring"], params["paramdelimiter"]))
	{
		isContMaster = true;
	}
	else if (MXReportUtil.isInAppHierarchy("CONTPURCH", params["paramstring"], params["paramdelimiter"]))
	{
		isContPurch = true;
	}
	else if (MXReportUtil.isInAppHierarchy("CONTWARRTY", params["paramstring"], params["paramdelimiter"]))
	{
		isContWarranty = true;
	}
}]]></method>
