            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();
var where = params["where"].value;
var fromjoin = "";

	if (isContPurch) {
		fromjoin = ", purchview";
		where = " purchview.contractnum in (select purchview.contractnum from purchview where " + where + ")";
		where = where + " and contract.contractnum = purchview.contractnum";
		where = where + " and contract.revisionnum = purchview.revisionnum";
		where = where + " and contract.orgid = purchview.orgid";
	}
	else if (isContLease) {
		fromjoin = ", leaseview";
		where = where + " and contract.contractnum = leaseview.contractnum";
		where = where + " and contract.revisionnum = leaseview.revisionnum";
		where = where + " and contract.orgid = leaseview.orgid";
	}
	else if (isContMaster) {
		fromjoin = ", masterview";
		where = where + " and contract.contractnum = masterview.contractnum";
		where = where + " and contract.revisionnum = masterview.revisionnum";
		where = where + " and contract.orgid = masterview.orgid";
	}
	else if (isContLabor) {
		fromjoin = ", laborview";
		where = where + " and contract.contractnum = laborview.contractnum";
		where = where + " and contract.revisionnum = laborview.revisionnum";
		where = where + " and contract.orgid = laborview.orgid";
	}
	else if (isContWarranty) {
		fromjoin = ", warrantyview";
		where = where + " and contract.contractnum = warrantyview.contractnum";
		where = where + " and contract.revisionnum = warrantyview.revisionnum";
		where = where + " and contract.orgid = warrantyview.orgid";
	}

sqlText = ""
+ "select contract.orgid, contract.contracttype, contract.contractnum, contract.description,"
+ "       contract.revisionnum, contract.status, contract.startdate, contract.enddate,"
+ "       contract.vendor, contract.purchaseagent"
+ "  from contract" + fromjoin
+ " where " + where
+ " order by contract.startdate desc, contract.contractnum, contract.revisionnum desc"
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["orgid"] = maximoDataSet.getString("orgid");	
row["contracttype"] = maximoDataSet.getString("contracttype");	
row["contractnum"] = maximoDataSet.getString("contractnum");	
row["description"] = maximoDataSet.getString("description");	
row["revisionnum"] = maximoDataSet.getString("revisionnum");	
row["status"] = maximoDataSet.getString("status");	
row["startdate"] = maximoDataSet.getDate("startdate");	
row["enddate"] = maximoDataSet.getDate("enddate");	
row["vendor"] = maximoDataSet.getString("vendor");	
row["purchaseagent"] = maximoDataSet.getString("purchaseagent");	

return (true);]]></method>
