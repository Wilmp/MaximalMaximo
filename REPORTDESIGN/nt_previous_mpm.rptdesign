            <property name="dataSource">maximoDataSource</property>
            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//MPI 31-07-2015: S29.FRO05: New report gives overview between relations in masterpm's

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " "
+ " select"
+ "   masterpm.masterpmnum,"
+ "   masterpm.description," 
+ "   mpmprev.masterpmnum as prevpm,"
+ "   mpmprev.description as prevpmdesc,"
+ "   alf2.actiontype as prevactiontype"
+ " from masterpm"
+ "   left outer join plusalfpmaction alf2 on alf2.secondarypmid = masterpm.masterpmid" 
      //and alf2.actiontype = 'ACTIVATE'"
+ "   left outer join masterpm mpmprev on mpmprev.masterpmid = alf2.primarypmid"
+ " where " + params["where"]
+ " order by masterpm.masterpmnum, mpmprev.masterpmnum"
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["masterpmnum"] = maximoDataSet.getString("masterpmnum");	
row["description"] = maximoDataSet.getString("description");	
row["prevpm"] = maximoDataSet.getString("prevpm");	
row["prevpmdesc"] = maximoDataSet.getString("prevpmdesc");	
row["prevactiontype"] = maximoDataSet.getString("prevactiontype");	

return (true);]]></method>
