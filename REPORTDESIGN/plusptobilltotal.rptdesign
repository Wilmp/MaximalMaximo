            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();



// Add query to sqlText variable.
sqlText = "select " +
"pluspbillbatch.agreement, " +
"pluspcustomer.name, " +
"pluspcustomer.billtostreetadd, pluspcustomer.billtocity, pluspcustomer.billtostateprov, pluspcustomer.billtocountry, pluspcustomer.billtopostalcode, " +
"person.displayname, " +
"phone.phonenum, " +
"min(pluspbillbatch.billenddate) as minenddate, " +
"max(pluspbillbatch.billenddate) as maxenddate, " +
"sum(pluspbillbatch.pretaxtotal) as pretaxtotalsum, " +
"pluspcustomer.currencycode, " +
"pluspagreement.orgid, " +
"pluspagreement.paymentterms " +
"from pluspbillbatch " +
"left outer join pluspagreement on pluspbillbatch.orgid=pluspagreement.orgid and pluspbillbatch.agreement=pluspagreement.agreement " +
"left outer join pluspcustomer on pluspcustomer.customer=pluspagreement.customer " +
"left outer join pluspcustcontact on pluspcustomer.customer = pluspcustcontact.customer and pluspcustcontact.isprimary=1 " +
"left outer join person on person.personid=pluspcustcontact.personid " +
"left outer join phone on phone.personid=pluspcustcontact.personid " +
"where " +
"pluspbillbatch.status in (select value from synonymdomain where domainid='PLUSPBILLBATCHST' and (maxvalue='INPROGRESS' or maxvalue='PREBILL')) " +
"and pluspagreement.status in (select value from synonymdomain where domainid='PLUSPAGREESTATUS' and maxvalue='APPR') " +

"and " + params["where"] +

" group by " +
"pluspbillbatch.agreement, " +
"pluspcustomer.name, " +
"pluspcustomer.billtostreetadd, pluspcustomer.billtocity, pluspcustomer.billtostateprov, pluspcustomer.billtocountry, pluspcustomer.billtopostalcode, " +
"person.displayname, " +
"phone.phonenum, " +
"pluspcustomer.currencycode, " +
"pluspagreement.orgid, " +
"pluspagreement.paymentterms "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["agreement"] = maximoDataSet.getString("agreement");
row["customername"] = maximoDataSet.getString("name");
row["customercontact"] = maximoDataSet.getString("displayname");
row["customercontactphone"] = maximoDataSet.getString("phonenum");
row["billingperiodstart"] = maximoDataSet.getDate("minenddate");
row["billingperiodend"] = maximoDataSet.getDate("maxenddate");
row["currencycode"] = maximoDataSet.getString("currencycode");
row["orgid"] = maximoDataSet.getString("orgid");

// Billing Address info
row["billtostreetadd"] = (maximoDataSet.getString("billtostreetadd") == null)? "" : maximoDataSet.getString("billtostreetadd");
row["billtocity"] = (maximoDataSet.getString("billtocity") == null)? "" : maximoDataSet.getString("billtocity");
row["billtostateprov"] = (maximoDataSet.getString("billtostateprov") == null)? "" : maximoDataSet.getString("billtostateprov");
row["billtocountry"] = (maximoDataSet.getString("billtocountry") == null)? "" : maximoDataSet.getString("billtocountry");
row["billtopostalcode"] = (maximoDataSet.getString("billtopostalcode") == null)? "" : maximoDataSet.getString("billtopostalcode");
row["billingaddress"] = row["billtostreetadd"].concat(" ").concat(row["billtocity"]).concat(" ").concat(row["billtostateprov"]).concat(" ").concat(row["billtocountry"]).concat(" ").concat(row["billtopostalcode"]);



// Calculation of due date based on payment term.
// It is an ALN domain, so user can insert anything.
// The report will only handle those starting with N, ending with D and having numbers between.
paymentTerm = maximoDataSet.getString("paymentterms");
if (paymentTerm != null &amp;&amp; paymentTerm.startsWith("N") &amp;&amp; paymentTerm.endsWith("D")) {
	row["duedate"] = BirtDateTime.addDay(BirtDateTime.today(), parseInt(paymentTerm.substring(1, paymentTerm.length() - 2)));
} else {
	row["duedate"] = BirtDateTime.today();
}



// Bill Schedule Total
bsTotalDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "bsTotalDataSet");
bsTotalDataSet.open();
bsTotalSQL = "select " +
"sum(billedprice) as sumbilledprice " +
"from pluspbillline " +
"where pricesched is null " +
"and billbatchnum in " +
"( " +
"   select " +
"   billbatchnum " +
"   from pluspbillbatch " +
"   where status in (select value from synonymdomain where domainid='PLUSPBILLBATCHST' and (maxvalue='INPROGRESS' or maxvalue='PREBILL')) " +
"   and " + MXReportSqlFormat.createParamWhereClause("agreement", "=" + maximoDataSet.getString("agreement")) +
"   and " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid")) +
") " +
"and " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid"));
bsTotalDataSet.setQuery(bsTotalSQL);
bsTotalDataSet.fetch();



// Price Schedule Total
var psBilledTotal;
psTotalDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "psTotalDataSet");
psTotalDataSet.open();
psTotalSQL = "select " +
"sum(billedprice) as sumbilledprice " +
"from pluspbillline " +
"where pricesched is not null " +
"and billbatchnum in " +
"( " +
"   select " +
"   billbatchnum " +
"   from pluspbillbatch " +
"   where status in (select value from synonymdomain where domainid='PLUSPBILLBATCHST' and (maxvalue='INPROGRESS' or maxvalue='PREBILL')) " +
"   and " + MXReportSqlFormat.createParamWhereClause("agreement", "=" + maximoDataSet.getString("agreement")) +
"   and " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid")) +
") " +
"and " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid"));
psTotalDataSet.setQuery(psTotalSQL);
psTotalDataSet.fetch();



// Retrieve currency code from organization
orgDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "orgDataSet");
orgDataSet.open();
	
orgSQL = "select basecurrency1 from organization where " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid"));
orgDataSet.setQuery(orgSQL);
orgDataSet.fetch();

// If customer currency is same as organizaton, just set value
if (orgDataSet.getString("basecurrency1").equals(maximoDataSet.getString("currencycode"))) {
	row["totaldue"] = maximoDataSet.getDouble("pretaxtotalsum");
	row["bsbilledtotal"] = bsTotalDataSet.getDouble("sumbilledprice");
	row["psbilledtotal"] = psTotalDataSet.getDouble("sumbilledprice");
	row["agmtbilledtotal"] = bsTotalDataSet.getDouble("sumbilledprice") + psTotalDataSet.getDouble("sumbilledprice");
	orgDataSet.close();
	bsTotalDataSet.close();
	psTotalDataSet.close();
	return (true);
}

// If not, retrieve exchange rate to calculate total due
exchangeDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "exchangeDataSet");
exchangeDataSet.open();
	
exchangeSQL = 
"select exchangerate "
+ "from exchange "
+ "where currencycode = "
+ "( "
+ "   select basecurrency1 from organization where " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + maximoDataSet.getString("orgid"))
+ ") "
+ "and " + MXReportSqlFormat.createParamWhereClause("currencycodeto", "=" + maximoDataSet.getString("currencycode"))
+ "and activedate <= " + MXReportSqlFormat.getCurrentTimestampFunction()
+ "and expiredate > " + MXReportSqlFormat.getCurrentTimestampFunction()
;

// Calculate Total Due using exchange rate. If there is no exchange rate,
// use currency code and amount of organization.
exchangeDataSet.setQuery(exchangeSQL);
if (exchangeDataSet.fetch()) {
	row["totaldue"] = maximoDataSet.getDouble("pretaxtotalsum") * exchangeDataSet.getDouble("exchangerate");
	row["bsbilledtotal"] = bsTotalDataSet.getDouble("sumbilledprice") * exchangeDataSet.getDouble("exchangerate");
	row["psbilledtotal"] = psTotalDataSet.getDouble("sumbilledprice") * exchangeDataSet.getDouble("exchangerate");
	row["agmtbilledtotal"] = (bsTotalDataSet.getDouble("sumbilledprice") + psTotalDataSet.getDouble("sumbilledprice")) * exchangeDataSet.getDouble("exchangerate");
} else {
	row["totaldue"] = maximoDataSet.getDouble("pretaxtotalsum");
	row["currencycode"] = orgDataSet.getString("basecurrency1");
	row["bsbilledtotal"] = bsTotalDataSet.getDouble("sumbilledprice");
	row["psbilledtotal"] = psTotalDataSet.getDouble("sumbilledprice");
	row["agmtbilledtotal"] = bsTotalDataSet.getDouble("sumbilledprice") + psTotalDataSet.getDouble("sumbilledprice");
}

orgDataSet.close();
exchangeDataSet.close();
bsTotalDataSet.close();
psTotalDataSet.close();

return (true);]]></method>
