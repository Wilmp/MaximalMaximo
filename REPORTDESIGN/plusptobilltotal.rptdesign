            <method name="fetch"><![CDATA[if (!bsDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["bs_bill"] = bsDataSet.getString("billbatchnum");
row["bs_billdescription"] = bsDataSet.getString("description");
row["bs_billline"] = bsDataSet.getString("linenum");
row["bs_order"] = bsDataSet.getString("ordernum");
row["bs_orderclass"] = bsDataSet.getString("objectclass");
row["bs_billedprice"] = bsDataSet.getDouble("billedprice");


// Bill line description is nonpersistent, must be retrieved from order
orderDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "orderDataSet");
orderDataSet.open();

var orderSQL = new String();
if (bsDataSet.getString("woclass") != null) {
	orderSQL = "select description from workorder where " + MXReportSqlFormat.createParamWhereClause("siteid", "=" + bsDataSet.getString("siteid")) + "and " + MXReportSqlFormat.createParamWhereClause("wonum", "=" + bsDataSet.getString("refwo"));
} else if (bsDataSet.getString("ticketclass") != null) {
	orderSQL = "select description from ticket where " + MXReportSqlFormat.createParamWhereClause("ticketid", "=" + bsDataSet.getString("ticketid"));
} else {
	orderSQL = "select description from pluspsalesorder where " + MXReportSqlFormat.createParamWhereClause("siteid", "=" + bsDataSet.getString("siteid")) + "and " + MXReportSqlFormat.createParamWhereClause("salesordernum", "=" + bsDataSet.getString("salesordernum"));
}
orderDataSet.setQuery(orderSQL);

if (orderDataSet.fetch()) {
	row["bs_billlinedescription"] = orderDataSet.getString("description");
}

orderDataSet.close();



// Retrieve currency code from organization
orgDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "orgDataSet");
orgDataSet.open();
	
orgSQL = "select basecurrency1 from organization where " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + bsDataSet.getString("orgid"));
orgDataSet.setQuery(orgSQL);
orgDataSet.fetch();

// If customer currency is same as organizaton, just set value
if (orgDataSet.getString("basecurrency1").equals(bsDataSet.getString("currencycode"))) {
	row["bs_billedprice"] = bsDataSet.getDouble("billedprice");
	orgDataSet.close();
	return (true);
}

// If not, retrieve exchange rate to calculate total due
exchangeDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "exchangeDataSet");
exchangeDataSet.open();
	
exchangeSQL = 
"select exchangerate "
+ "from exchange "
+ "where currencycode = "
+ "( "
+ "   select basecurrency1 from organization where " + MXReportSqlFormat.createParamWhereClause("orgid", "=" + bsDataSet.getString("orgid"))
+ ") "
+ "and " + MXReportSqlFormat.createParamWhereClause("currencycodeto", "=" + bsDataSet.getString("currencycode"))
+ "and activedate <= " + MXReportSqlFormat.getCurrentTimestampFunction()
+ "and expiredate > " + MXReportSqlFormat.getCurrentTimestampFunction()
;

// Calculate Total Due using exchange rate. If there is no exchange rate,
// use currency code and amount of organization.
exchangeDataSet.setQuery(exchangeSQL);
if (exchangeDataSet.fetch()) {
	row["bs_billedprice"] = bsDataSet.getDouble("billedprice") * exchangeDataSet.getDouble("exchangerate");	
} else {
	row["bs_billedprice"] = bsDataSet.getDouble("billedprice");
}

orgDataSet.close();
exchangeDataSet.close();



return (true);]]></method>
