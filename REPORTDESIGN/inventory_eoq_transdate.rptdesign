            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();
var avgcostSql = new String();
var where = new String();
where = "";

var currDate = new Date();

var request = reportContext.getHttpServletRequest();
if(request != null){
	var session = request.getSession();
	session.setAttribute(dskey, this.getDataSource().getName());
}

if( BirtComp.notEqual(params["storeroom"],null) &amp;&amp; BirtComp.notEqual(params["storeroom"], "") ) {
	where += " and " + MXReportSqlFormat.createParamWhereClause("inventory.location", params["storeroom"].toUpperCase());
}

if( BirtComp.notEqual(params["site"], null) &amp;&amp; BirtComp.notEqual(params["site"], "") ) {
	where += " and " + MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"].toUpperCase());
}

if(maximoDataSet.isSQLServer()){
	avgcostSql = " (case when mut1.mut_qty = 0 then 0 else (cast(mut1.mut_sumlinecost as decimal(19,4))/cast(mut1.mut_qty as decimal(19,4))) end) as avgcost "
	;
} else if(maximoDataSet.isOracle()){
	avgcostSql = " (case when mut1.mut_qty = 0 then 0 else (cast(mut1.mut_sumlinecost as decimal(19,4))/cast(mut1.mut_qty as decimal(19,4))) end) as avgcost "
	;
} else {
	avgcostSql = " (case when mut1.mut_qty = 0 then 0 else (decimal(mut1.mut_sumlinecost,19,4)/decimal(mut1.mut_qty,19,4)) end) as avgcost "
	;
}

params["paramdelimiter"] = "||";
params["paramstring"] = attkey + params["paramdelimiter"] + dskey;

// Add query to sqlText variable.
sqlText = "select inventory.siteid, inventory.location, inventory.costtype, inventory.itemnum, inventory.itemsetid, item.description, inventory.orderunit, inventory.orderqty, mut1.mut_qty as quantity, "
+ avgcostSql
+ " from item, inventory "
+ " left outer join ( "
+ " select matusetrans.itemnum, matusetrans.siteid, matusetrans.itemsetid, matusetrans.storeloc, coalesce(sum(matusetrans.quantity),0) as mut_qty, " 
+ " coalesce(sum(matusetrans.linecost),0) as mut_sumlinecost "
+ " from matusetrans "
+ " where matusetrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
+ " and matusetrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
+ " group by matusetrans.itemnum, matusetrans.siteid, matusetrans.itemsetid, matusetrans.storeloc "
+ " ) mut1 on "	
+ " mut1.siteid = inventory.siteid "
+ " and mut1.itemnum = inventory.itemnum "
+ " and mut1.itemsetid = inventory.itemsetid "
+ " and mut1.storeloc = inventory.location "					 
+ " where inventory.itemnum = item.itemnum " 
+ " and inventory.itemsetid = item.itemsetid "
+ " and item.itemtype in (select value from synonymdomain where domainid='ITEMTYPE' and maxvalue='ITEM') "
+ " and inventory.status not in (select value from synonymdomain where domainid='ITEMSTATUS' and maxvalue='OBSOLETE') "
+ " and item.status not in (select value from synonymdomain where domainid='ITEMSTATUS' and maxvalue='OBSOLETE') "
+ where 
+ " and inventory.orderunit is not null " 
+ " order by inventory.itemnum asc "
;

maximoDataSet.setQuery(sqlText);
]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.

row["itemnum"] = maximoDataSet.getString("itemnum");
row["description"] = maximoDataSet.getString("description");
row["orderunit"] = maximoDataSet.getString("orderunit");
row["costtype"] = maximoDataSet.getString("costtype");
row["previouseoq"] = maximoDataSet.getInteger("orderqty");

var differenceInDays = ((params["enddate"].getTime() - params["startdate"].getTime())/(1000*60*60*24));
var differenceInMonths = (differenceInDays/30.44);

var numeratorEOQCalc = 2 * (maximoDataSet.getInteger("quantity")/differenceInMonths) * params["ordCost"];
var denominatorEOQCalc = maximoDataSet.getFloat("avgcost") * (params["cryCost"].value/100);
var recommendedeoq = Math.round(Math.sqrt(BirtMath.safeDivide(numeratorEOQCalc,denominatorEOQCalc,1)));

		if (scriptLogger.isDebugEnabled()) {
			scriptLogger.debug("["+ maximoDataSet.getName() + "] >>> recommendedeoq : " + recommendedeoq);
		}
		
if ((recommendedeoq < 1) || isNaN(recommendedeoq)) {
	recommendedeoq = 1;
}

row["recommendedeoq"] = recommendedeoq;

var diffeoq = recommendedeoq - maximoDataSet.getInteger("orderqty");

if (diffeoq >= 0) {
	row["eoqincrease"] = Math.abs(diffeoq);
} else {
	row["eoqdecrease"] = Math.abs(diffeoq);
}

var request = reportContext.getHttpServletRequest();
if(request != null) {
	var session = request.getSession();
	var updStmtList = session.getAttribute(attkey);

	if(updStmtList==null){
		updStmtList = new Array;
	}
	
	var strSite = "";
	var strItem = "";
	var strItemset = "";
	var strLocation = "";
		
	if( BirtComp.notEqual(maximoDataSet.getString("siteid"),null)) {
		strSite = " = '" + maximoDataSet.getString("siteid") + "'";
	} else {
		strSite = " is null";
	}
		
	if( BirtComp.notEqual(maximoDataSet.getString("itemnum"),null)) {
		strItem = " = '" + maximoDataSet.getString("itemnum") + "'";
	} else {
		strItem = " is null";
	}
		
	if( BirtComp.notEqual(maximoDataSet.getString("itemsetid"),null)) {
		strItemset = " = '" + maximoDataSet.getString("itemsetid") + "'";
	} else {
		strItemset = " is null";
	}
	
	if( BirtComp.notEqual(maximoDataSet.getString("location"),null)) {
		strLocation = " = '" + maximoDataSet.getString("location") + "'";
	} else {
		strLocation = " is null";
	}

	var updateQuery = "update inventory set orderqty=" + recommendedeoq 
		+ " where siteid " + strSite
		+ " and itemsetid " + strItemset
		+ " and location " + strLocation
		+ " and itemnum " + strItem
	;
	updStmtList.push(updateQuery);
	
	session.setAttribute(attkey, updStmtList);
}

return (true);]]></method>
