row["serialnum"] = maximoDataSet.getString("serialnum");	
row["parent"] = maximoDataSet.getString("parent");
row["wodesc"] = maximoDataSet.getString("wodesc");
row["status"] = maximoDataSet.getString("status");
row["taskid"] = maximoDataSet.getString("taskid");
row["taskdesc"] = maximoDataSet.getString("taskdesc");
row["assignstatus"] = maximoDataSet.getString("assignstatus");
row["laborcode"] = maximoDataSet.getString("laborcode");
row["displayname"] = maximoDataSet.getString("displayname");
//row["textfield"] = maximoDataSet.getString("textfield");

return (true);]]></method>
