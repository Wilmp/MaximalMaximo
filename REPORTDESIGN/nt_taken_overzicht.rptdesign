            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//MPI 22-07-2013: Elementtool 3147
//1) wmassignment and person LEFT joined
//2) restriction laborcode not null commented out

//MPI 12-08-2013: Elementtool 2332 toevoegen kolom spoor (asset.ntloctrack) NB: niet altijd gevuld

//MPI 26-08-2013: ET # 3147 in rebound. Now explicitly clear what statusses should be presented. Added assignstatus WOT  
//MPI 26-05-2014: Report runs fine execpt on PROD. Time outs probably a query / performance issue due to big amount of data  
//				  Revised sql

var sqlText = new String();
var stam    = new String();

params["stam"]=params["stam"].replace("=","");
stam+= " where s1.serialnum = '" + params["stam"] +"'"

// Add query to sqlText variable.
sqlText = " "
+ " select" 
+ "   st.ntloctrack,"
+ "   st.assetnum,"
+ "   st.serialnum," 
+ "   wa.parent," 
+ "   wo.description as wodesc," 
+ "   wm.status," 
+ "   wa.taskid," 
+ "   wa.description as taskdesc," 
+ "   wm.assignstatus," 
+ "   wm.laborcode," 
+ "   pe.displayname" 
+ " from "
+ "   asset st "
+ "     inner join "
+ "     ( "
+ "       select aa.assetnum "
+ "       from assetancestor aa "
+ "         inner join  "
+ "         ( "
+ "           select s1.assetnum, s1.ntloctrack " 
+ "           from asset s1  "
+				stam
//+ "           where s1.serialnum = '9401' "
+ "         ) s2 on aa.ancestor = s2.assetnum "
+ "       ) s3 on s3.assetnum = st.assetnum "
+ "   join workorder wa on st.assetnum = wa.assetnum and wa.historyflag = 0 and wa.istask = 1 "
+ "   join workorder wo on wo.wonum = wa.parent and wo.historyflag = 0 and wo.istask = 0 "
+ "   join wmassignment wm on wm.wonum = wa.wonum and wm.assignstatus in('TOEGEWEZEN','GESTART','ONDERBROKEN','WOT') "
+ "   left join person pe on pe.personid = wm.laborcode "
+ " order by wa.wonum, wa.taskid "



//+ "   woactivity wa"
//+ "     join workorder wo on wo.wonum = wa.parent"
//+ "     join asset st on wo.assetnum = st.assetnum"
//+ "     left join wmassignment wm on wm.wonum = wa.wonum"
//+ "     left join person pe on pe.personid = wm.laborcode"
//+ " where "
//+ "   wa.assetnum in" 
//+ "   ("
//+ "     select" 
//+ "       a.assetnum"
//+ "     from "
//+ "       asset a"
//+ "     connect by prior assetnum = parent"
//+           stam 
//+ "   )"
//+ " and "
//+ "   wm.assignstatus in('TOEGEWEZEN','GESTART','ONDERBROKEN','WOT') " // 26-8-2013 added status WOT
//+ " order by wa.parent, wa.taskid " 

// Include the Maximo where clause
//+ " where " + params["where"]
;

maximoDataSet.setQuery(sqlText);]]></method>
