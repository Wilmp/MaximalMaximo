            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

if (params["appname"].value) {
	tableDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "maintable");
	tableDataSet.open();
	sqlText = "select maintbname from maxapps where app='" + params["appname"] + "'";
	tableDataSet.setQuery(sqlText);
	if (tableDataSet.fetch()) {
		maintable = tableDataSet.getString("maintbname").toLowerCase();
	}
	tableDataSet.close();
}

sqlText = "select " + maintable + ".ticketuid, " + maintable + ".class, " + maintable + ".isglobal, " + maintable + ".ticketid, " + maintable + ".description, " + maintable + ".status, " + maintable + ".owner, " + maintable + ".ownergroup,"
+ " " + maintable + ".reportdate, " + maintable + ".reportedby, " + maintable + ".assetnum, " + maintable + ".location, " + maintable + ".targetstart, " + maintable + ".targetfinish,"
+ " classstructure.description as classification, " + maintable + ".siteid, " + maintable + ".cinum"
+ " from ticket " + maintable + " left outer join "
+ " classstructure on " + maintable + ".classstructureid = classstructure.classstructureid"
+ " where " + maintable + ".class in (select value from synonymdomain where domainid = 'TKCLASS' and maxvalue = '" + maintable.toUpperCase() + "')"
+ " and " + params["where"] 
+ " order by " + maintable + ".siteid, " + maintable + ".status, " + maintable + ".ticketid"
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["ticketuid"] = maximoDataSet.getDouble("ticketuid");
row["class"] = maximoDataSet.getString("class");
row["isglobal"] = maximoDataSet.getBooleanString("isglobal");
row["ticketid"] = maximoDataSet.getString("ticketid");
row["description"] = maximoDataSet.getString("description");
row["classification"] = maximoDataSet.getString("classification");
row["status"] = maximoDataSet.getString("status");
var owner = maximoDataSet.getString("owner");
var ownergroup = maximoDataSet.getString("ownergroup");
row["reportdate"] = maximoDataSet.getTimestamp("reportdate");
row["reportedby"] = maximoDataSet.getString("reportedby");
row["assetnum"] = maximoDataSet.getString("assetnum");
row["location"] = maximoDataSet.getString("location");
row["targetstart"] = maximoDataSet.getTimestamp("targetstart");
row["targetfinish"] = maximoDataSet.getTimestamp("targetfinish");
row["siteid"] = maximoDataSet.getString("siteid");
row["cinum"] = maximoDataSet.getString("cinum");

if (isSR)
	row["isglobal"] = "";

// So TOC generates cleanly
if (row["siteid"] == null)
	row["siteid"] = "";
	
if (owner == null)
	owner = "";
if (ownergroup == null)
	ownergroup = "";

row["owner_ownergroup"] = owner + ownergroup;

return (true);]]></method>
