            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// 12-08-2013 MPI: ET #2332

params["worklocation"]=params["worklocation"].replace("=","");

// Add query to sqlText variable.
sqlText = " "
+ " select " 
+ "   sub.ntloctrack as ntloctrack, "
+ "   st.ntserie as type, "
+ "   sub.serialnum as stam, "
+ "   st.serialnum as eenheid, "
+ "   st.ntrsnmaint as reden_binnenkomst, "
+ "   sub1.targstartdate as bwo_datum_gepland, "
+ "   sub1.actstart as bwo_datum_in, "
+ "   sub1.targcompdate as bwo_datum_uit, "
+ "   sub1.schedfinish as bwo_datum_prognose, "
+ "   workorder.wonum as workorder, " 
+ "   workorder.description as werkzaamheden, "
//  -- vanaf hier eventueel extra kolommen, maar niet gepresenteerd op rapport
+ "   sub1.worklocation as locatie, "
+ "   workorder.parent as bwo, "
+ "   sub1.description as bwodesc, "
+ "   sub1.status as bwostatus, "
+ "   sub1.actfinish as bwoacteind, "
+ "   workorder.status as wostatus, "
+ "   workorder.reportdate, "
+ "   workorder.targstartdate as wostart, "
+ "   workorder.targcompdate as woeind, "
+ "   workorder.assetnum, " 
+ "   sub.description as stamdesc, "
+ "   st.description, "
+ "   lc.description as locdescription "
+ " from  "  
+ "   workorder "
+ "    join asset st on workorder.assetnum = st.assetnum "
+ "    left join  "
+ "      ( "
+ "        select " 
+ "          sw.wonum, "  
+ "          sa.ntloctrack, "  
+ "          sa.serialnum, "
+ "          sa.description, "
+ "          sn.assetnum  "
+ "        from  "
+ "          asset sa, "
+ "          workorder sw, "
+ "          assetancestor sn "
+ "        where sa.parent is null "
+ "        and   sa.assetnum = sn.ancestor "
+ "        and   sn.assetnum = sw.assetnum "
+ "       ) sub on st.assetnum = sub.assetnum and workorder.wonum = sub.wonum "
+ "    join  "
+ "       ( "
+ "         select " 
+ "           a.wonum, " 
+ "           a.ancestor, " 
+ "           b.description, " 
+ "           b.targstartdate, " 
+ "           b.targcompdate, " 
+ "           b.status, " 
+ "           b.worklocation, " 
+ "           b.actstart,  "
+ "           b.actfinish,  "
+ "           b.schedfinish "
+ "         from  "
+ "           woancestor a "
+ "         join workorder b on " 
+ "           a.ancestor = b.wonum and " 
+ "           b.parent is null and  "
+ "           b.worktype = 'BWO' and  "
+ "           b.status <> 'GEREED'  "
+ "           and b.historyflag = 0 "
+ "       ) sub1 on workorder.wonum = sub1.wonum "
+ "   join locations lc " 
+ "     on workorder.worklocation = lc.location  "
+ " where 1=1 "
+ " and   workorder.worklocation = '" + params["worklocation"].toUpperCase() +"'"
+ " and   workorder.parent is not null "
+ " and   workorder.istask = 0 "
+ " and   workorder.historyflag = 0 "
+ " and   workorder.status <> 'GEREED' "
+ " order by sub.serialnum "

// Include the Maximo where clause
//+ " where " + params["where"]
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.

row["type"] = maximoDataSet.getString("type");	
row["stam"] = maximoDataSet.getString("stam");	
row["eenheid"] = maximoDataSet.getString("eenheid");	
row["reden_binnenkomst"] = maximoDataSet.getString("reden_binnenkomst");	
row["bwo_datum_gepland"] = maximoDataSet.getTimestamp("bwo_datum_gepland");	
row["bwo_datum_in"] = maximoDataSet.getTimestamp("bwo_datum_in");	
row["bwo_datum_uit"] = maximoDataSet.getTimestamp("bwo_datum_uit");	
row["bwo_datum_prognose"] = maximoDataSet.getTimestamp("bwo_datum_prognose");	
row["workorder"] = maximoDataSet.getString("workorder");	
row["werkzaamheden"] = maximoDataSet.getString("werkzaamheden");	
row["locatie"] = maximoDataSet.getString("locatie");	
row["bwo"] = maximoDataSet.getString("bwo");	
row["bwodesc"] = maximoDataSet.getString("bwodesc");	
row["bwostatus"] = maximoDataSet.getString("bwostatus");	
row["wostatus"] = maximoDataSet.getString("wostatus");	
row["bwoacteind"] = maximoDataSet.getTimestamp("bwoacteind");	
row["reportdate"] = maximoDataSet.getTimestamp("reportdate");	
row["wostart"] = maximoDataSet.getTimestamp("wostart");	
row["woeind"] = maximoDataSet.getTimestamp("woeind");	
row["assetnum"] = maximoDataSet.getString("assetnum");
row["stamdesc"] = maximoDataSet.getString("stamdesc");
row["description"] = maximoDataSet.getString("description");
row["locdescription"] = maximoDataSet.getString("locdescription");
row["ntloctrack"] = maximoDataSet.getString("ntloctrack");
return (true);]]></method>
