            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

// RYZ 10122018: S78.937 nieuwe rapport t.b.v. inzet back-up materieel SNG

var sqlText = new String();

var locatie	  = new String();

if(params["location"].value) 
{
    locatie +=   MXReportSqlFormat.createParamWhereClause("asset.location", params["location"] );
}

// Add query to sqlText variable.
sqlText = " "
+ " select "
+ " asset.serialnum, "
+ " asset.ntserie, "
+ " asset.status, "
+ " assettrans.toloc, "
+ " assettrans.datemoved as binnenkomst, "
+ " (sysdate-assettrans.datemoved) as aantaldagen, "
+ " asset.assetnum, "
+ " asset.location "
+ " from asset "
+ " left outer join locations on asset.location = locations.location and asset.siteid = locations.siteid "
+ " join assettrans on assettransid = "
+ " ( "
+ " select max(assettransid) "
+ " from  assettrans "
+ " where assetid = asset.assetid "
+ " and   fromloc = 'MAT' and toloc in "
+ "            ( "
+ "             select location "
+ "             from   locations "
+ "             where  location != 'MAT' "
+ "             and    type = 'OPERATING' "
+ "             and    siteid = asset.siteid "
+ "            ) "
+ "     and siteid = asset.siteid "
+ "     and transtype = 'MOVED' "
+ "    ) "
+ " where " + locatie
+ " and asset.status in ('OVERSTAND','NBVD') "
+ " and assettype in ('EENHEID','LOC') and parent is null "
+ " and asset.ntserie in ('SLT','ICM') "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["serialnum"] = maximoDataSet.getString("serialnum");	
row["ntserie"] = maximoDataSet.getString("ntserie");	
row["status"] = maximoDataSet.getString("status");	
row["toloc"] = maximoDataSet.getString("toloc");
row["binnenkomst"] = maximoDataSet.getTimestamp("binnenkomst");
row["aantaldagen"] = maximoDataSet.getString("aantaldagen");
row["location"] = maximoDataSet.getString("location");	

return (true);]]></method>
