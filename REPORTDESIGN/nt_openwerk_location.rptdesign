            <method name="open"><![CDATA[dataSetNonBWO = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
dataSetNonBWO.open();

var sqlText = new String();

var locatie	  = new String();

if(params["location"].value) 
{
    locatie +=   MXReportSqlFormat.createParamWhereClause("asset.location", params["location"] );
}

sqlText = " "
+ " select "
+ "   asset.assetnum,"
+ "   asset.serialnum,"
+ "   asset.location,"
+ "   workorder.wonum," 
+ "   workorder.taskid,"
+ "   workorder.istask,"
+ "   workorder.status,"
+ "   workorder.worktype,"
+ "   workorder.description,"
+ "   workorder.ntserialnum,"
+ "   workorder.wopriority,"
+ "   workorder.haschildren,"
+ "   workorder.estdur"
//+ "   jobplan.jpduration"
+ " from asset "
+ "  join workorder on workorder.nttopserialnum = asset.serialnum"
//+ " left outer join jobplan"
//+ "   on jobplan.jpnum = workorder.jpnum and jobplan.status='ACTIEF'"
//+ " where " + params["where"]
+ " where " + locatie
+ "   and workorder.nttopserialnum = asset.serialnum"
+ "   and (workorder.worklocation is null or workorder.worklocation = asset.location)"
// S67.543
// + "   and workorder.worktype in ('CO', 'WUI')"
// + "   and workorder.status in ('WGK') and workorder.parent is null"
+ "   and ((workorder.worktype in ('CO', 'WUI') and workorder.status in ('WGK')) or "
+ "   (workorder.worktype in ('DHST') and workorder.status in ('GGK','INUITV'))) "
+ "   and workorder.parent is null "
+ "   and workorder.ntwocontr = 0"
+ "   and workorder.istask = 0"
+ "   and workorder.historyflag = 0"
+ "   and workorder.pmnum is null"
;

dataSetNonBWO.setQuery(sqlText);


]]></method>
            <method name="fetch"><![CDATA[if (!dataSetNonBWO.fetch())
	return (false);

row["assetnum"] = dataSetNonBWO.getString("assetnum");
row["serialnum"] = dataSetNonBWO.getString("serialnum");		
row["location"] = dataSetNonBWO.getString("location");
row["wonum"] = dataSetNonBWO.getString("wonum");
row["taskid"] = dataSetNonBWO.getString("taskid");
row["istask"] = dataSetNonBWO.getString("istask");
row["status"] = dataSetNonBWO.getString("status");
row["worktype"] = dataSetNonBWO.getString("worktype");
row["description"] = dataSetNonBWO.getString("description");
row["wopriority"] = dataSetNonBWO.getString("wopriority");
row["ntserialnum"] = dataSetNonBWO.getString("ntserialnum");
row["haschildren"] = dataSetNonBWO.getInteger("haschildren");
row["estdur"] = dataSetNonBWO.getDuration("estdur");

return (true);]]></method>
