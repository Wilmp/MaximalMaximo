            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//RYZ 15-11-2018: Openstaand werk per locatie; kopie rapport nt_openwerk

var sqlText = new String();

var locatie	  = new String();

if(params["location"].value) 
{
    locatie +=   MXReportSqlFormat.createParamWhereClause("asset.location", params["location"] );
}

sqlText = " "
+ " select"
+ "   sysdate,"
+ "   asset.assetnum,"
+ "   asset.serialnum,"
+ "   asset.location, "
+ "   workorder1.wonum as bwonum,"
+ "   workorder1.status as bwostatus,"
+ "   workorder2.wonum," 
+ "   workorder2.taskid,"
+ "   workorder2.istask,"
+ "   workorder2.status,"
+ "   workorder2.worktype,"
+ "   workorder2.description,"
+ "   workorder2.haschildren,"
+ "   wmassignment.assignstatus,"
+ "   wmassignment.laborcode,"
+ "   wmassignment.wopriority,"
+ "   wmassignment.schedfinish,"
+ "   wmassignment.ntserialnum,"  
+ "   labor.crewid,"
+ "   person.displayname,"
+ "   woactivity.estdur,"
//+ "   jobplan.jpduration,"
//+ "   case when wmassignment.assignstatus = 'ONDERBROKEN' and wmassignment.laborcode is null then 'HERTOEWIJZEN'"
//+ "        when wmassignment.assignstatus = 'ONDERBROKEN' and wmassignment.laborcode is not null then 'PAUZE'"
//+ "        else wmassignment.assignstatus"
//+ "   end as case1,"
+ "   wmassignment.assignstatus as case1,"
+ "   case when ((wmassignment.assignstatus = 'HERTOEWIJZEN') or wmassignment.assignstatus = 'WOT') then '1. Nog toe te wijzen openstaand werk'"
+ "        when ((wmassignment.assignstatus = 'ONDERBROKEN' ) or wmassignment.assignstatus = 'TOEGEWEZEN' or wmassignment.assignstatus = 'GESTART') then '2. Toegewezen en onderbroken openstaand werk'"
+ "        else '3. Openstaande (vervolg)werkorders in BWO'" //IMPORTANT: String used in visibility property in table
+ "   end as case2"       
//+ "   case when ((wmassignment.assignstatus = 'ONDERBROKEN' and wmassignment.laborcode is null) or wmassignment.assignstatus = 'WOT') then '1. Nog toe te wijzen openstaand werk'"
//+ "        when ((wmassignment.assignstatus = 'ONDERBROKEN' and wmassignment.laborcode is not null) or wmassignment.assignstatus = 'TOEGEWEZEN' or wmassignment.assignstatus = 'GESTART') then '2. Toegewezen en onderbroken openstaand werk'"
//+ "        else '3. Openstaande (vervolg)werkorders in BWO'" //IMPORTANT: String used in visibility property in table
//+ "   end as case2"      
+ " from asset"
+ "   join workorder workorder1" 
+ "     on workorder1.assetnum = asset.assetnum " // and workorder1.worktype = 'BWO' and workorder1.status not in ('SLUITEN', 'GEREED') and workorder1.historyflag = 0"
+ "   join woancestor"
+ "     on woancestor.ancestor = workorder1.wonum and woancestor.orgid = workorder1.orgid and woancestor.siteid = workorder1.siteid"
+ "   join workorder workorder2" 
+ "     on workorder2.wonum = woancestor.wonum and workorder2.orgid = woancestor.orgid and workorder2.siteid = woancestor.siteid " //and workorder2.status not in ('GEREED','SLUITEN','ANNUL')"
+ "   left outer join wmassignment"
+ "     on wmassignment.wonum = workorder2.wonum"
+ "   left outer join woactivity"
+ "     on workorder2.siteid = woactivity.siteid and workorder2.orgid = woactivity.orgid and workorder2.wonum = woactivity.wonum"
//+ "   left outer join jobplan"
//+ "     on jobplan.jpnum = workorder2.jpnum and jobplan.status='ACTIEF'"
+ "   left outer join person"
+ "     on person.personid = wmassignment.laborcode"
+ "   left outer join labor"
+ "     on labor.laborcode = person.personid"
+ " where " + locatie
+ "  and workorder2.status not in ('GEREED','SLUITEN','ANNUL')"
+ "  and workorder1.worktype = 'BWO' and workorder1.status <> 'SLUITEN' and workorder1.historyflag = 0"
+ " union "
+ " select "
+ "   sysdate,"
+ "   asset.assetnum,"
+ "   asset.serialnum,"
+ "   asset.location, "
+ "   workorder.wonum as bwonum," 
+ "   workorder.status as bwostatus,"
+ "   workorder.wonum,"
+ "   workorder.taskid,"
+ "   workorder.istask,"
+ "   workorder.status,"
+ "   workorder.worktype,"
+ "   workorder.description,"
+ "   workorder.haschildren,"
+ "   '' as assignstatus,"
+ "   '' as laborcode,"
+ "   workorder.wopriority,"
+ "   workorder.schedfinish,"
+ "   workorder.ntserialnum,"
+ "   '' as crewid,"
+ "   '' as displayname,"
+ "   workorder.estdur,"
+ "   '' as case1,"
+ "   '4. Openstaande werkorders niet in BWO (uit inspectie)' as case2 "
+ " from asset "
+ "  join workorder on workorder.nttopserialnum = asset.serialnum"
+ " where " + locatie
+ "   and workorder.nttopserialnum = asset.serialnum"
+ "   and (workorder.worklocation is null or workorder.worklocation = asset.location)"
// S67.543
//+ "   and workorder.worktype in ('CO', 'WUI')"
//+ "   and workorder.status in ('WGK') and workorder.parent is null"
+ "   and ((workorder.worktype in ('CO', 'WUI') and workorder.status in ('WGK')) or "
+ "   (workorder.worktype in ('DHST') and workorder.status in ('GGK','INUITV'))) "
+ "   and workorder.parent is null "
+ "   and workorder.ntwocontr = 0"
+ "   and workorder.istask = 0"
+ "   and workorder.historyflag = 0"
+ "   and workorder.pmnum is null"
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["assetnum"] = maximoDataSet.getString("assetnum");
row["serialnum"] = maximoDataSet.getString("serialnum");
row["location"] = maximoDataSet.getString("location");		
row["sysdate"] = maximoDataSet.getTimestamp("sysdate");	
row["bwonum"] = maximoDataSet.getString("bwonum");
row["bwostatus"] = maximoDataSet.getString("bwostatus");
row["wonum"] = maximoDataSet.getString("wonum");
row["taskid"] = maximoDataSet.getString("taskid");
row["istask"] = maximoDataSet.getString("istask");
row["status"] = maximoDataSet.getString("status");
row["worktype"] = maximoDataSet.getString("worktype");
row["description"] = maximoDataSet.getString("description");
row["assignstatus"] = maximoDataSet.getString("assignstatus");
row["laborcode"] = maximoDataSet.getString("laborcode");
row["wopriority"] = maximoDataSet.getString("wopriority");
row["schedfinish"] = maximoDataSet.getTimestamp("schedfinish");	
row["ntserialnum"] = maximoDataSet.getString("ntserialnum");
row["crewid"] = maximoDataSet.getString("crewid");
row["displayname"] = maximoDataSet.getString("displayname");
row["case1"] = maximoDataSet.getString("case1");
row["case2"] = maximoDataSet.getString("case2");
row["haschildren"] = maximoDataSet.getInteger("haschildren");
row["estdur"] = maximoDataSet.getDuration("estdur");

var parts = row["estdur"].split(':'); //Split string	
var minutes = parseInt((+parts[0]) * 60 + (+parts[1])); 
row["estminutes"] = minutes;

return (true);]]></method>
