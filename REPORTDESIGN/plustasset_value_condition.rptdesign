            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " select asset.siteid, asset.assetnum, asset.plustyear, asset.manufacturer, asset.plustmodel, "
		+ " asset.status,asset.plustcondition,asset.totalcost, asset.plustsolddate, asset.plustdepcost, "
		+ " asset.plustsoldamt, asset.plustrepairlimit, asset.purchaseprice, "
		//+ " asset.plustdepperiodtype, "
		+ " (select maxvalue from synonymdomain "
		+ "   where synonymdomain.domainid = 'PLUSTDEPPERIODTYPE' and "
		+ "   synonymdomain.value = asset.plustdepperiodtype) as plustdepperiodtype, "
		+ " asset.plustdepmeter "
		+ " from asset "
		+ " where " + params["where"]
		+ " order by asset.assetnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["siteid"] = maximoDataSet.getString("siteid");
row["assetnum"] = maximoDataSet.getString("assetnum");
row["plustyear"] = maximoDataSet.getString("plustyear");
row["manufacturer"] = maximoDataSet.getString("manufacturer");
row["plustmodel"] = maximoDataSet.getString("plustmodel");
row["status"] = maximoDataSet.getString("status");
row["plustcondition"] = maximoDataSet.getString("plustcondition");
row["totalcost"] = maximoDataSet.getDouble("totalcost");
row["plustsolddate"] = maximoDataSet.getTimestamp("plustsolddate");
var plustdepcost = maximoDataSet.getDouble("plustdepcost");
row["plustdepcost"] = plustdepcost;
row["plustsoldamt"] = maximoDataSet.getDouble("plustsoldamt");
var plustrepairlimit = maximoDataSet.getDouble("plustrepairlimit");
row["plustrepairlimit"] = plustrepairlimit;
var purchaseprice = maximoDataSet.getDouble("purchaseprice");
row["purchaseprice"] = purchaseprice;
var plustdepperiodtype = maximoDataSet.getString("plustdepperiodtype");
row["plustdepperiodtype"] = plustdepperiodtype;
row["plustdepmeter"] = maximoDataSet.getString("plustdepmeter");

var currentassetvalue=0.0;
var repairlimit=0.0;

// Depreciation date
var depDate = MXReportSqlFormat.getCurrentTimestampFunction();

if (plustdepperiodtype == "DATE") {
	// current value date
	currValueByDateDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "currValueByDateDS");
	currValueByDateDS.open();
	
	var currValueByDateSQL = "select plustdepschedule.currentassetvalue "
	+ " from plustdepschedule "
	+ " where plustdepschedule.siteid = ? "
	+ " and plustdepschedule.assetnum = ? "
	+ " and plustdepschedule.depreciationdate <= " + depDate
	+ " order by plustdepschedule.depreciationdate desc "
	;

	currValueByDateDS.setQuery(currValueByDateSQL);
	currValueByDateDS.setQueryParameterValue(1, maximoDataSet.getString("siteid"));
	currValueByDateDS.setQueryParameterValue(2, maximoDataSet.getString("assetnum"));
	
	if (currValueByDateDS.fetch()) {
		currentassetvalue = currValueByDateDS.getDouble("currentassetvalue");
	} else {
		// Set current value to null, so it will be retrieved from PLUSTDEPSCHEDULE in pediod # 1
		currentassetvalue = null;
	}

	currValueByDateDS.close();
}
else if (plustdepperiodtype == "METER") {
	// Determine latest life to date for this asset meter from table meterreading
	meterReadingLifeToDateDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "meterReadingLifeToDateDS");
	meterReadingLifeToDateDS.open();

	var meterReadingLifeToDateSQL = "select meterreading.plustltd "
	+ " from meterreading "
	+ " where meterreading.assetnum = ? "
	+ " and meterreading.metername = ? "
	+ " and meterreading.siteid = ? "
	+ " and meterreading.readingdate <= " + depDate
	+ " order by meterreading.plustltd desc "
	;

	meterReadingLifeToDateDS.setQuery(meterReadingLifeToDateSQL);
	meterReadingLifeToDateDS.setQueryParameterValue(1, maximoDataSet.getString("assetnum"));
	meterReadingLifeToDateDS.setQueryParameterValue(2, maximoDataSet.getString("plustdepmeter"));
	meterReadingLifeToDateDS.setQueryParameterValue(3, maximoDataSet.getString("siteid"));

	// If any meterreading is found, take the greatest life to date
	var lifeToDate = null;
	if (meterReadingLifeToDateDS.fetch()) {
		lifeToDate = meterReadingLifeToDateDS.getDouble("plustltd")
	} else {
		// Otherwise, determine latest life to date for the deprecation asset meter from table assetmeter
		assetMeterLifeToDateDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "assetMeterLifeToDateDS");
		assetMeterLifeToDateDS.open();

		var assetMeterLifeToDateSQL = "select assetmeter.lifetodate "
		+ " from assetmeter "
		+ " where assetmeter.assetnum = ? "
		+ " and assetmeter.metername = ? "
		+ " and assetmeter.siteid = ? "
		+ " order by assetmeter.lifetodate desc "
		;

		assetMeterLifeToDateDS.setQuery(assetMeterLifeToDateSQL);
		assetMeterLifeToDateDS.setQueryParameterValue(1, maximoDataSet.getString("assetnum"));
		assetMeterLifeToDateDS.setQueryParameterValue(2, maximoDataSet.getString("plustdepmeter"));
		assetMeterLifeToDateDS.setQueryParameterValue(3, maximoDataSet.getString("siteid"));

		// The asset meter should be found here (database is incossistent, otherwise). Anyway, if it exists, take its life to date
		if (assetMeterLifeToDateDS.fetch()) {
			lifeToDate = assetMeterLifeToDateDS.getDouble("lifetodate")
		}

		assetMeterLifeToDateDS.close();
	}

	meterReadingLifeToDateDS.close();

	if (lifeToDate != null) {
		// Retrieve current value
		currValueByMeterDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "currValueByMeterDS");
		currValueByMeterDS.open();
		
		var currValueByMeterSQL = "select plustdepschedule.currentassetvalue "
		+ " from plustdepschedule "
		+ " where plustdepschedule.siteid = ? "
		+ " and plustdepschedule.assetnum = ? "
		+ " and plustdepschedule.meterltd <= ? "
		+ " order by plustdepschedule.meterltd desc "
		;
	
		currValueByMeterDS.setQuery(currValueByMeterSQL);
		currValueByMeterDS.setQueryParameterValue(1, maximoDataSet.getString("siteid"));
		currValueByMeterDS.setQueryParameterValue(2, maximoDataSet.getString("assetnum"));
		currValueByMeterDS.setQueryParameterValue(3, lifeToDate);
	
		if (currValueByMeterDS.fetch()) {
			currentassetvalue = currValueByMeterDS.getDouble("currentassetvalue");
		} else {
			// Set current value to null, so it will be retrieved from PLUSTDEPSCHEDULE in pediod # 1
			currentassetvalue = null;
		}

		currValueByMeterDS.close();
	}
} else {
	currentassetvalue = null;
}

if (currentassetvalue == null && 
	(plustdepperiodtype == "DATE" || plustdepperiodtype == "METER")) {
	// Retrieve initial value
	initialValueDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "initialValueDS");
	initialValueDS.open();
	
	var initialValueSQL = "select plustdepschedule.currentassetvalue, "
	+ " plustdepschedule.depreciationamt "
	+ " from plustdepschedule "
	+ " where plustdepschedule.siteid = ? "
	+ " and plustdepschedule.assetnum = ? "
	+ " and plustdepschedule.periodnum = 1 "
	;

	initialValueDS.setQuery(initialValueSQL);
	initialValueDS.setQueryParameterValue(1, maximoDataSet.getString("siteid"));
	initialValueDS.setQueryParameterValue(2, maximoDataSet.getString("assetnum"));

	if (initialValueDS.fetch()) {
		currentassetvalue = initialValueDS.getDouble("depreciationamt")
		+ initialValueDS.getDouble("currentassetvalue");
	}

	initialValueDS.close();
}

row["currentassetvalue"] = currentassetvalue;
var calulatedRepairLimit;
	
if(plustrepairlimit==null) {
	calulatedRepairLimit=null;
} else if(currentassetvalue!=null) {
	calulatedRepairLimit=currentassetvalue*(plustrepairlimit/100)
} else if(currentassetvalue==null) {
	calulatedRepairLimit=purchaseprice*(plustrepairlimit/100)
}
row["calulatedRepairLimit"] = calulatedRepairLimit;

return (true);]]></method>
