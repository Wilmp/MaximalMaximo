            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

if(maximoDataSet.isOracle()){
	dbClause = "nvl";
	asClause = "";
	db2QueryAlias = "";	
}
else{
	dbClause = "coalesce";
	asClause = " as "
	db2QueryAlias = " as totalcost ";	
}

if(params["location"].value) {
	params["location"] = params["location"].toUpperCase();
	params["where"] += " and " + MXReportSqlFormat.createParamWhereClause("asset.location", params["location"].replace(/[=!,]/,""));
}

if (params["siteid"].value){
	params["siteid"] = params["siteid"].toUpperCase();
	params["siteid"] = params["siteid"].replace("=", "");
	params["where"] += " and asset.siteid = '" + params["siteid"] + "' ";
}

if(parentParam.equals(""))
	params["where"] += " and parentloc.location = asset.location and asset.parent is null ";
else 
	params["where"] += " and parentloc.location in (select parentasset.location from asset " + asClause + " parentasset join locations "
	+ " on parentasset.location = locations.location and parentasset.siteid = locations.siteid and parentasset.assetnum = '" + parentParam + "') "
	+ " and asset.parent = '" + parentParam + "'";

if(params["startdate"].value){
	labtransStartDateParam = " and labtrans.startdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
				 
	matusetransStartDateParam = " and matusetrans.actualdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
	
	tooltransStartDateParam = " and tooltrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
	
	servrectransStartDateParam = " and servrectrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
}

if(params["enddate"].value){
	labtransEndDateParam = " and labtrans.startdate <= "  + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
			   
	matusetransEndDateParam = " and matusetrans.actualdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
	
	tooltransEndDateParam = " and tooltrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
	
	servrectransEndDateParam  = " and servrectrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
	
}

sqlText = " select asset.assetnum, asset.description, asset.children, asset.location, parentloc.description as parentlocdesc, "
		+ " childasset.description as childlocdesc from asset "
		+ " join locations on asset.location = locations.location and asset.siteid = locations.siteid "
		+ " left outer join locations " + asClause + " parentloc on asset.siteid = parentloc.siteid "
		+ " left outer join asset childasset on asset.parent = childasset.assetnum and asset.siteid = childasset.siteid "
		+ " where " + params["where"]
		;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);
// populate location descriptions

row["assetnum"] = maximoDataSet.getString("assetnum");	
row["description"] = maximoDataSet.getString("description");	
row["children"] = maximoDataSet.getBooleanString("children");
row["location"] = maximoDataSet.getString("location");
row["parentlocdesc"] = maximoDataSet.getString("parentlocdesc");
row["childlocdesc"] = maximoDataSet.getString("childlocdesc");
row["groupfield"] = row["assetnum"];

parentTotalCost = MXReportDataSetProvider.create(getDataSource().getName(),"parentTotalCost");
parentTotalCost.open();

var cost = 0;	
var parentTotalCostSQL = new String();

parentTotalCostSQL = " select " + dbClause + "(sum(totalcost),0) as totalcost "
				  + " from   (select " + dbClause + "(sum(labtrans.linecost),0) as totalcost "
				  + " from   labtrans "
				  + " where  labtrans.assetnum = '" + row["assetnum"] + "' "
				  + " and 	 labtrans.siteid = '" + params["siteid"] + "' "
				  + labtransStartDateParam
				  + labtransEndDateParam
				  + " union  all "
				  + " select " + dbClause + "(sum(matusetrans.linecost),0) as totalcost "
				  + " from   matusetrans "
				  + " where  matusetrans.assetnum = '" + row["assetnum"] + "' "
				  + " and 	 matusetrans.siteid = '" + params["siteid"] + "' "
				  + matusetransStartDateParam
				  + matusetransEndDateParam
				  + " union  all "
				  + " select " + dbClause + "(sum(tooltrans.linecost),0) as totalcost "
				  + " from   tooltrans "
				  + " where  tooltrans.assetnum = '" + row["assetnum"] + "' "
				  + " and 	 tooltrans.siteid = '" + params["siteid"] + "' "
				  + tooltransStartDateParam
				  + tooltransEndDateParam
				  + " union  all "
				  + " select " + dbClause + "(sum(servrectrans.linecost),0) as totalcost "
				  + " from   servrectrans "
				  + " where  servrectrans.assetnum = '" + row["assetnum"] + "' "
				  + " and 	 servrectrans.siteid = '" + params["siteid"]  + "' "
				  + servrectransStartDateParam
				  + servrectransEndDateParam
				  + ")" + db2QueryAlias
				  ;

parentTotalCost.setQuery(parentTotalCostSQL);

if(parentTotalCost.fetch())
{
	row["totalcost"] = parentTotalCost.getDouble("totalcost");
}
	
parentTotalCost.close();


return (true);

maximoDataSet.close();]]></method>
