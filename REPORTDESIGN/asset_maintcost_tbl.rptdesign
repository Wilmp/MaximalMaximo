            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);
// populate location descriptions

row["childdesc"] = maximoDataSet.getString("childdesc");
row["children"] = maximoDataSet.getBooleanString("children");
row["childrenbool"] = maximoDataSet.getInteger("children");
row["childassetnum"] = maximoDataSet.getString("childassetnum");
row["childlocation"] = maximoDataSet.getString("childlocation");


if(row["childrenbool"]){
	var allChildAry = new Array();
	allChildAry.push(row["childassetnum"]);	
	getChildAsset(row["childassetnum"]);

	row["child_totalcost"] = getTotalCost("('" + allChildAry.join("','") + "')");

}
else {
	row["child_totalcost"] = getTotalCost("('" + row["childassetnum"] + "')");
}

return(true);


function getChildAsset(childassetnum){

	childRec = MXReportDataSetProvider.create(getDataSource().getName(),"childRec");
	childRec.open();
		
	var childRecSQL = new String();
	var childArray = new Array();
				
	childRecSQL = " select  asset.assetnum "
				+ " from    asset "
				+ " join 	locations on asset.location = locations.location and asset.siteid = locations.siteid "
				+ " where   asset.parent = ? "
				+ " and 	asset.siteid = ? ";
	
	childRec.setQuery(childRecSQL);
	
	childRec.setQueryParameterValue(1, childassetnum);
	childRec.setQueryParameterValue(2, params["siteid"]);

	
	while(childRec.fetch()){
		childArray.push(childRec.getString("assetnum"));
	}

	for(var i = 0; i < childArray.length; i++){
		allChildAry.push(childArray[i]);	
		getChildAsset(childArray[i]);
	}

}

function getTotalCost(assetnum){

	childTotalCost = MXReportDataSetProvider.create(getDataSource().getName(),"childTotalCost");
	childTotalCost.open();

	var cost = 0;	
	var childTotalCostSQL = new String();

	childTotalCostSQL = " select " + dbClause + "(sum(totalcost),0) as totalcost "
					  + " from   (select " + dbClause + "(sum(labtrans.linecost),0) as totalcost "
					  + " from   labtrans "
					  + " where  labtrans.assetnum in " + assetnum 
					  + " and 	 labtrans.siteid = '" + params["siteid"] + "' "
					  + labtransStartDateParam
					  + labtransEndDateParam
					  + " union  all "
					  + " select " + dbClause + "(sum(matusetrans.linecost),0) as totalcost "
					  + " from   matusetrans "
					  + " where  matusetrans.assetnum in " + assetnum
					  + " and 	 matusetrans.siteid = '" + params["siteid"] + "' "
					  + matusetransStartDateParam
					  + matusetransEndDateParam
					  + " union  all "
					  + " select " + dbClause + "(sum(tooltrans.linecost),0) as totalcost "
					  + " from   tooltrans "
					  + " where  tooltrans.assetnum in " + assetnum
					  + " and 	 tooltrans.siteid = '" + params["siteid"] + "' "
					  + tooltransStartDateParam
					  + tooltransEndDateParam
					  + " union  all "
					  + " select " + dbClause + "(sum(servrectrans.linecost),0) as totalcost "
					  + " from   servrectrans "
					  + " where  servrectrans.assetnum in " + assetnum
					  + " and 	 servrectrans.siteid = '" + params["siteid"] + "' " 
					  + servrectransStartDateParam
					  + servrectransEndDateParam
					  + ")" + db2QueryAlias
					  ;

	childTotalCost.setQuery(childTotalCostSQL);
	
	if(childTotalCost.fetch())
	{
		cost = childTotalCost.getDouble("totalcost");
	}
		
	childTotalCost.close();
	
	return cost;

}]]></method>
