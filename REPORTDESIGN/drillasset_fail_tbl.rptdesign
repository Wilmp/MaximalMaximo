            <method name="open"><![CDATA[mainDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
mainDataSet.open();

var sqlText = new String();
if(params["hasmoved"].value){
	if(mainDataSet.getBooleanInteger(params["hasmoved"].toUpperCase())==1) {
		params["where"] += " and asset.moved = 1 ";

	}else if (mainDataSet.getBooleanInteger(params["hasmoved"].toUpperCase())==0) {
		params["where"] += " and asset.moved = 0 ";
	}
}


var sqlText = " select distinct asset.assetnum, asset.description as asset_description, asset.moved, "
+ " asset.siteid, asset.location, locations.description as locations_description, "
+ " workorder.problemcode, failurecode.description as failurecode_description "
+ " from asset left outer join locations "
+ " on asset.location = locations.location "
+ " and asset.siteid = locations.siteid "
+ " left outer join workorder "
+ " on workorder.assetnum = asset.assetnum "
+ " and workorder.siteid = asset.siteid "
+ " left outer join failurecode "
+ " on failurecode.failurecode = workorder.problemcode "
+ " and failurecode.orgid = workorder.orgid "
+ " where " + params["where"]
+ " and " 
+ MXReportSqlFormat.createParamWhereClause("workorder.problemcode", params["problemcode"].toUpperCase()) 
;
mainDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!mainDataSet.fetch())
	return (false);

row["hasmoved"] = mainDataSet.getBooleanString("moved"); 
row["failurecode_description"] = mainDataSet.getString("failurecode_description");	
row["asset_description"] = mainDataSet.getString("asset_description");	
row["locations_description"] = mainDataSet.getString("locations_description");	
row["main_assetnum"] = mainDataSet.getString("assetnum");	
row["main_location"] = mainDataSet.getString("location");	
row["main_problemcode"] = mainDataSet.getString("problemcode");	
row["main_siteid"] = mainDataSet.getString("siteid");	

return (true);]]></method>
