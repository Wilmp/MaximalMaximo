            <method name="open"><![CDATA[subDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
subDataSet.open();

var sqlText = new String();
var where = new String(" 1=1 ");

if (params["startdate"].value)
{
	if(subDataSet.isOracle() || subDataSet.isSQLServer() ){ 
		where += " and  workorder.faildate >= " 
		+ MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " ";
	}else if(subDataSet.isDB2()) {
		where += " and  timestamp(workorder.faildate) >= " 
		+ MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " ";
	}
}

if (params["enddate"].value)
{
	if(subDataSet.isOracle() || subDataSet.isSQLServer() ) {
		where += " and workorder.faildate <= " 
		+ MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " ";
	}else if(subDataSet.isDB2()) {
		where += " and timestamp(workorder.faildate) <= " 
		+ MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " ";
	}
}

sqlText = "select workorder.faildate, workorder.wonum, assetstatus.downtime "
+ " from workorder, assetstatus "
+ " where assetstatus.isrunning = 1 "
+ " and workorder.wonum = assetstatus.wonum "
+ " and workorder.siteid = assetstatus.siteid "
+ " and "
+ MXReportSqlFormat.createParamWhereClause("assetstatus.assetnum", params["assetnum"].toUpperCase())
+ " and "
+ MXReportSqlFormat.createParamWhereClause("assetstatus.siteid", params["siteid"].toUpperCase())
+ " and "
+ MXReportSqlFormat.createParamWhereClause("workorder.problemcode", params["problemcode"].toUpperCase())
+ " and "
+ MXReportSqlFormat.createParamWhereClause("workorder.location", params["location"].toUpperCase())
+ " and " + where
+ " order by workorder.faildate desc"
;

subDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!subDataSet.fetch())
	return (false);

row["short_faildate"] =  DateFormat.getDateInstance(DateFormat.SHORT, mxReportScriptContext.getLocale()).format(subDataSet.getTimestamp("faildate"));
row["faildate"] = subDataSet.getString("faildate");	
row["downtime"] = subDataSet.getFloat("downtime");	
row["wonum"] = subDataSet.getString("wonum");	

causeDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "causeDataSet");
causeDataSet.open();

causeSQLQuery = "select distinct failurereport.failurecode, failurecode.description "
+ " from failurereport, failurecode "
+ " where failurereport.wonum = '" + row["wonum"] + "' "
+ " and failurereport.type = 'CAUSE' "
+ " and failurereport.failurecode = failurecode.failurecode "

causeDataSet.setQuery(causeSQLQuery);

if (!causeDataSet.fetch())
	return (false);
	
remedyDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "remedyDataSet");
remedyDataSet.open();

remedySQLQuery = "select distinct failurereport.failurecode, failurecode.description "
+ " from failurereport, failurecode "
+ " where failurereport.wonum = '" + row["wonum"] + "' "
+ " and failurereport.type = 'REMEDY' "
+ " and failurereport.failurecode = failurecode.failurecode "

remedyDataSet.setQuery(remedySQLQuery);

if (!remedyDataSet.fetch())
	return (false);

row["cause_code"] = causeDataSet.getString("failurecode");	
row["cause_description"] = causeDataSet.getString("description");
row["remedy_code"] = remedyDataSet.getString("failurecode");	
row["remedy_description"] = remedyDataSet.getString("description");
	
return (true);]]></method>
