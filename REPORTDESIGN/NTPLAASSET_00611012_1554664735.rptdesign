            <property name="dataSource">maximoDataSource</property>
            <method name="open"><![CDATA[
 dataSet_asset = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
 dataSet_asset.open();
 var sqlText = new String();
 sqlText = 
   "select  " 
 + " asset.serialnum,asset.description,asset.status,asset.location,asset.ntserie "
 + "from asset " 
 + "where " +  params["where"] +  " and ( "  + "(  asset.assettype   =   'WIELSTEL'    and asset.status   !=  'AFGEVOERD'    and exists    (    select 1    from pm     where pm.assetnum=asset.assetnum     and pm.status = 'ACTIEF'     and (pm.masterpm = 'POH75171' or pm.masterpm = 'POH75180' or pm.masterpm = 'REV4005V' or pm.masterpm = 'REV4006V'  )       )    and exists    (    select 1    from assetspec    where assetspec.assetnum = asset.assetnum    and assetspec.assetattrid in ('NTZVETHALS','TZVETHALS'  )    and assetspec.alnvalue in ('P', 'S','PS'  )      )    and exists    (    select 1    from pm pmvet    where asset.assetnum=pmvet.assetnum     and pmvet.description like ('%V0219%'  )    and exists       (      select 1       from plusalfevent alfvet      where pmvet.pmuid = alfvet.pmuid      and alfvet.inactivedate is null      and alfvet.activedate > sysdate - 31        )      )    )" + " ) " 
 + "  "
 ;

 dataSet_asset.setQuery(sqlText);
]]></method>
            <method name="fetch"><![CDATA[if (!dataSet_asset.fetch()) 
   return (false);
 
//         Add a line for each output column
//         The specific get method should match the data type of the output column.
 row["serialnum"] = dataSet_asset.getString("serialnum"); 
 row["description"] = dataSet_asset.getString("description"); 
 row["status"] = dataSet_asset.getString("status"); 
 row["location"] = dataSet_asset.getString("location"); 
 row["ntserie"] = dataSet_asset.getString("ntserie"); 
 return (true);]]></method>
