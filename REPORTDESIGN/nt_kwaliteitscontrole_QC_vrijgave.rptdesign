	var sqlText = new String();

sqlText = " select "
+ " NTQCONWO.WONUM as wonum, "
+ " WORKORDER.DESCRIPTION as woomschr, "
+ " WORKORDER.NTPRIMPOTYPE as primpotype, "
+ " WORKORDER.WORKTYPE as werktype, "
+ " ASSET.SERIALNUM as serialnum, "
