            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//RYZ 20181012: S75.847 Nieuw rapport, Kwaliteitscontrole QC-vrijgave
//RYZ 20190730: S87.1802 uitbreiding met QC vrijgave, QC checklijst, WIG checklijst, Resultaat vrijgave, groepering op WOgroup
//WMU 20200305: S97.2405 QC checklijst en WIG checklijst in subqueries gezet

var sqlText = new String();
var stelstam= new String();
var matserie= new String();
var locatie	= new String();
var actfinishBetween = new (String);

if(params["stelstam"].value) 
{
    stelstam +=   MXReportSqlFormat.createParamWhereClause("woactivity.ntserialnum", params["stelstam"] );
}
if(params["matserie"].value) 
{
    matserie +=   MXReportSqlFormat.createParamWhereClause("woactivity.ntserie", params["matserie"] );
}
if(params["worklocation"].value) 
{
    locatie +=   MXReportSqlFormat.createParamWhereClause("woactivity.worklocation", params["worklocation"] );
}
if (params["startdt"].value)
	actfinishBetween += " and WOACTIVITY.ACTFINISH >= "+ MXReportSqlFormat.getTimestampFunction(params["startdt"]) ;
if (params["enddt"].value)
	actfinishBetween += " and WOACTIVITY.ACTFINISH <= "+ MXReportSqlFormat.getTimestampFunction(params["enddt"]) ;
	
// Add query to sqlText variable.
sqlText = " "
+ " select "
+ " WOACTIVITY.WOGROUP as wogroup, "
+ " (select workorder.description from workorder where workorder.wonum=WOACTIVITY.wogroup) as beschrijving, "
+ " WOACTIVITY.NTSERIE as materieelserie, "
+ " WOACTIVITY.NTSERIALNUM as stelstam, "
+ " WOACTIVITY.STATUS as status, "
+ " wostatus.CHANGEBY as uitvoerder, "
+ " person.displayname as persoon, "
+ " WOACTIVITY.ACTFINISH as actfinish, "
+ " WOACTIVITY.WORKLOCATION as werklocatie, "
+ " WOACTIVITY.wonum as taskwo, "
+ " NTCHECKLIST.NTQCTYPE as qctype, "
+ " NTCHECKLIST.NTMATERIEEL as matinfo, "
+ " (case when NTCHECKLISTTASK.DEFECT=1 then 'J' else 'N' end) CONF, "
+ " NTCHECKLISTTASK.REMARKS as reden, "
//--Resultaat vrijgave
+ " longdescription.ldtext, "
+ " NTCHECKLIST.NTVRIJGAVE as vrijgave "
+ " from WOACTIVITY "
+ " left join NTCHECKLIST on NTCHECKLIST.REFWO=WOACTIVITY.WONUM "
+ " left join NTCHECKLISTTASK on NTCHECKLISTTASK.REFWO=WOACTIVITY.wonum and NTCHECKLISTTASK.CLTASKNUM =10"
+ " left outer join wostatus on wostatus.wonum=woactivity.wonum  and wostatus.status='GEREED' and wostatus.siteid=woactivity.siteid "
+ " left outer join person on person.personid=wostatus.changeby and wostatus.status='GEREED' "
+ " left join longdescription on longdescription.ldkey=ntchecklist.ntchecklistid and NTCHECKLIST.REFWO=WOACTIVITY.wogroup"
+ " left outer join RELATEDRECORD on relatedrecord.relatedreckey=woactivity.wogroup and RELATEDRECORD.RELATETYPE='FOLLOWUP' "
+ " where WOACTIVITY.NTCLNUM='QC' "
+ " and" + stelstam
+ " and" + matserie
+ " and" + locatie
+ actfinishBetween
+ " order by woactivity.actfinish"
;

maximoDataSet.setQuery(sqlText);]]></method>
