            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var request = reportContext.getHttpServletRequest();
if(request != null){
	var session = request.getSession();
	session.setAttribute(dskey, this.getDataSource().getName());
}

params["paramdelimiter"] = "||";
params["paramstring"] = attkey + params["paramdelimiter"] + dskey;

var sqlText = new String();

if((BirtComp.notEqual(params["location"],null))&amp;&amp;(BirtComp.notEqual(params["location"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.location", params["location"]);
}
if((BirtComp.notEqual(params["site"],null))&amp;&amp;(BirtComp.notEqual(params["site"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"]);
}
	
sqlText = "select inventory.siteid, inventory.itemsetid, inventory.location, inventory.itemnum, "
+ " item.description, "
+ " inventory.orderunit, "
+ " inventory.minlevel, "
+ " inventory.deliverytime as delivery, "
+ " inventory.sstock, "
+ maximoDataSet.getNullValueFunction(
	"(select conversion.conversion from conversion where inventory.itemsetid=conversion.itemsetid and inventory.itemnum=conversion.itemnum and inventory.orderunit=conversion.frommeasureunit)", 
	"(select conversion.conversion from conversion where inventory.itemsetid=conversion.itemsetid and conversion.itemnum is null and inventory.orderunit=conversion.frommeasureunit)"
  ) 
+ " as conversion, "
+ maximoDataSet.getNullValueFunction(
	"(select conversion.conversion from conversion where inventory.itemsetid=conversion.itemsetid and inventory.itemnum=conversion.itemnum and inventory.orderunit=conversion.tomeasureunit)", 
	"(select conversion.conversion from conversion where inventory.itemsetid=conversion.itemsetid and conversion.itemnum is null and inventory.orderunit=conversion.tomeasureunit)"
  ) 
+ " as revconversion "
+ " from item inner join "
+ " inventory on "
+ " inventory.itemsetid=item.itemsetid "
+ " and inventory.itemnum=item.itemnum "
+ " where " + params["where"]
+ " and item.itemtype in (select value from synonymdomain where domainid='ITEMTYPE' and maxvalue='ITEM') "
+ " order by inventory.siteid, inventory.location, inventory.itemnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

var siteid = maximoDataSet.getString("siteid");
var itemsetid = maximoDataSet.getString("itemsetid");
var location = maximoDataSet.getString("location");
var itemnum = maximoDataSet.getString("itemnum");
var enddate = new Date();

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> itemnum: " + itemnum);
}

//row["siteid"] = maximoDataSet.getString("siteid");
//row["itemsetid"] = maximoDataSet.getString("itemsetid");
//row["location"] = maximoDataSet.getString("location");
row["itemnum"] = maximoDataSet.getString("itemnum");
row["description"] = maximoDataSet.getString("description");
row["orderunit"] = maximoDataSet.getString("orderunit");
row["minlevel"] = maximoDataSet.getString("minlevel");
//row["delivery"] = maximoDataSet.getString("delivery");
row["sstock"] = maximoDataSet.getString("sstock");
//row["conversion"] = maximoDataSet.getString("conversion");

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> leadtimemonths: " + leadtimemonths);
}

var leadtime = 0;
var sstock = 0;
var newlevel = 0;

// ********* MATRECTRANS part
var matrectransTotal = 0;

matrectransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "matrectransDS");
matrectransDS.open();

if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+matrectransDS.getName() + "] >>> created ");
}

var matrectransSQL = "select sum(coalesce(matrectrans.quantity,0)) as matrectransTotal "
+ " from item inner join "
+ " inventory on "
+ " item.itemsetid = inventory.itemsetid "
+ " and item.itemnum = inventory.itemnum inner join "
+ " matrectrans on "
+ " inventory.siteid = matrectrans.siteid "
+ " and inventory.location = matrectrans.tostoreloc "
+ " and inventory.itemsetid = matrectrans.itemsetid "
+ " and inventory.itemnum = matrectrans.itemnum "
+ " where matrectrans.issuetype in (select value from synonymdomain where domainid='ISSUETYP' and maxvalue='TRANSFER') "
+ " and item.itemtype in (select value from synonymdomain where domainid='ITEMTYPE' and maxvalue='ITEM') "
;
if(params["startdate"].value) {
	matrectransSQL+= " and matrectrans.actualdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
matrectransSQL+= " and matrectrans.actualdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(enddate)
+ " and inventory.siteid = ? "
+ " and inventory.itemsetid = ? "
+ " and inventory.location = ? "
+ " and inventory.itemnum = ? "
;

matrectransDS.setQuery(matrectransSQL);
matrectransDS.setQueryParameterValue(1, siteid);
matrectransDS.setQueryParameterValue(2, itemsetid);
matrectransDS.setQueryParameterValue(3, location);
matrectransDS.setQueryParameterValue(4, itemnum);

if(matrectransDS.fetch()) {
	matrectransTotal = Math.abs(matrectransDS.getInteger("matrectransTotal"));
	//matrectransTotal = matrectransDS.getInteger("matrectransTotal");
}
matrectransDS.close();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+matrectransDS.getName() + "] >>> closed");
}

// ********* MATUSETRANS part
var matusetransTotal = 0;

matusetransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "matusetransDS");
matusetransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+matusetransDS.getName() + "] >>> created");
}

var matusetransSQL = "select sum(coalesce(quantity,0)) as matusetransTotal "
+ " from inventory inner join "
+ " matusetrans on "
+ " inventory.siteid = matusetrans.siteid "
+ " and inventory.itemsetid = matusetrans.itemsetid "
+ " and inventory.location = matusetrans.storeloc "
+ " and inventory.itemnum = matusetrans.itemnum "
+ " where matusetrans.issuetype in (select value from synonymdomain where domainid='ISSUETYP' and maxvalue='ISSUE') "
;
if(params["startdate"].value) {
	matusetransSQL+= " and matusetrans.actualdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
matusetransSQL+= " and matusetrans.actualdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(enddate)
+ " and inventory.siteid = ? "
+ " and inventory.itemsetid = ? "
+ " and inventory.location = ? "
+ " and inventory.itemnum = ? "
;

matusetransDS.setQuery(matusetransSQL);
matusetransDS.setQueryParameterValue(1, siteid);
matusetransDS.setQueryParameterValue(2, itemsetid);
matusetransDS.setQueryParameterValue(3, location);
matusetransDS.setQueryParameterValue(4, itemnum);

if(matusetransDS.fetch()) {
	matusetransTotal = Math.abs(matusetransDS.getInteger("matusetransTotal"));
	//matusetransTotal = matusetransDS.getInteger("matusetransTotal");
}
matusetransDS.close();

if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+matusetransDS.getName() + "] >>> closed");
}


//Caculations
var totalitems = matrectransTotal + matusetransTotal; 
if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> totalitems: " + totalitems);
}

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> (totalitems!=0) &amp;&amp; (leadtimemonths!=0): " + ((totalitems!=0) &amp;&amp; (leadtimemonths!=0)));
}
if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> maximoDataSet.getInteger(\"delivery\")!=0: " + (maximoDataSet.getInteger("delivery")!=0));
}

if ( (totalitems!=0) &amp;&amp; (leadtimemonths!=0) ) { 
	leadtime = totalitems * leadtimemonths / 30.44;

	if(maximoDataSet.getInteger("delivery")!=0){
		sstock = 2 * maximoDataSet.getDouble("delivery") * Math.pow( (leadtime / 30.44), (1/1.6) );
	}
}

newlevel = Math.ceil(sstock + leadtime);

row["newrop"] = newlevel;
row["newsstock"] = Math.ceil(sstock);

var conversion = maximoDataSet.getInteger("conversion");
var revconversion = maximoDataSet.getInteger("revconversion");

if(BirtComp.notEqual(conversion, null) &amp;&amp; conversion!=0) {
	row["cfactor"] = maximoDataSet.getDouble("conversion");
}
else if(BirtComp.notEqual(revconversion, null) &amp;&amp; revconversion!=0) {
	row["cfactor"] = 1.0/maximoDataSet.getDouble("revconversion");
}

var request = reportContext.getHttpServletRequest();
if( BirtComp.notEqual(request,null) ) {
	var session = request.getSession();
	var updStmtList = session.getAttribute(attkey);

	if( BirtComp.equalTo(updStmtList,null) ){
		updStmtList = new Array;
	}
	
	var updateQuery = "update inventory set minlevel=" + newlevel
	+ ", sstock=" +  Math.ceil(sstock)
	+ " where siteid='" +maximoDataSet.getString("siteid")+ "' "
	+ " and itemsetid='" +maximoDataSet.getString("itemsetid") + "' "
	+ " and location='" +maximoDataSet.getString("location") + "' "
	+ " and itemnum='" +maximoDataSet.getString("itemnum") + "' "
	;
	if (scriptLogger.isDebugEnabled()) {
		scriptLogger.debug("["+maximoDataSet.getName() + "] >>> updateQuery: " + updateQuery);
	}
	updStmtList.push(updateQuery);
	
	session.setAttribute(attkey, updStmtList);
}

return (true);]]></method>
