            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

sqlText = "select distinct workperiod.workdate, labor.laborcode, workperiod.starttime, workperiod.endtime, workperiod.workhours "
+ " from workperiod, personcal, wmassignment, labor "
+ " where workperiod.shiftnum = personcal.shiftnum "
+ " and workperiod.calnum = personcal.calnum "
+ " and workperiod.workdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
+ " and workperiod.workdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
+ " and personcal.personid = labor.personid "
+ " and personcal.orgid = labor.orgid "
+ " and wmassignment.laborcode = labor.laborcode "
+ " and wmassignment.orgid = labor.orgid "
+ " and " + params["where"] 
+ " order by workperiod.workdate "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["workdate"] = maximoDataSet.getDate("workdate");	
row["starttime"] = maximoDataSet.getTimestamp("starttime");	
row["endtime"] = maximoDataSet.getTimestamp("endtime");	
row["hours"] = maximoDataSet.getFloat("workhours");	
row["labor"] = maximoDataSet.getString("laborcode");
// queryStarTime must be of type String to avoid timezone conversion and successfully query the database elements	
row["queryStartTime"] = maximoDataSet.getString("starttime");
return (true);]]></method>
