            <method name="open"><![CDATA[timeDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "timeDataSet");
timeDataSet.open();

recordCounter = 0;
counterLimit = rows[0]["hours"] * 2;

dateCounter = MXReportSqlFormat.getCombinedDateAndTime(rows[0]["workdate"], rows[0]["starttime"]);

calWorkdate = Calendar.getInstance();
calWorkdate.setTime(rows[0]["workdate"]);

sdf = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss.S");
queryStartTime = sdf.parse(rows[0]["queryStartTime"], new ParsePosition(0));

calWorkTime = Calendar.getInstance();
calWorkTime.setTime(queryStartTime);

calWorkdate.set(Calendar.HOUR_OF_DAY, calWorkTime.get(Calendar.HOUR_OF_DAY));
calWorkdate.set(Calendar.MINUTE, calWorkTime.get(Calendar.MINUTE));
calWorkdate.set(Calendar.SECOND, calWorkTime.get(Calendar.SECOND));
calWorkdate.set(Calendar.MILLISECOND, calWorkTime.get(Calendar.MILLISECOND));
startDateForQuery = calWorkdate.getTime();
]]></method>
            <method name="fetch"><![CDATA[if(recordCounter > counterLimit) return (false);
else{
recordCounter++;

row["dailyTime"] = dateCounter;
dateUpper = DateTimeSpan.addTime(startDateForQuery, 0, 30, 0);
woDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "woDataSet");
woDataSet.open();

var sqlText = new String();

sqlText = "select wmassignment.laborcode, workorder.wonum, workorder.taskid, "
+ " workorder.description, workorder.assetnum, workorder.location, workorder.estdur "
+ " from workorder, wmassignment " 
+ " where workorder.wonum = wmassignment.wonum "
+ " and workorder.siteid = wmassignment.siteid "
+ " and wmassignment.scheduledate >= " + MXReportSqlFormat.getTimestampFunction(startDateForQuery)
+ " and wmassignment.scheduledate < " + MXReportSqlFormat.getTimestampFunction(dateUpper)
+ " and wmassignment.assignstatus in (select value from synonymdomain where domainid = 'ASSTAT' and maxvalue = 'ASSIGNED') "
+ " and wmassignment.laborcode = ? "
;

woDataSet.setQuery(sqlText);
woDataSet.setQueryParameterValue(1, inputParams["laborcode"].toUpperCase());

if (woDataSet.fetch()){	
row["woDescription"] = woDataSet.getString("description");
row["wonum"] = woDataSet.getString("wonum");
row["taskid"] = woDataSet.getString("taskid");
row["asset"] = woDataSet.getString("assetnum");
row["location"] = woDataSet.getString("location");
row["estdur"] = woDataSet.getFloat("estdur");
}
startDateForQuery = dateUpper;
dateCounter = DateTimeSpan.addTime(dateCounter, 0, 30, 0);

woDataSet.close();
return (true);
}
]]></method>
