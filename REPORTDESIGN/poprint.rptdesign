            <method name="open"><![CDATA[subDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
subDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " select poterm.termid, poterm.description, poterm.ponum, poterm.potermid "
 + " from poterm inner join "
 + " term on "
 + " poterm.orgid = term.orgid "
 + " and poterm.termid = term.termid " 
 + " where poterm.siteid = ? "
 + " and poterm.ponum = ? "
 + " and poterm.revisionnum = ? "
 + " and poterm.sendtovendor = 1 "
 + " order by poterm.ponum, poterm.seqnum "
 ;

subDataSet.setQuery(sqlText);
subDataSet.setQueryParameterValue(1, rows[0]["siteid"]);
subDataSet.setQueryParameterValue(2, rows[0]["ponum"]);
subDataSet.setQueryParameterValue(3, rows[0]["revisionnum"]);]]></method>
            <method name="fetch"><![CDATA[if (!subDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["termid"] = subDataSet.getString("termid");	
row["description"] = subDataSet.getString("description");

// Long Description
	longDescDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "longDescDataSet");
	longDescDataSet.open();
	
	longDescSQL = "select longdescription.ldtext  "
	+ "from longdescription "
    + "where longdescription.ldownertable='POTERM' and longdescription.ldownercol='DESCRIPTION' "
    + "and longdescription.ldkey=" + subDataSet.getString("potermid");
	
	
	longDescDataSet.setQuery(longDescSQL);

	if(longDescDataSet.fetch()) {
		if (longDescDataSet.getString("ldtext") != null){
			row["ldtext"] = longDescDataSet.getString("ldtext").replace('&amp;amp;lt;',"<")
			.replace('&amp;amp;gt;', ">").replace('&amp;amp;quot;', "'");
		}
	}
	
	longDescDataSet.close();
// End Long Description

row["ponum"] = subDataSet.getString("ponum");	


return (true);]]></method>
