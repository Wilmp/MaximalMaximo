            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " select po.ponum, po.siteid, po.description, po.orderdate, po.requireddate, "
 + " po.poid, po.vendor, po.contractrefnum, companies.name, companies.address1, "
 + " companies.address2, companies.address3, companies.address4, companies.phone, "
 + " po.currencycode, po.contact, po.paymentterms, po.freightterms, po.shipvia, "
 + " po.fob, po.purchaseagent, po.billto, companies.fax, companies.orgid, "
 + " po.revisionnum, address.address1 as siteadd1, address.address2 as siteadd2, "
 + " address.address3 as siteadd3, address.address4 as siteadd4, longdescription.ldtext "
 + " from po "
 + " left outer join companies "
 + " on po.vendor = companies.company "
 + " and po.orgid = companies.orgid "
 + " left outer join address "
 + " on po.billto = address.addresscode "
 + " and po.orgid = address.orgid "
 + " left outer join longdescription "
 + " on longdescription.ldownertable='PO' "
 + " and longdescription.ldownercol='DESCRIPTION' "
 + " and longdescription.ldkey=po.poid "
 + " where " + params["where"]
 + " and po.revisionnum = (select max(p.revisionnum) "
 + " from po p "
 + " where p.ponum = po.ponum "
 + " and p.siteid = po.siteid "
 + " group by p.ponum) "
 + " order by po.ponum "
;

maximoDataSet.setQuery(sqlText);]]></method>
