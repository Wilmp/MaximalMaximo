row["manufacturer"] = polineDataSet.getString("manufacturer");
row["modelnum"] = polineDataSet.getString("modelnum");	
row["unitcost"] = polineDataSet.getDouble("unitcost");	
row["linecost"] = polineDataSet.getDouble("linecost");	
row["tax1"] = polineDataSet.getDouble("tax1");	
row["shipto"] = polineDataSet.getString("shipto");
