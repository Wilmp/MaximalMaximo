            <method name="open"><![CDATA[polineDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
polineDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " select poline.polinenum, poline.catalogcode, poline.itemnum, poline.manufacturer, "
 + " poline.conditioncode, po.currencycode,poline.description, po.totalcost, "
 + " poline.orderqty, poline.orderunit, poline.modelnum, poline.unitcost, "
 + " poline.linecost, po.inclusive1, poline.linecost, poline.tax1, "
 + " address.address1 as siteaddr1, address.address2 as siteaddr2, " 
 + " address.address3 as siteaddr3, address.address4 as siteaddr4, poline.shipto "
 + " from po po, poline poline "
 + " left outer join site site "
 + " on poline.tositeid = site.siteid "
 + " and poline.orgid = site.orgid "
 + " left outer join billtoshipto billtoshipto "
 + " on site.siteid = billtoshipto.siteid "
 + " and site.orgid = billtoshipto.orgid "
 + " and poline.shipto = billtoshipto.addresscode "
 + " left outer join address address "
 + " on billtoshipto.addresscode = address.addresscode "
 + " and billtoshipto.orgid = address.orgid "
 + " where po.ponum = poline.ponum "
 + " and po.siteid = poline.siteid "
 + " and po.siteid = ? "
 + " and po.ponum = ? "
 + " and po.revisionnum = poline.revisionnum "
 + " and po.revisionnum = ? "
 + " order by poline.polinenum "
;

polineDataSet.setQuery(sqlText);
polineDataSet.setQueryParameterValue(1, rows[0]["siteid"]);
polineDataSet.setQueryParameterValue(2, rows[0]["ponum"]);
polineDataSet.setQueryParameterValue(3, rows[0]["revisionnum"]);]]></method>
            <method name="fetch"><![CDATA[if (!polineDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["polinenum"] = polineDataSet.getString("polinenum");	
row["catalogcode"] = polineDataSet.getString("catalogcode");	
row["itemnum"] = polineDataSet.getString("itemnum");	
row["conditioncode"] = polineDataSet.getString("conditioncode");	
row["currencycode"] = polineDataSet.getString("currencycode");	
row["description"] = polineDataSet.getString("description");	
