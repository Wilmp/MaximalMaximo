            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//RYZ 20171221: S63.RAP05 Nieuw rapport, foutrapportage NTIFINVTRANS interface berichten
//RYZ 20180920: S74.910 sortering toegevoegd op ontvangstdatum en vervolgens op serienummer.

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " "
+ " select " 
+ " maxintmsgtrk.extsysname, "
+ " maxintmsgtrk.initialdatetime, "
+ " maxintmsgtrkdtl.errortext, "
+ " substr(errortext, 30, 9) as substr, "
+ " asset.assetnum, "
+ " asset.serialnum, "
+ " asset.location "
+ " from maxintmsgtrk "  
+ " join maxintmsgtrkdtl on maxintmsgtrk.meamsgid = maxintmsgtrkdtl.meamsgid "
+ " join asset on assetnum = substr(errortext, 30, 9) and siteid = '9030' "
+ " where maxintmsgtrkdtl.status = 'ERROR' "
+ " and maxintmsgtrk.status = 'DELETED' " 
+ " and maxintmsgtrk.EXTSYSNAME = 'NTIFINVTRANS' "
+ " and maxintmsgtrk.ifacename = 'NTIFINVTRANS' "
+ " and maxintmsgtrk.messagetype = 'Sync' "
+ " and maxintmsgtrkdtl.errortext like 'BMXZZ0026E - Parent of asset %' "
+ " and maxintmsgtrk.initialdatetime > sysdate -7 "
+ " order by maxintmsgtrk.initialdatetime,asset.serialnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["extsysname"] = maximoDataSet.getString("extsysname");
row["initialdatetime"] = maximoDataSet.getTimestamp("initialdatetime");
row["errortext"] = maximoDataSet.getString("errortext");
row["substr"] = maximoDataSet.getString("substr");
row["assetnum"] = maximoDataSet.getString("assetnum");	
row["serialnum"] = maximoDataSet.getString("serialnum");
row["location"] = maximoDataSet.getString("location");

return (true);]]></method>
