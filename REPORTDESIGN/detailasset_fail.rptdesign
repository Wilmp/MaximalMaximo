            <method name="open"><![CDATA[sub1DataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
sub1DataSet.open();

var sub1Query = " select distinct count(workorder.wonum) as numfail, workorder.problemcode, "
+ " max(workorder.reportdate) as max_date, min(workorder.reportdate) as min_date, "
+ " max(workorder.wonum) as lastwonum, "
+ " failurecode.description as failure_description "
+ " from workorder, failurecode, asset "
+ " where workorder.assetnum = asset.assetnum "
+ " and workorder.problemcode is not null "
+ " and workorder.siteid = asset.siteid "
+ " and workorder.problemcode = failurecode.failurecode "
+ " and workorder.orgid = failurecode.orgid "
+ " and asset.assetnum = ? "
+ " and asset.siteid = ? "
+ " and asset.location = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " group by workorder.problemcode, failurecode.description, workorder.assetnum  "
+ " order by numfail desc ";

sub1DataSet.setQuery(sub1Query);
sub1DataSet.setQueryParameterValue(1, rows[0]["assetnum"]);
sub1DataSet.setQueryParameterValue(2, rows[0]["data_siteid"]);
sub1DataSet.setQueryParameterValue(3, rows[0]["data_location"]);]]></method>
            <method name="fetch"><![CDATA[if (!sub1DataSet.fetch())
	return (false);

var numFail = sub1DataSet.getInteger("numfail");
row["problemcode"] = sub1DataSet.getString("problemcode");

row["lastwonum"] = sub1DataSet.getString("lastwonum");
row["numfail"] = numFail;
row["failure_description"] = sub1DataSet.getString("failure_description");
var maxDate = sub1DataSet.getTimestamp("max_date");
var minDate = sub1DataSet.getTimestamp("min_date");

var days = ((maxDate.getTime() - minDate.getTime()) /( numFail * 1000 * 60 * 60 * 24));

row["mtbf"] = days;

lastDateDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "lastDateDataSet");
lastDateDataSet.open();

var lastDateQuery = " select workorder.reportdate from workorder "
+ " where workorder.wonum = ? "
+ " and workorder.problemcode is not null "
+ " and workorder.siteid = ? "
+ " and workorder.problemcode = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
;

lastDateDataSet.setQuery(lastDateQuery);
lastDateDataSet.setQueryParameterValue(1, row["lastwonum"]);
lastDateDataSet.setQueryParameterValue(2, rows[0]["data_siteid"]);
lastDateDataSet.setQueryParameterValue(3, row["problemcode"]);


if (!lastDateDataSet.fetch())
	return (false);
row["lastdate"] = lastDateDataSet.getTimestamp("reportdate");

avgAllDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "avgAllDataSet");
avgAllDataSet.open();

var avgAllQuery = 	" select ((select sum(assetstatus.downtime) from assetstatus, workorder "
+ " where assetstatus.isrunning = 1 "
+ " and assetstatus.wonum = workorder.wonum and workorder.siteid = ? "
+ " and workorder.problemcode = ? "
+ " and workorder.location = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " ) / ( select count(workorder.wonum) from  assetstatus, workorder " 
+ "where assetstatus.isrunning = 1 " 
+ "and assetstatus.wonum = workorder.wonum and workorder.siteid = ? "
+ " and workorder.problemcode = ? "
+ " and workorder.location = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " ) ) as avg_all from workorder ";

avgAllDataSet.setQuery(avgAllQuery);
avgAllDataSet.setQueryParameterValue(1, rows[0]["data_siteid"]);
avgAllDataSet.setQueryParameterValue(2, row["problemcode"]);
avgAllDataSet.setQueryParameterValue(3, rows[0]["data_location"]);
avgAllDataSet.setQueryParameterValue(4, rows[0]["data_siteid"]);
avgAllDataSet.setQueryParameterValue(5, row["problemcode"]);
avgAllDataSet.setQueryParameterValue(6, rows[0]["data_location"]);

if (!avgAllDataSet.fetch())
	return (false);
//row["avg_failures"] = maximoDataSet.getFloat("avg_failures");
row["avg_all"] = avgAllDataSet.getFloat("avg_all");

avgFailuresDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "avgFailuresDataSet");
avgFailuresDataSet.open();

var avgFailuresQuery = 	" select ((select sum(assetstatus.downtime) from assetstatus, workorder "
+ " where assetstatus.isrunning = 1 "
+ " and assetstatus.wonum = workorder.wonum and workorder.siteid = ? "
+ " and workorder.problemcode = ? "
+ " and workorder.location = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " ) / ( select count(workorder.wonum) from  assetstatus, workorder " 
+ "where assetstatus.isrunning = 1 " 
+ "and assetstatus.wonum = workorder.wonum and workorder.siteid = ? "
+ " and workorder.problemcode = ? "
+ " and workorder.location = ? "
+ " and workorder.faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ " and workorder.faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " ) ) as avg_failures from workorder ";

avgFailuresDataSet.setQuery(avgFailuresQuery);
avgFailuresDataSet.setQueryParameterValue(1, rows[0]["data_siteid"]);
avgFailuresDataSet.setQueryParameterValue(2, row["problemcode"]);
avgFailuresDataSet.setQueryParameterValue(3, rows[0]["data_location"]);
avgFailuresDataSet.setQueryParameterValue(4, rows[0]["data_siteid"]);
avgFailuresDataSet.setQueryParameterValue(5, row["problemcode"]);
avgFailuresDataSet.setQueryParameterValue(6, rows[0]["data_location"]);


if (!avgFailuresDataSet.fetch())
	return (false);
row["avg_failures"] = avgFailuresDataSet.getFloat("avg_failures");
return (true);]]></method>
