            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

//if it comes from oeekpi_by_asset.rptdesign where clause has to be built here
if(params["paramstring"].value &amp;&amp; params["paramstring"]=="oeekpi_by_asset.rptdesign") {
	params["where"]= " ( "
		+ " (asset.siteid='" + params["siteid"].toUpperCase() + "' and asset.location='" + params["location"].toUpperCase() + "' ) "
		+ " and ( "
		+ " exists ( "
		+ " select siteid from workorder where ( "
		+ " (faildate<=" + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
		+ " and assetnum='" + params["asset"] + "' "
		+ " and faildate>=" + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " ) "
		+ " ) "
		+ " and (assetnum = asset.assetnum and siteid=asset.siteid ) "
		+ " ) "
		+ " ) "
		+ " ) "
	;
}

if(params["paramstring"].value &amp;&amp; params["paramstring"]=="sumasset_fail.rptdesign") {
	params["where"]= " ( "
		+ " ( asset.assetnum = '" + params["asset"].toUpperCase() + "' and  asset.siteid='" + params["siteid"].toUpperCase() + "' and asset.location='" + params["location"].toUpperCase() + "' ) "
		+ " and ( "
		+ " exists ( "
		+ " select siteid from workorder where ( "
		+ " (faildate<=" + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
		+ " and assetnum='" + params["asset"] + "' "
		+ " and faildate>=" + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " ) "
		+ " ) "
		+ " and (assetnum = asset.assetnum and siteid=asset.siteid ) "
		+ " ) "
		+ " ) "
		+ " ) "
	;
}

var sqlText = "select distinct asset.siteid, asset.moved, asset.location, asset.assetnum, "
+ " asset.description as asset_description,  locations.description as locations_description "
+ " from asset left outer join "
+ " locations on "
+ "  asset.siteid = locations.siteid "
+ "  and asset.location = locations.location  "
+ " where asset.assetnum in ("
+ "  select assetnum "
+ "  from workorder " 
+ "  where problemcode is not null "
+ "  and faildate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) + " "
+ "  and faildate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]) + " "
+ " ) "
+ " and " + params["where"]
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);


row["locations_description"] = maximoDataSet.getString("locations_description");	
row["asset_description"] = maximoDataSet.getString("asset_description");
row["assetnum"] = maximoDataSet.getString("assetnum");	
row["data_siteid"] = maximoDataSet.getString("siteid");	
row["data_location"] = maximoDataSet.getString("location");	

if(params["hasmoved"].value) {
	 row["hasmoved"] = maximoDataSet.getBooleanString("moved");
} else {
	row["hasmoved"] = "";
}

return (true);]]></method>
