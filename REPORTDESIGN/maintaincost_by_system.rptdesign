            <method name="open"><![CDATA[subDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
subDataSet.open();

sqlText = "select lochierarchy.siteid, lochierarchy.systemid, lochierarchy.location, lochierarchy.parent, "
+ " lochierarchy.children, locations.description "
+ " from lochierarchy inner join "
+ " locations on "
+ "  lochierarchy.siteid = locations.siteid "
+ "  and lochierarchy.location = locations.location "
+ " where 2=2 "
+ " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.parent", "=" + rows[0]["location"])
+ " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.siteid", "=" + rows[0]["siteid"])
+ " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.systemid", "=" + rows[0]["systemid"]);
+ " order by lochierarchy.siteid, lochierarchy.systemid, lochierarchy.location"
;

subDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!subDataSet.fetch())
	return (false);

var siteid = subDataSet.getString("siteid");
var location = subDataSet.getString("location");
var systemid = subDataSet.getString("systemid");
row["siteid"] = siteid;
row["location"] = location;
row["systemid"] = systemid;
row["description"] = subDataSet.getString("description");
row["children"] = subDataSet.getInteger("children");
row["haschildren"] = subDataSet.getBooleanString("children");
//row["parent"] = inputParams["parent"];
//row["totalcost"] = 0;	

// HIERARCHY
hierarchyDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "hierarchyDS");
hierarchyDS.open();

var hierarchySQL;
if(hierarchyDS.isDB2() || hierarchyDS.isSQLServer()) {
hierarchySQL = "with ancestor (siteid, location, systemid, parent) "
	+ " as ( "
	+ " select parent.siteid, parent.location, parent.systemid, parent.parent "
	+ " from lochierarchy parent "
	+ " where parent.siteid = ? "
	+ " and parent.location = ? "
	+ " and parent.systemid = ? "
	+ " union all "
	+ " select child.siteid, child.location, child.systemid, child.parent "
	+ " from ancestor parent, lochierarchy child "
	+ " where child.parent = parent.location "
	+ " ) "
	+ " select distinct location "
	+ " from ancestor "
	;
}
if(hierarchyDS.isOracle()) {
	hierarchySQL = "select location "
	+ " from lochierarchy "
	+ " start with siteid = ? "
	+ " and location = ? "
	+ " and systemid = ? "
	+ " connect by nocycle prior siteid=siteid "
	+ " and prior location = parent "
	;
}

hierarchyDS.setQuery(hierarchySQL);
hierarchyDS.setQueryParameterValue(1, siteid);
hierarchyDS.setQueryParameterValue(2, location);
hierarchyDS.setQueryParameterValue(3, systemid);

var thisBranch = new Array();
while(hierarchyDS.fetch()) {
	thisBranch.push(hierarchyDS.getString("location"));
}
hierarchyDS.close();
var hierarchy = "";
if(thisBranch.length > 0) {
	hierarchy = "=" + thisBranch.join(", =");
}
if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("[" + subDataSet.getName() + "]>>> hierarchy: " + hierarchy);
}

//hierarchy end
// LABTRANS
var labtransTotal = 0.0;

labtransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "labtransDS");
labtransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+labtransDS.getName() + "] >>> created");
}

var labtransSQL = "select sum(" + labtransDS.getNullValueFunction("labtrans.linecost", "0") + ") as labtransTotal "
+ " from labtrans "
+ " where labtrans.siteid = ? "
