            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

//for debugging purposes
//params["siteid"]="BEDFORD";
//params["systemid"]="PRIMARY"
//params["paramstring"] = "lala=1||lele=2||lili=3||linkedto=true||parent=BOILER";
//params["paramdelimiter"] = "||";

if(params["paramstring"].value &amp;&amp; params["paramdelimiter"].value) {
	isLinked = BirtComp.equalTo(MXReportUtil.getParameterFromString("linkedto", params["paramstring"], params["paramdelimiter"]), "true");

	parentParam = MXReportUtil.getParameterFromString("parent", params["paramstring"], params["paramdelimiter"]);
	locationParam = MXReportUtil.getParameterFromString("location", params["paramstring"], params["paramdelimiter"]);
}

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> params[\"paramstring\"] : " + params["paramstring"]);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> params[\"paramdelimiter\"] : " + params["paramdelimiter"]);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> isLinked : " + isLinked);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> parentParam : " + parentParam);
}

if(!isLinked) {
	params["where"] += " and lochierarchy.parent is null ";
}
else {
	params["where"] += " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.parent", "=" + parentParam)
	+ " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.location" , "=" + locationParam);
}

if((BirtComp.notEqual(params["siteid"],null))&amp;&amp;(BirtComp.notEqual(params["siteid"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.siteid", params["siteid"].toUpperCase());
}

if((BirtComp.notEqual(params["systemid"],null))&amp;&amp;(BirtComp.notEqual(params["systemid"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("lochierarchy.systemid", params["systemid"].toUpperCase());
}

sqlText = "select lochierarchy.siteid, lochierarchy.systemid, lochierarchy.location, "
+ " lochierarchy.children, lochierarchy.parent, "
+ " locations.description, "
+ " locsystem.description as systemdesc, "
+ " parentloc.description as parentdesc "
+ " from lochierarchy inner join "
+ " locations on "
+ "  lochierarchy.siteid = locations.siteid "
+ "  and lochierarchy.location = locations.location inner join "
+ " locsystem on "
+ "  locsystem.siteid = lochierarchy.siteid "
+ "  and locsystem.systemid = lochierarchy.systemid left outer join "
+ " locations parentloc on "
+ "  lochierarchy.siteid = parentloc.siteid "
+ "  and lochierarchy.parent = parentloc.location "
+ " where " + params["where"]
+ " order by lochierarchy.siteid, lochierarchy.systemid, lochierarchy.location "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

var siteid = maximoDataSet.getString("siteid");
var location = maximoDataSet.getString("location");
row["siteid"] = siteid;
row["location"] = location;
row["description"] = maximoDataSet.getString("description");
row["systemid"] = maximoDataSet.getString("systemid");
row["systemdesc"] = maximoDataSet.getString("systemdesc");
row["children"] = maximoDataSet.getInteger("children");
row["haschildren"] = maximoDataSet.getBooleanString("children");
row["parentdesc"] = maximoDataSet.getString("parentdesc");
row["parent"] = maximoDataSet.getString("parent");
//row["totalcost"] = maximoDataSet.getDouble("totalcost");


// LABTRANS
var labtransTotal = 0.0;

labtransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "labtransDS");
labtransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+labtransDS.getName() + "] >>> created");
}

var labtransSQL = "select sum(" + labtransDS.getNullValueFunction("labtrans.linecost", "0") + ") as labtransTotal "
+ " from labtrans "
+ " where labtrans.siteid = ? "
+ " and labtrans.location = ? "
;
if(params["startdate"].value) {
	labtransSQL+= " and labtrans.startdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
if(params["enddate"].value) {
	labtransSQL+= " and labtrans.startdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
	;
}

labtransDS.setQuery(labtransSQL);
labtransDS.setQueryParameterValue(1, siteid);
labtransDS.setQueryParameterValue(2, location);

if(labtransDS.fetch()) {
	labtransTotal = Math.abs(labtransDS.getDouble("labtransTotal"));
}
labtransDS.close();

// MATUSETRANS
var matusetransTotal = 0.0;

matusetransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "matusetransDS");
matusetransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+matusetransDS.getName() + "] >>> created");
}

var matusetransSQL = "select sum(" + matusetransDS.getNullValueFunction("matusetrans.linecost", "0") + ") as matusetransTotal "
+ " from matusetrans "
+ " where matusetrans.siteid = ? "
+ " and matusetrans.location = ? "
;
if(params["startdate"].value) {
	matusetransSQL+= " and matusetrans.actualdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
if(params["enddate"].value) {
	matusetransSQL+= " and matusetrans.actualdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
	;
}

matusetransDS.setQuery(matusetransSQL);
matusetransDS.setQueryParameterValue(1, siteid);
matusetransDS.setQueryParameterValue(2, location);

if(matusetransDS.fetch()) {
	matusetransTotal = Math.abs(matusetransDS.getDouble("matusetransTotal"));
}
matusetransDS.close();

// TOOLTRANS
var tooltransTotal = 0.0;

tooltransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "tooltransDS");
tooltransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+tooltransDS.getName() + "] >>> created");
}

var tooltransSQL = "select sum(" + tooltransDS.getNullValueFunction("tooltrans.linecost", "0") + ") as tooltransTotal "
+ " from tooltrans "
+ " where tooltrans.siteid = ? "
+ " and tooltrans.location = ? "
;
if(params["startdate"].value) {
	tooltransSQL+= " and tooltrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
if(params["enddate"].value) {
	tooltransSQL+= " and tooltrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
	;
}

tooltransDS.setQuery(tooltransSQL);
tooltransDS.setQueryParameterValue(1, siteid);
tooltransDS.setQueryParameterValue(2, location);

if(tooltransDS.fetch()) {
	tooltransTotal = Math.abs(tooltransDS.getDouble("tooltransTotal"));
}
tooltransDS.close();

// SERVRECTRANS
var servrectransTotal = 0.0;

servrectransDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "servrectransDS");
servrectransDS.open();
if (scriptLogger.isDebugEnabled()) {
	//scriptLogger.debug("["+servrectransDS.getName() + "] >>> created");
}

var servrectransSQL = "select sum(" + servrectransDS.getNullValueFunction("servrectrans.linecost", "0") + ") as servrectransTotal "
+ " from servrectrans "
+ " where servrectrans.siteid = ? "
+ " and servrectrans.location = ? "
;
if(params["startdate"].value) {
	servrectransSQL+= " and servrectrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
	;
}
if(params["enddate"].value) {
	servrectransSQL+= " and servrectrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
	;
}

servrectransDS.setQuery(servrectransSQL);
servrectransDS.setQueryParameterValue(1, siteid);
servrectransDS.setQueryParameterValue(2, location);

if(servrectransDS.fetch()) {
	servrectransTotal = Math.abs(servrectransDS.getDouble("servrectransTotal"));
}
servrectransDS.close();

row["totalcost"] = labtransTotal + matusetransTotal + tooltransTotal + servrectransTotal;

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> labtransTotal      : " + labtransTotal);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> matusetransTotal   : " + matusetransTotal);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> tooltransTotal     : " + tooltransTotal);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> servrectransTotal  : " + servrectransTotal);
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> --------------------------------------- ");
	scriptLogger.debug("["+maximoDataSet.getName() + "] >>> row[\"totalcost\"] : " + row["totalcost"]);
}

return (true);
]]></method>
