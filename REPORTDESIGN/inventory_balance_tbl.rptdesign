            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// ********************
// *	2008/10/07 15:51
// *	08-22130 - No data displays in inventory balance report when data exists. (inventory_balance_tbl.rptdesign)
// *	Changed the if and where creation
// ********************
// * 2009/04/07 
// * 09-11616 - INVENTORY BALANCE REPORT DOES NOT DISPLAY THE LAST ISSUE DATE
// * Changed the join between inventory and matusetrans and matrectrans to the proper MAXRELATIONSHIP
// ********************
// 2009/09/10
// 09-14145 - BIRT INVENTORY BALANCES REPORT SHOWS INCORRECT VALUES FOR SOME  ITEMS [P1]
//  Fixed query, moved all outer joins to subselects to prevent duplicates and removed recursive subquery on fetch()
// ********************

if(params["storeroom"].value) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.location", params["storeroom"].toUpperCase());
}

if(params["site"].value) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"].toUpperCase());
}

if(params["item"].value) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.itemnum", params["item"].toUpperCase());
}

sqlText = "select distinct inventory.costtype, inventory.orgid, inventory.siteid, inventory.location, inventory.itemsetid, inventory.itemnum, "
+ " item.description as itemDesc, "
+ " inventory.issueunit as iUnit, "
+ " ( "
+ " select " + maximoDataSet.getNullValueFunction("sum(invbalances.curbal)","0") + " from invbalances "
+ " where siteid=inventory.siteid and location=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " and ((conditioncode=invcost.conditioncode) or (conditioncode is null and invcost.conditioncode is null))"
+ " ) as cBal, "
+ " ( "
+ " select " + maximoDataSet.getNullValueFunction("sum(invreserve.reservedqty)","0") + " from invreserve "
+ " where storelocsiteid=inventory.siteid and location=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " and ((conditioncode=invcost.conditioncode) or (conditioncode is null and invcost.conditioncode is null))"
+ " ) as rQty, "
+ " invcost.stdcost as sCost, "
+ " invcost.lastcost as lCost, "
+ " invcost.avgcost as aCost, "
+ " ( "
+ " select max(actualdate) from matusetrans "
+ " where issuetype in (select value from synonymdomain where domainid = 'ISSUETYP' and maxvalue = 'ISSUE') "
+ " and siteid=inventory.siteid and storeloc=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " ) as useTransDate, "
+ " ( "
+ " select max(actualdate) from matrectrans "
+ " where issuetype in (select value from synonymdomain where domainid = 'ISSUETYP' and maxvalue = 'RECEIPT') "
+ " and siteid=inventory.siteid and tostoreloc=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " ) as recTransDate, "
+ " ( "
+ " select varvalue from maxvars where varname='DEFISSUECOST' and siteid=inventory.siteid "
+ " ) as maxvarvalue "
+ " from inventory inner join "
+ " item on "
+ " inventory.itemsetid = item.itemsetid "
+ " and inventory.itemnum = item.itemnum inner join "
+ " invcost on "
+ " inventory.siteid = invcost.siteid "
+ " and inventory.location = invcost.location "
+ " and inventory.itemsetid = invcost.itemsetid "
+ " and inventory.itemnum = invcost.itemnum "
+ " where " + params["where"]
+ " and item.itemtype in ( select value from synonymdomain where domainid = 'ITEMTYPE' and maxvalue in ('ITEM') ) "
+ " and inventory.status not in ( select value from synonymdomain where domainid='ITEMSTATUS' and maxvalue in ('OBSOLETE') ) "

+ " union all "
+ " select distinct inventory.costtype, inventory.orgid, inventory.siteid, inventory.location, inventory.itemsetid, inventory.itemnum, "
+ " item.description as itemDesc, "
+ " inventory.issueunit as iUnit, "
+ " (select " + maximoDataSet.getNullValueFunction("sum(invbalances.curbal)","0") + " from invbalances "
+ " where siteid=inventory.siteid and location=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " and ((conditioncode=invlifofifocost.conditioncode) or (conditioncode is null and invlifofifocost.conditioncode is null)) ) as cBal, "
+ " (select " + maximoDataSet.getNullValueFunction("sum(invreserve.reservedqty)","0") + " from invreserve "
+ " where storelocsiteid=inventory.siteid and location=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " and ((conditioncode=invlifofifocost.conditioncode) or (conditioncode is null and invlifofifocost.conditioncode is null)) ) as rQty, "
+ " (select " + maximoDataSet.getNullValueFunction("sum(invlifofifocost.unitcost)","0") + " from invlifofifocost "
+ " where inventory.itemnum = itemnum and inventory.siteid = siteid and inventory.location = location) as sCost, "
+ " 0 as lCost, "
+ " (select (" + maximoDataSet.getNullValueFunction("sum(invlifofifocost.unitcost*invlifofifocost.quantity)","0") + " / " + maximoDataSet.getNullValueFunction("sum(invlifofifocost.quantity)","0") + ") from invlifofifocost "
+ " where inventory.itemnum = itemnum and inventory.siteid = siteid and inventory.location = location) as aCost, "
+ " (select max(actualdate) from matusetrans "
+ " where issuetype in (select value from synonymdomain where domainid = 'ISSUETYP' and maxvalue = 'ISSUE') "
+ " and siteid=inventory.siteid and storeloc=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " ) as useTransDate, "
+ " (select max(actualdate) from matrectrans "
+ " where issuetype in (select value from synonymdomain where domainid = 'ISSUETYP' and maxvalue = 'RECEIPT') "
+ " and siteid=inventory.siteid and tostoreloc=inventory.location and itemsetid=inventory.itemsetid and itemnum=inventory.itemnum "
+ " ) as recTransDate, "
+ " (select varvalue from maxvars where varname='DEFISSUECOST' and siteid=inventory.siteid "
+ " ) as maxvarvalue "
+ " from inventory "
+ " inner join item "
+ " on inventory.itemsetid = item.itemsetid "
+ " and inventory.itemnum = item.itemnum "
+ " inner join invlifofifocost "
+ " on inventory.itemnum = invlifofifocost.itemnum "
+ " and inventory.itemsetid = invlifofifocost.itemsetid "
+ " and inventory.location = invlifofifocost.location "
+ " and inventory.siteid = invlifofifocost.siteid "
+ " where " + params["where"]
+ " and invlifofifocost.costdate = (select max(costdate) from invlifofifocost "
+ " where inventory.itemnum = itemnum "
+ " and inventory.itemsetid = itemsetid "
+ " and inventory.location = location "
+ " and inventory.siteid = siteid) "
+ " order by orgid, siteid, location, itemnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["orgid"] = maximoDataSet.getString("orgid");
row["siteid"] = maximoDataSet.getString("siteid");
row["location"] = maximoDataSet.getString("location");
row["itemnum"] = maximoDataSet.getString("itemnum");
row["itemDesc"] = maximoDataSet.getString("itemDesc");
row["iUnit"] = maximoDataSet.getString("iUnit");
row["cBal"] = maximoDataSet.getDouble("cBal");
row["rQty"] = maximoDataSet.getDouble("rQty");

row["useTransDate"] = maximoDataSet.getTimestamp("useTransDate");
row["recTransDate"] = maximoDataSet.getTimestamp("recTransDate");

row["costType"] = maximoDataSet.getString("costType");
var maxvarvalue = maximoDataSet.getString("maxvarvalue");

if(BirtComp.equalTo("AVGCOST",maxvarvalue)) {
	row["Cost"] = maximoDataSet.getString("aCost");
} else if(BirtComp.equalTo("STDCOST",maxvarvalue)) {
	row["Cost"] = maximoDataSet.getString("sCost");
} else if(BirtComp.equalTo("LASTCOST",maxvarvalue)) {
	row["Cost"] = maximoDataSet.getString("lCost");
}
if (row["costType"] == 'LIFO' || row["costType"] == 'FIFO'){
	row["Cost"] = maximoDataSet.getString("aCost");
	row["extdCost"] = maximoDataSet.getDouble("sCost");
}else{
	row["extdCost"] = row["cBal"]*row["Cost"];
}

return (true);]]></method>
