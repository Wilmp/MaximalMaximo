            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = "select inventory.orgid, inventory.itemnum, inventory.siteid, inventory.location, item.description, item.itemid, "
+ " invbalances.binnum, invbalances.conditioncode, invbalances.curbal, inventory.issueunit, invlifofifocost.unitcost,  "
+ " invlot.lotnum, invlot.useby, invcost.stdcost, invcost.avgcost, "
+ "   (select sum(invb.curbal) as sumbal  "
+ "   from invbalances invb  "
+ "   where invb.itemnum = inventory.itemnum  "
+ "   and invb.siteid = inventory.siteid  "
+ "   and invb.orgid = inventory.orgid "
+ "   and invb.location = inventory.location and invb.curbal > 0) as sumbal, "
+ "   (select sum(invr.reservedqty) as qtyres  "
+ "   from invreserve invr  "
+ "   where invr.itemnum = inventory.itemnum  "
+ "   and invr.siteid = inventory.siteid  "
+ "   and invr.orgid = inventory.orgid "
+ "   and invr.location = inventory.location) as qtyres "
+ "   from item, site, inventory"
+ " left outer join invlot invlot on "
+ " inventory.itemnum = invlot.itemnum "
+ " and inventory.itemsetid = invlot.itemsetid "
+ " and inventory.siteid = invlot.siteid "
+ " and inventory.location = invlot.location "
+ " left outer join invcost invcost on "
+ " inventory.itemnum = invcost.itemnum "
+ " and inventory.itemsetid = invcost.itemsetid "
+ " and inventory.siteid = invcost.siteid "
+ " and inventory.location = invcost.location, "
+ " invbalances "
+ " left outer join invlifofifocost invlifofifocost on "
+ " invbalances.itemnum = invlifofifocost.itemnum "
+ " and invbalances.itemsetid = invlifofifocost.itemsetid "
+ " and invbalances.siteid = invlifofifocost.siteid "
+ " and invbalances.location = invlifofifocost.location "
+ " and invbalances.curbal = invlifofifocost.quantity "
+ "   where item.itemnum = inventory.itemnum  "
+ "   and item.itemsetid = inventory.itemsetid "
+ "   and inventory.siteid = site.siteid  "
+ "   and inventory.orgid = site.orgid "
+ "   and inventory.itemnum = invbalances.itemnum  "
+ "   and inventory.itemsetid = invbalances.itemsetid  "
+ "   and inventory.siteid = invbalances.siteid  "
+ "   and inventory.orgid = invbalances.orgid "
+ "   and inventory.location = invbalances.location "
+ " and (invbalances.lotnum = invlot.lotnum or (invbalances.lotnum is null or invlot.lotnum is null)) "
+ " and (invbalances.conditioncode = invcost.conditioncode or (invbalances.conditioncode is null or invcost.conditioncode is null)) "
+ " and (invlot.useby >= " + MXReportSqlFormat.getCurrentDateFunction() + " or invlot.useby is null) "
+ "   and invbalances.curbal > 0 "
+ " and " + params["where"]
+ " group by inventory.orgid, inventory.siteid, inventory.location, inventory.itemnum, item.description, item.itemid,  invbalances.binnum, invbalances.conditioncode, invbalances.curbal, inventory.issueunit, "
+ "  invlifofifocost.unitcost,   invlot.lotnum, invlot.useby, invcost.stdcost, invcost.avgcost "
+ " order by inventory.orgid, inventory.siteid, inventory.location, inventory.itemnum, item.description, item.itemid,  invbalances.binnum, invbalances.conditioncode, invbalances.curbal, inventory.issueunit, "
+ "  invlifofifocost.unitcost,   invlot.lotnum, invlot.useby, invcost.stdcost, invcost.avgcost "
;

maximoDataSet.setQuery(sqlText);

maximoDataSet.registerDataTranslation("description", "itemid", "ITEM", "DESCRIPTION");]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["orgid"] = maximoDataSet.getString("orgid");
row["itemnum"] = maximoDataSet.getString("itemnum");
row["siteid"] = maximoDataSet.getString("siteid");
row["location"] = maximoDataSet.getString("location");
row["description"] = maximoDataSet.getString("description");
row["sumbal"] = maximoDataSet.getDouble("sumbal");
row["qtyres"] = maximoDataSet.getDouble("qtyres");
row["qtyavail"] = row["sumbal"] - row["qtyres"];

row["binnum"] = maximoDataSet.getString("binnum");
row["conditioncode"] = maximoDataSet.getString("conditioncode");
row["curbal"] = maximoDataSet.getDouble("curbal");
row["issueunit"] = maximoDataSet.getString("issueunit");
row["lotnum"] = maximoDataSet.getString("lotnum");
row["useby"] = maximoDataSet.getDate("useby");
row["stdcost"] = maximoDataSet.getDouble("stdcost") + maximoDataSet.getDouble("unitcost");
row["avgcost"] = maximoDataSet.getDouble("avgcost") + maximoDataSet.getDouble("unitcost");


return(true);]]></method>
