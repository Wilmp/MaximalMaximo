            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

isName = (BirtComp.notEqual(params["name"],null) &amp;&amp; BirtComp.notEqual(params["name"],""));
isLabor = (BirtComp.notEqual(params["laborcode"],null) &amp;&amp; BirtComp.notEqual(params["laborcode"],""));

if (scriptLogger.isDebugEnabled()) {
	scriptLogger.debug("[initialize()] >>> isName:  " + isName);
	scriptLogger.debug("[initialize()] >>> isLabor: " + isLabor);
}

//Sets empty string for non-required parameters that cause problems for being null
if(BirtComp.equalTo(params["name"], null)) {
	params["name"] = "";
}
//Sets empty string for non-required parameters that cause problems for being null
if(BirtComp.equalTo(params["laborcode"], null)) {
	params["laborcode"] = "";
}

var sqlText = new String();

mustStop = true;

//if both name and labor are null values an error must be shown to the user
if(!isName &amp;&amp; !isLabor) {
	mustStop=true;
}

if(!mustStop) {
	// work list and labor are not null
	if (isName &amp;&amp; isLabor) 
	{
		sqlText = "select distinct workperiod.workdate, labor.laborcode, workperiod.starttime, workperiod.endtime, workperiod.workhours "
		+ " from workperiod, personcal, wmassignment, labor "
		+ " where workperiod.shiftnum = personcal.shiftnum "
		+ " and workperiod.calnum = personcal.calnum "
		+ " and workperiod.workdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
		+ " and workperiod.workdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
		+ " and workperiod.workdate not in (select modavail.workdate from modavail, labor "
		+ " where modavail.personid = labor.personid "
		+ " and "
		+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase()) + ") "
		+ " and personcal.personid = labor.personid "
		+ " and personcal.orgid = labor.orgid "
		+ " and wmassignment.assignmentid in " 
		+ " (select objectid from skdactivity, skdproject where skdactivity.skdprojectid=skdproject.skdprojectid and objectname='ASSIGNMENT' and "
	//	+ " and skdproject.name like '%" + params["name"].toUpperCase() + "%' )" 
		+ MXReportSqlFormat.createParamWhereClause("skdproject.name",params["name"].toUpperCase()) + ") "
		+ " and wmassignment.laborcode = labor.laborcode "
		+ " and wmassignment.orgid = labor.orgid "
		+ " and ( exists (select 1 from labor where "
		+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase())
		+ " and (laborcode=wmassignment.laborcode and orgid=wmassignment.orgid ) )) "
	//	+ " and " + params["where"] 
		+ " order by workperiod.workdate "
		;
	}
	// work list is null. labor is not null
	else if (!isName &amp;&amp; isLabor) 
	{
		sqlText = "select distinct workperiod.workdate, labor.laborcode, workperiod.starttime, workperiod.endtime, workperiod.workhours "
		+ " from workperiod, personcal, wmassignment, labor "
		+ " where workperiod.shiftnum = personcal.shiftnum "
		+ " and workperiod.calnum = personcal.calnum "
		+ " and workperiod.workdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
		+ " and workperiod.workdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
		+ " and workperiod.workdate not in (select modavail.workdate from modavail, labor "
		+ " where modavail.personid = labor.personid "
		+ " and "
		+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase()) + ") "
		+ " and personcal.personid = labor.personid "
		+ " and personcal.orgid = labor.orgid "
		+ " and wmassignment.laborcode = labor.laborcode "
		+ " and wmassignment.orgid = labor.orgid "
		+ " and ( exists (select 1 from labor where "
		+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase())
		+ " and (laborcode=wmassignment.laborcode and orgid=wmassignment.orgid ) )) "
	//	+ " and " + params["where"] 
		+ " order by workperiod.workdate "
		;
	}
	// work list is not null. labor is null
	else if (isName &amp;&amp; !isLabor) 
	{
		sqlText = "select distinct workperiod.workdate, labor.laborcode, workperiod.starttime, workperiod.endtime, workperiod.workhours "
		+ " from workperiod, personcal, wmassignment, labor "
		+ " where workperiod.shiftnum = personcal.shiftnum "
		+ " and workperiod.calnum = personcal.calnum "
		+ " and workperiod.workdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
		+ " and workperiod.workdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
	//	+ " and workperiod.workdate not in (select modavail.workdate from modavail, labor "
	//	+ " where modavail.personid = labor.personid "
	//	+ " and "
	//	+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase()) + ") "
		+ " and personcal.personid = labor.personid "
		+ " and personcal.orgid = labor.orgid "
		+ " and wmassignment.assignmentid in " 
		+ " (select objectid from skdactivity, skdproject where skdactivity.skdprojectid=skdproject.skdprojectid and objectname='ASSIGNMENT' and "
	//	+ " and skdproject.name like '%" + params["name"].toUpperCase() + "%' )"  
		+ MXReportSqlFormat.createParamWhereClause("skdproject.name",params["name"].toUpperCase()) + ") "
		+ " and wmassignment.laborcode = labor.laborcode "
		+ " and wmassignment.orgid = labor.orgid "
	//	+ " and ( exists (select 1 from labor where "
	//	+ MXReportSqlFormat.createParamWhereClause("labor.laborcode", params["laborcode"].toUpperCase())
	//	+ " and (laborcode=wmassignment.laborcode and orgid=wmassignment.orgid ) )) "
	//	+ " and " + params["where"] 
		+ " order by workperiod.workdate "
		;
	}
} else {
	// failure query for showing error message
	sqlText = "select distinct workperiod.workdate, labor.laborcode, workperiod.starttime, workperiod.endtime, workperiod.workhours "
		+ " from workperiod, personcal, wmassignment, labor "
		+ " where workperiod.shiftnum = personcal.shiftnum "
		+ " and workperiod.calnum = personcal.calnum "
		+ " and personcal.personid = labor.personid "
		+ " and personcal.orgid = labor.orgid " 
		+ " and workperiod.workdate <> workperiod.workdate "
		+ " and wmassignment.assignmentid in "
		+ " (select objectid from skdactivity, skdproject where skdactivity.skdprojectid=skdproject.skdprojectid and objectname='ASSIGNMENT') "
		+ " and wmassignment.laborcode = labor.laborcode "
		+ " and wmassignment.orgid = labor.orgid "
		;
}

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["workdate"] = maximoDataSet.getDate("workdate");	
row["starttime"] = maximoDataSet.getTimestamp("starttime");	
row["endtime"] = maximoDataSet.getTimestamp("endtime");	
row["hours"] = maximoDataSet.getFloat("workhours");	
row["labor"] = maximoDataSet.getString("laborcode");	
return (true);]]></method>
