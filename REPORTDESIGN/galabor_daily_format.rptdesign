            <method name="open"><![CDATA[timeDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "timeDataSet");
timeDataSet.open();

recordCounter = 0;
counterLimit = rows[0]["hours"] * 2;

dateCounter = MXReportSqlFormat.getCombinedDateAndTime(rows[0]["workdate"], rows[0]["starttime"]);]]></method>
            <method name="fetch"><![CDATA[//if(recordCounter <= 0) return (false);

//if(dateCounter > dateLimit) return (false);
if(recordCounter > counterLimit) {
	return (false);
} else {
	recordCounter++;
	row["dailyTime"] = dateCounter;
	
	dateUpper = DateTimeSpan.addTime(dateCounter, 0, 30, 0 );
	woDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "woDataSet");
	woDataSet.open();
	
	var sqlText = new String();
	
	// work list is not null
	if (isName) 
	{
		sqlText = "select wmassignment.laborcode, workorder.wonum, workorder.taskid, "
		+ " workorder.description, workorder.assetnum, workorder.location, workorder.estdur "
		+ " from workorder, wmassignment " 
		+ " where workorder.wonum = wmassignment.wonum "
		+ " and workorder.siteid = wmassignment.siteid "
		+ " and wmassignment.assignmentid in " 
		+ " (select objectid from skdactivity, skdproject where skdactivity.skdprojectid=skdproject.skdprojectid and objectname='ASSIGNMENT' and "
	//	+ " and skdproject.name like '%" + params["name"].toUpperCase() + "%' )"  
		+ MXReportSqlFormat.createParamWhereClause("skdproject.name",params["name"].toUpperCase()) + ") "
		+ " and wmassignment.scheduledate >= " + MXReportSqlFormat.getTimestampFunction(dateCounter)
		+ " and wmassignment.scheduledate < " + MXReportSqlFormat.getTimestampFunction(dateUpper)
		+ " and wmassignment.assignstatus in (select value from synonymdomain where domainid = 'ASSTAT' and maxvalue = 'ASSIGNED') "
		+ " and "
		+ MXReportSqlFormat.createParamWhereClause("wmassignment.laborcode", rows[0]["labor"].toUpperCase())
		;
	}
	// work list is null
	else if (!isName) 
	{
		sqlText = "select wmassignment.laborcode, workorder.wonum, workorder.taskid, "
		+ " workorder.description, workorder.assetnum, workorder.location, workorder.estdur "
		+ " from workorder, wmassignment " 
		+ " where workorder.wonum = wmassignment.wonum "
		+ " and workorder.siteid = wmassignment.siteid "
		+ " and wmassignment.scheduledate >= " + MXReportSqlFormat.getTimestampFunction(dateCounter)
		+ " and wmassignment.scheduledate < " + MXReportSqlFormat.getTimestampFunction(dateUpper)
		+ " and wmassignment.assignstatus in (select value from synonymdomain where domainid = 'ASSTAT' and maxvalue = 'ASSIGNED') "
		+ " and "
		+ MXReportSqlFormat.createParamWhereClause("wmassignment.laborcode", rows[0]["labor"].toUpperCase())
		;
	}
	
	woDataSet.setQuery(sqlText);
	// Labor is not null
	if (isLabor)
	{
		if (woDataSet.fetch()){	
		row["woDescription"] = woDataSet.getString("description");
		row["wonum"] = woDataSet.getString("wonum");
		row["taskid"] = woDataSet.getString("taskid");
		row["asset"] = woDataSet.getString("assetnum");
		row["location"] = woDataSet.getString("location");
		row["estdur"] = woDataSet.getFloat("estdur");
		}
	}
	else
	{
		if (woDataSet.fetch()){	
	//	scriptLogger.debug("timeDataSet fetch >>> labor code =  " + woDataSet.getString("laborcode"));
		row["woDescription"] = woDataSet.getString("description");
		row["wonum"] =  woDataSet.getString("wonum")+" "+reportContext.getMessage("labor_colon", reportContext.getLocale())+woDataSet.getString("laborcode") ;
		row["taskid"] = woDataSet.getString("taskid");
		row["asset"] = woDataSet.getString("assetnum");
		row["location"] = woDataSet.getString("location");
		row["estdur"] = woDataSet.getFloat("estdur");
		}
	}
	dateCounter = dateUpper;
	woDataSet.close();
	
	return (true);
}]]></method>
