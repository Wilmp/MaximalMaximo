	craftrateDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "craftrateDataSet");
	craftrateDataSet.open();
	
	sqlText = "select cr.standardrate "
	+ " from craftrate cr "
	+ " right outer join ( "
	+ " select craftrate.craftrateid, max(craftrate.revisionnum) as revnum "
	+ " from craftrate "
	+ " where craftrate.craft = '" + craft + "' ";
	
		if (contract != null)
			sqlText = sqlText + " and craftrate.contractnum='" + contract + "' ";
		else
			sqlText = sqlText + " and craftrate.contractnum is null";
			
		if (skilllevel != null)
			sqlText = sqlText + " and craftrate.skilllevel='" + skilllevel + "'";
		else
			sqlText = sqlText + " and craftrate.skilllevel is null";
			
		if  (vendor != null)
			sqlText = sqlText + " and craftrate.vendor='" + vendor + "'";
		else
			sqlText = sqlText + " and craftrate.vendor is null";
	
	sqlText = sqlText + " group by craftrate.craftrateid "
	+ " ) as cr2 "
	+ " on cr.craftrateid = cr2.craftrateid ";
	
	craftrateDataSet.setQuery(sqlText);
	if (craftrateDataSet.fetch())
	{
		laborrate = craftrateDataSet.getDouble("standardrate");
		row["rate"] = craftrateDataSet.getString("standardrate");
		row["linecost"] = laborhrs * quantity * laborrate;
	}
}
else
{
	var hrs = craftDataSet.getDouble("laborhrs");
	
	// This is a Labor entry, get the appropriate fields.
	laborDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "labor");
	laborDataSet.open();
	
	sqlText = "select joblabor.jptask, joblabor.laborcode, joblabor.laborhrs, laborcraftrate.rate, laborcraftrate.inherit,"
	+ " joblabor.orgid, joblabor.craft as craftcraft, laborcraftrate.craft as laborcraft, laborcraftrate.contractnum, laborcraftrate.vendor, laborcraftrate.skilllevel "
	+ " from joblabor inner join laborcraftrate on laborcraftrate.laborcode = joblabor.laborcode" 
	+ " and joblabor.orgid = laborcraftrate.orgid and laborcraftrate.defaultcraft = 1"
	+ " where joblabor.joblaborid = " + craftDataSet.getInteger("joblaborid");
    
	var contract = "";
	var skilllevel = "";
	var vendor = "";
	var orgid = "";
	var craft = "";
	var inherit = true;
	
	laborDataSet.setQuery(sqlText);
	if (laborDataSet.fetch())
	{
		row["laborcode"] = laborDataSet.getString("laborcode");
		if (laborDataSet.getInteger("inherit") != 1)
		{
			// If inherit is false, rate comes from laborcraftrate
			row["rate"] = laborDataSet.getDouble("rate");
			inherit = false;
		}
		else
		{
			contract = laborDataSet.getString("contractnum");
			skilllevel = laborDataSet.getString("skilllevel");
			vendor = laborDataSet.getString("vendor");
			if (row["laborcode"] != null) {
				craft = laborDataSet.getString("laborcraft");
			} else {
				craft = laborDataSet.getString("craftcraft");
			}
			orgid = laborDataSet.getString("orgid");
		}
	}
	laborDataSet.close();
	
	// If inherit is true, we get the rate from craftrate
	if (inherit == true)
	{
		inheritDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "inherit");
		inheritDataSet.open();
		
		sqlText = "select craftrate.standardrate from craftrate left outer join contract"
		+ " on craftrate.contractnum = contract.contractnum and craftrate.orgid = contract.orgid where"
		+ " craftrate.craft = '" + craft + "' and craftrate.orgid = '" + orgid + "'";
		
		if (contract != null)
			sqlText = sqlText + " and craftrate.contractnum='" + contract + "' and contract.status='APPR'";
		else
			sqlText = sqlText + " and craftrate.contractnum is null";
			
		if (skilllevel != null)
			sqlText = sqlText + " and craftrate.skilllevel='" + skilllevel + "'";
		else
			sqlText = sqlText + " and craftrate.skilllevel is null";
			
		if  (vendor != null)
			sqlText = sqlText + " and craftrate.vendor='" + vendor + "'";
		else
			sqlText = sqlText + " and craftrate.vendor is null";
		
		inheritDataSet.setQuery(sqlText);
		
		if (inheritDataSet.fetch())
		{
			row["rate"] = inheritDataSet.getDouble("standardrate");
		}
		inheritDataSet.close();
	}	
	// Use hrs variable, which is a double, rather than row laborhrs value, which is a string
	row["linecost"] = row["rate"] * hrs;

}

return (true);]]></method>
