            <method name="open"><![CDATA[materialsDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
materialsDataSet.open();

var sqlText = new String();

sqlText = "select jobitem.jptask, jobitem.itemnum, jobitem.location, jobitem.vendor, jobitem.itemqty, jobitem.storelocsite,"
+ " item.description," 
+ " (select inventory.category from inventory where jobitem.itemnum = inventory.itemnum" 
+ " and jobitem.itemsetid = inventory.itemsetid" 
+ " and jobitem.storelocsite = inventory.siteid" 
+ " and jobitem.location = inventory.location) as category,"  
+ " invcost.avgcost, invcost.stdcost, invcost.lastcost," 
+ " invlifofifocost.unitcost," 
+ " (select invvendor.lastcost from invvendor where invvendor.itemnum = jobitem.itemnum" 
+ " and invvendor.itemsetid = jobitem.itemsetid" 
+ " and invvendor.orgid = jobitem.orgid" 
+ " and isdefault=1" 
+ " and vendor in (select company from companies where disabled=0 and orgid=jobitem.orgid)) as vend_lastcost" 
+ " from jobitem" 
+ " inner join item" 
+ " on jobitem.itemnum = item.itemnum" 
+ " and jobitem.itemsetid = item.itemsetid"  
+ " left outer join invcost" 
+ " on jobitem.itemnum = invcost.itemnum" 
+ " and jobitem.itemsetid = invcost.itemsetid" 
+ " and jobitem.location = invcost.location" 
+ " and jobitem.siteid = invcost.siteid"  
+ " left outer join invlifofifocost" 
+ " on jobitem.itemnum = invlifofifocost.itemnum" 
+ " and jobitem.itemsetid = invlifofifocost.itemsetid" 
+ " and jobitem.location = invlifofifocost.location"
+ " and jobitem.siteid = invlifofifocost.siteid"  
+ " where jobitem.linetype in (select value from synonymdomain where domainid = 'LINETYPE' and maxvalue = 'ITEM')" 
+ " and jobitem.jobplanid = " + rows[0]["jobplanid"] 
+ " order by jobitem.jptask"
;

materialsDataSet.setQuery(sqlText);

// Look up the DEFISSUECOST value each site to know which field from invcost to display on the report.
defaultIssueCost = MXReportDataSetProvider.create(this.getDataSource().getName(), "defaultIssueCost");
defaultIssueCost.open();

defaultIssueCostSQL = "select maxvars.siteid, maxvars.varvalue from maxvars where maxvars.varname='DEFISSUECOST'";
defaultIssueCost.setQuery(defaultIssueCostSQL);

defissuecost = new Packages.java.util.HashMap();
while (defaultIssueCost.fetch()) {
	// store defissuecost values for each site in global hashmap; can be used throughout report
	defissuecost.put(defaultIssueCost.getString("siteid"), defaultIssueCost.getString("varvalue"));
}

defaultIssueCost.close();]]></method>
            <method name="fetch"><![CDATA[if (!materialsDataSet.fetch())
