// Determine unit cost &amp; line cost based on cost type
var unitcost = 0;
if (vendor != null &amp;&amp; vendor != "")
{
	unitcost = materialsDataSet.getDouble("vend_lastcost");
}
if ((unitcost == null || unitcost == 0) &amp;&amp; (materialsDataSet.getDouble("unitcost") > 0)) {
// GET LIFO / FIFO COST
	unitcost = materialsDataSet.getDouble("unitcost");
}
else if (location != null &amp;&amp; location != "")
{
	var costType = "";
	var site = materialsDataSet.getString("storelocsite");
	costType = defissuecost.get(site);
	if (costType != null &amp;&amp; costType != "")
		unitcost = materialsDataSet.getDouble(costType);
}
row["unitcost"] = unitcost;
row["linecost"] = (qty * unitcost);

return (true);]]></method>
