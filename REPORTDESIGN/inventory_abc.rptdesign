+ " order by inv.svalue desc "
+ " ) cumultable, (" + abcAfterSql + " ) as sumtab) sumtab2 group by newabctype) secondt where abctype=newabctype "
+ nullQuery + " ) nulltab group by abctype order by abctype "
;

