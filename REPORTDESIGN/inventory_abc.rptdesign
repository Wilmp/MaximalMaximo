secondQuery = " select (inventory.issueytd * (decimal((coalesce(invcost.lastcost,0)+coalesce(invlifofifocost.unitcost,0)),10,5))/(decimal((coalesce(tot_ytdissuecost,0)+coalesce(tot_ytdlifofifocost,0)),10,5))) as svalue,"
