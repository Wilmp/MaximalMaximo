            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

cum_ytd = 0;
temp_ytd = 0;

var request = reportContext.getHttpServletRequest();

if( BirtComp.notEqual(request, null) ) {
	var session = request.getSession();
	session.setAttribute(dskey, this.getDataSource().getName());
}

params["paramdelimiter"] = "||";
params["paramstring"] = attkey + params["paramdelimiter"] + dskey;

if( BirtComp.notEqual(params["storeroom"],null) &amp;&amp; BirtComp.notEqual(params["storeroom"], "") ) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.location", params["storeroom"].toUpperCase());
}

if( BirtComp.notEqual(params["site"], null) &amp;&amp; BirtComp.notEqual(params["site"], "") ) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"].toUpperCase());
}

var sqlJoin = " inner join item on inventory.itemnum = item.itemnum and inventory.itemsetid = item.itemsetid "
+ " and (inventory.abctype not in('N', 'NA') or inventory.abctype is null) "
+ " left outer join invcost on inventory.location = invcost.location and inventory.itemnum = invcost.itemnum "
+ " and inventory.itemsetid = invcost.itemsetid and inventory.siteid = invcost.siteid "
+ " left outer join invlifofifocost on inventory.location = invlifofifocost.location and inventory.itemnum = invlifofifocost.itemnum "
+ " and inventory.itemsetid = invlifofifocost.itemsetid and inventory.siteid = invlifofifocost.siteid "
;

var sqlTotalIssueCost = " ( select  sum(coalesce(inventory.issueytd,0) * coalesce(invcost.lastcost,0)) as tot_ytdissuecost, sum(coalesce(inventory.issueytd,0) * coalesce(invlifofifocost.unitcost,0)) as tot_ytdlifofifocost "
+ " from inventory " + sqlJoin + " where " + params["where"] + " ) "
;

if(!(maximoDataSet.isOracle())) sqlTotalIssueCost += " as totytdissuecost ";



var sqlText = " select inventory.itemnum, item.description, inventory.abctype, inventory.ccf, "
+ " sum(inventory.issueytd) as issueytd, sum(invcost.lastcost) as lastcost, sum(invlifofifocost.unitcost) as unitcost, sum((coalesce(inventory.issueytd,0) * coalesce(invcost.lastcost,0))) as ytd_issuecost, sum((coalesce(inventory.issueytd,0) * coalesce(invlifofifocost.unitcost,0))) as ytd_lifofifocost, "
+ " tot_ytdissuecost, tot_ytdlifofifocost, inventory.itemsetid from inventory " 
+ sqlJoin + "," + sqlTotalIssueCost + " where " + params["where"]
+ " group by inventory.itemnum, inventory.abctype, inventory.ccf, item.description, inventory.itemsetid, tot_ytdlifofifocost, tot_ytdissuecost "
+ " order by ytd_issuecost desc "
;

//scriptLogger.debug("sql Query ==> " + sqlText);

maximoDataSet.setQuery(sqlText);

abcAfterDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "abcAfterDataSet");
abcAfterDataSet.open();



var abcAfterSql=new String();

if(abcAfterDataSet.isOracle()){
abcAfterSql = " select * from "
+ " (select " + sumFunction(abcAfterDataSet, "replace(maxvars.varvalue,',','')") + " as a_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  )  ";
abcAfterSql += ", (select " + sumFunction(abcAfterDataSet, "replace(maxvars.varvalue,',','')") + " as b_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) ";
abcAfterSql += ", (select " + sumFunction(abcAfterDataSet, "replace(maxvars.varvalue,',','')") + " as c_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT', 'C_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) "
;
}

if(abcAfterDataSet.isDB2()){
abcAfterSql = " select a_abcafter, b_abcafter, c_abcafter from ";
abcAfterSql += " (select sum(decimal((a_abcafter),10,5)) as a_abcafter from ( select(replace(maxvars.varvalue,',','')) as a_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as a) atable,  "; 
abcAfterSql += " (select sum(decimal((b_abcafter),10,5)) as b_abcafter from ( select(replace(maxvars.varvalue,',','')) as b_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as b) btable,  "; 
abcAfterSql += " (select sum(decimal((c_abcafter),10,5)) as c_abcafter from ( select(replace(maxvars.varvalue,',','')) as c_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT', 'C_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as c) ctable  "; 
}

if(abcAfterDataSet.isSQLServer()){
abcAfterSql = " select a_abcafter, b_abcafter, c_abcafter from ";
abcAfterSql += " (select sum(cast((a_abcafter) as float)) as a_abcafter from ( select(replace(maxvars.varvalue,',','')) as a_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as a) atable,  "; 
abcAfterSql += " (select sum(cast((b_abcafter) as float)) as b_abcafter from ( select(replace(maxvars.varvalue,',','')) as b_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as b) btable,  "; 
abcAfterSql += " (select sum(cast((c_abcafter) as float)) as c_abcafter from ( select(replace(maxvars.varvalue,',','')) as c_abcafter from maxvars where maxvars.varname in ('A_BREAKPOINT', 'B_BREAKPOINT', 'C_BREAKPOINT') and maxvars.orgid = '" + orgid + "'  ) as c) ctable  "; 
}

abcAfterDataSet.setQuery(abcAfterSql);

if(abcAfterDataSet.fetch()){
a_abcafter = abcAfterDataSet.getFloat("a_abcafter");
b_abcafter = abcAfterDataSet.getFloat("b_abcafter");
c_abcafter = abcAfterDataSet.getFloat("c_abcafter");
}
abcAfterDataSet.close();

ccfAfterDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "ccfAfterDataSet");
ccfAfterDataSet.open();

var ccfAfterSql = " select * from "
+ " (select " + sumFunction(ccfAfterDataSet, "replace(maxvars.varvalue,',','')") + " as a_ccfafter from maxvars where maxvars.varname in ('A_CCF') and maxvars.orgid = '" + orgid + "' ) ";

if(!(ccfAfterDataSet.isOracle())) ccfAfterSql += " as a, ";
else  ccfAfterSql += " , ";

ccfAfterSql += " (select " + sumFunction(ccfAfterDataSet, "replace(maxvars.varvalue,',','')") + " as b_ccfafter from maxvars where maxvars.varname in ('B_CCF') and maxvars.orgid = '" + orgid + "' ) ";

if(!(ccfAfterDataSet.isOracle())) ccfAfterSql += " as b, ";
else  ccfAfterSql += " , ";

ccfAfterSql += " (select " + sumFunction(ccfAfterDataSet, "replace(maxvars.varvalue,',','')") + " as c_ccfafter from maxvars where maxvars.varname in ('C_CCF') and maxvars.orgid = '" + orgid + "' ) ";

if(!(ccfAfterDataSet.isOracle())) ccfAfterSql += " as c ";

ccfAfterDataSet.setQuery(ccfAfterSql);

if(ccfAfterDataSet.fetch()){
a_ccfafter = ccfAfterDataSet.getFloat("a_ccfafter");
b_ccfafter = ccfAfterDataSet.getFloat("b_ccfafter");
c_ccfafter = ccfAfterDataSet.getFloat("c_ccfafter");
}
ccfAfterDataSet.close();]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

tot_ytdissuecost = maximoDataSet.getFloat("tot_ytdissuecost") + maximoDataSet.getFloat("tot_ytdlifofifocost");
ytd_issuecost = maximoDataSet.getFloat("ytd_issuecost") + maximoDataSet.getFloat("ytd_lifofifocost");

cum_ytd = (ytd_issuecost/tot_ytdissuecost) + temp_ytd;
temp_ytd = cum_ytd;

if(cum_ytd <= a_abcafter){
	abcafter = "A";
	ccfafter = a_ccfafter;
}
else if (cum_ytd >= b_abcafter){
	abcafter = "C";
	ccfafter = c_ccfafter;
}
else{
	abcafter = "B";
	ccfafter =	b_ccfafter;
}

row["itemnum"] = maximoDataSet.getString("itemnum");
row["description"] = maximoDataSet.getString("description");
row["abctype"] = maximoDataSet.getString("abctype");
row["abcafter"] = abcafter;
row["ccf"] = maximoDataSet.getString("ccf");
row["ccfafter"] = ccfafter ;
row["issueytd"] = maximoDataSet.getString("issueytd");
row["lastcost"] = maximoDataSet.getFloat("lastcost") + maximoDataSet.getFloat("unitcost");
row["ytd_issuecost"] = ytd_issuecost;
row["tot_ytdissuecost"] = tot_ytdissuecost;


var request = reportContext.getHttpServletRequest();

if(BirtComp.notEqual(request,null)) {
	var session = request.getSession();
	var updStmtList = session.getAttribute(attkey);
	
	//if (scriptLogger.isDebugEnabled()) {
		//scriptLogger.debug("["+ maximoDataSet.getName() + "] >>> BirtComp.equalTo(updStmtList,null) : '" + BirtComp.equalTo(updStmtList,null) + "'");
	//}
	
	if(BirtComp.equalTo(updStmtList,null)) {
		updStmtList = new Array;
	}
	
	var updateQuery = "update inventory set inventory.abctype = '" + abcafter + "', inventory.ccf = " + ccfafter
	+ " where " + params["where"] + " and (inventory.abctype not in('N', 'NA') or inventory.abctype is null) "
	+ " and inventory.itemnum = '" + maximoDataSet.getString("itemnum") + "'"
	+ " and inventory.itemsetid = '" + maximoDataSet.getString("itemsetid") + "'"
	;

	updStmtList.push(updateQuery);
	
	session.setAttribute(attkey, updStmtList);
}

return (true);]]></method>
