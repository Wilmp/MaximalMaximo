    <method name="initialize"><![CDATA[importPackage(Packages.com.ibm.tivoli.maximo.report.script);

var timeInMs = Date.now();

attkey = "st-inventor_abc_" + params["appname"]+ "_"+ timeInMs;
dskey = "ds-inventor_abc_" + params["appname"]+ "_"+ timeInMs;

mxReportScriptContext = MXReportScriptContext.initialize(reportContext);

mxReportScriptContext.setDefaultLogLevel("DEBUG");
mxReportScriptContext.setDefaultLogFile("C:/temp/inventory_abc.log");

scriptLogger = mxReportScriptContext.getReportScriptLogger();

var orgid = "";
orgIdDataSet = MXReportDataSetProvider.create("maximoDataSource", "orgIdDataSet");
orgIdDataSet.open();
var orgIdSql = " select site.orgid from site where siteid = '" + (params["site"].replace(/[=!,]/,"").toUpperCase()) + "'";

orgIdDataSet.setQuery(orgIdSql);

if(orgIdDataSet.fetch()){
orgid = orgIdDataSet.getString("orgid");
}
orgIdDataSet.close();

function sumFunction(dataSet, field){
	if(dataSet.isDB2())
		return(" sum(decimal(" + field + ", 10, 5))");
	else 
		if(dataSet.isOracle())
			return(" sum(" + field + ")");
		else 
			return(" sum(cast(( "+ field +") as decimal)) ");
}]]></method>
