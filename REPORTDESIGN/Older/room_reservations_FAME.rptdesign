            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	
//var startdate = params["startdate"];
//var enddate = params["enddate"];

var sqlText = new String();

sqlText = " select sr.ticketid "
 + " , sr.siteid "
 + " , sr.description "
 + " , longdescription.ldtext "
 + " , sr.locationrr "
 + " , persona.displayname affectedperson "
 + " , sr.affectedemail "
 + " , sr.affectedphone "
 + " , personb.displayname reportedby "
 + " , sr.targetstart "
 + " , sr.targetfinish "
 + " , sr.catering "
 + ", sr.format "
 + ", sr.lcd "
 + " ,sr.video "
 + " from sr "
 + " left outer join person persona on sr.affectedperson = persona.personid  "
 + " left outer join person personb on sr.reportedby = personb.personid "
 + " left outer join longdescription on sr.ticketuid = longdescription.ldkey  and longdescription.ldownertable='TICKET' and longdescription.ldownercol = 'DESCRIPTION'  "
 + " where sr.type = 'RESERVATION' "
 + " and sr.targetstart between " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"])
 + " and "+ MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"])
 + " and " + MXReportSqlFormat.createParamWhereClause("sr.locationrr", params["location"].toUpperCase())
 + " order by targetstart"
//	 + " and " + params["where"];
	maximoDataSet.setQuery(sqlText);
	
			params["sqltext"] = sqlText;
]]></method>
