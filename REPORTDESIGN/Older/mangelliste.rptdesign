            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();
	// Add MM-Maximo query to sqlText variable.
	sqlText = " SELECT Workorder.Wonum, " 
 + "                 Workorder.siteid, "
 + "                 Woactivity.Taskid, "
 + "                 Woactivity.Wonum as woactwonum,  "
 + "                 Woactivity.Description AS Wodesc,  "
 + "                 Woactivity.Wogroup,  "
 + "                 Woactivity.Wopriority, "
 + "                 Woactivity.Status,  "
 + "                 Woactivity.Assetnum, "
 + "                 Woactivity.Location,  "
 + "                 Locations.Description,  "
 + "                 Longdescription.Ldtext AS Longdesc, "
 + " 								WORKLOGVIEW.DESCRIPTION as worklogdesc, "
 + " 								WORKLOGVIEW.CREATEDATE, "
 + " 								worklogview.createby, "
 + " 								workloglong.LDTEXT as workloglongdesc "
 + "   	   FROM Workorder,  "
 + "               woactivity "
 + "               LEFT OUTER JOIN LOCATIONS "
 + "                                   ON WOACTIVITY.LOCATION = LOCATIONS.LOCATION "
 + "                                  AND WOACTIVITY.ORGID   = LOCATIONS.ORGID  "
 + "                                  AND Woactivity.Siteid  = Locations.Siteid  "
 + "               LEFT OUTER JOIN LONGDESCRIPTION  "
 + "                                   ON Woactivity.Workorderid        = Longdescription.Ldkey  "
 + "                                  AND Longdescription.Ldownertable = 'WORKORDER' "
 + "                                  AND longdescription.ldownercol   ='DESCRIPTION' "
 + " 							left outer join WORKLOGVIEW "
 + " 																	on woactivity.wonum = WORKLOGVIEW.RECORDKEY "
 + " 																 and WORKLOGVIEW.CLASS in ('ACTIVITY') "
 + " 							left outer join LONGDESCRIPTION as workloglong "
 + " 																	on WORKLOGVIEW.WORKLOGID = workloglong.LDKEY "
 + " 																 and workloglong.LDOWNERTABLE = 'WORKLOG' "
 + " 																 and workloglong.ldownercol = 'DESCRIPTION' "
  + "          WHERE WORKORDER.WONUM = WOACTIVITY.WOGROUP "
 + "            AND Workorder.Orgid = Woactivity.Orgid  "
 + "            AND Workorder.Siteid = Woactivity.Siteid "
	 + " and " + params["where"];
	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
