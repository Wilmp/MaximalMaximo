            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = "SELECT workorder.wonum, woactivity.taskid, woactivity.wonum, woactivity.description as wodesc, woactivity.wogroup, woactivity.wopriority, woactivity.status,  woactivity.assetnum, woactivity.location,  locations.description, longdescription.ldtext as longdesc "
+ " FROM WORKORDER "
+ " LEFT OUTER JOIN WOACTIVITY "
+ " ON WORKORDER.WONUM   = WOACTIVITY.WOGROUP "
+ " AND WORKORDER.ORGID  = WOACTIVITY.ORGID "
+ " AND WORKORDER.SITEID = WOACTIVITY.SITEID "
+ " INNER JOIN LOCATIONS "
+ " ON WOACTIVITY.LOCATION = LOCATIONS.LOCATION "
+ " AND WOACTIVITY.ORGID   = LOCATIONS.ORGID "
+ " AND WOACTIVITY.SITEID  = LOCATIONS.SITEID "
+ " INNER JOIN LONGDESCRIPTION "
+ " On Woactivity.Workorderid = Longdescription.Ldkey "
+ " And Longdescription.Ldownertable = 'WOACTIVITY' "
+ " and longdescription.ldownercol ='DESCRIPTION' "
+ " where " + params["where"]
;


maximoDataSet.setQuery(sqlText);]]></method>
