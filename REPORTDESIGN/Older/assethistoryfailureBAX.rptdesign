            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();
	// Add MM-Maximo query to sqlText variable.
	
	params["where"] = params["where"].replace('asset.warrantyexpdate ', 'a_asset.eaudittimestamp >');
	params["where"] = params["where"].replace('asset.installdate ', 'a_asset.eaudittimestamp <');
	
	params["where"] = params["where"].replace('warrantyexpdate ', 'a_asset.eaudittimestamp >');
	params["where"] = params["where"].replace('installdate ', 'a_asset.eaudittimestamp <');
	
	sqlText = " SELECT a_asset.eaudittimestamp eaudittimestamp, "
 + "   a_asset.failurecode failurecode, "
 + "   a_asset.eauditusername eauditusername, "
 + "   asset.assetnum assetnum, "
 + "   asset.description assetdescription, "
 + "   asset.parent parent, "
 + "   assetparent.description assetparentdescription, "
 + "   asset.location location, "
 + "   locations.description locationdescription, "
 + "   coalesce(asset.serialnum,'N/A') serialnum, "
 + "   asset.orgid orgid, "
 + "   asset.siteid siteid, "
 + " (select reason from logintracking where transid = esigtransid and app = 'ASSET') reason "
 + " FROM asset "
 + "   LEFT OUTER JOIN a_asset "
 + "     ON asset.assetuid = a_asset.assetuid "
 + "   left outer join asset assetparent "
 + "     on asset.parent = assetparent.assetnum "
 + "     and asset.orgid = assetparent.orgid "
 + "     and asset.siteid = assetparent.siteid "
 + "   left outer join locations "
 + "     on asset.location = locations.location "
 + "     and asset.orgid = locations.orgid "
 + "     and asset.siteid = locations.siteid"
	+ " where " + params["where"] 
 	+ " ";
	
	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);
	// Add a line for each output column
	row["ASSETDESCRIPTION"] = maximoDataSet.getString("ASSETDESCRIPTION");
	row["ASSETNUM"] = maximoDataSet.getString("ASSETNUM");
	row["ASSETPARENTDESCRIPTION"] = maximoDataSet.getString("ASSETPARENTDESCRIPTION");
	row["EAUDITTIMESTAMP"] = maximoDataSet.getTimestamp("EAUDITTIMESTAMP");
	row["EAUDITUSERNAME"] = maximoDataSet.getString("EAUDITUSERNAME");
	row["FAILURECODE"] = maximoDataSet.getString("FAILURECODE");
	row["LOCATION"] = maximoDataSet.getString("LOCATION");
	row["LOCATIONDESCRIPTION"] = maximoDataSet.getString("LOCATIONDESCRIPTION");
	row["ORGID"] = maximoDataSet.getString("ORGID");
	row["PARENT"] = maximoDataSet.getString("PARENT");
	row["SERIALNUM"] = maximoDataSet.getString("SERIALNUM");
	row["SITEID"] = maximoDataSet.getString("SITEID");
	row["REASON"] = maximoDataSet.getString("REASON");
	return (true);]]></method>
