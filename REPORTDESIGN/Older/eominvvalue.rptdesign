            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();
	// Add MM-Maximo query to sqlText variable.
	sqlText = " SELECT eom_inventory.EOM_INVENTORYID EOM_INVENTORYID, "
 + "   item.DESCRIPTION description, "
 + "   eom_inventory.ORGID orgid, "
 + "   eom_inventory.SITEID siteid, "
 + "   eom_inventory.FINANCIALPERIOD FINANCIALPERIOD, "
 + "   eom_inventory.ITEM item, "
 + "   eom_inventory.LOT lot, "
 + "   eom_inventory.WAREHOUSE warehouse, "
 + "   eom_inventory.LOCATION location, "
 + "   eom_inventory.QUANTITY quantity, "
 + "   eom_inventory.STANDARDCOST standardcost, "
 + "   eom_inventory.LINECOST invvalue, "
 + "   inventory.binnum bin, "
 + "   site.description sitedesc "
 + " FROM eom_inventory "
 + " LEFT OUTER JOIN site "
 + " ON eom_inventory.siteid = site.siteid"
 + " LEFT OUTER JOIN item "
 + " ON eom_inventory.item = item.itemnum "
 + " LEFT OUTER JOIN inventory "
 + " ON eom_inventory.item = inventory.itemnum "
 + " AND eom_inventory.warehouse = inventory.location "	
 	+ " where eom_inventory.FINANCIALPERIOD = '" + params["finperiod"] + "' "
 	+ " and eom_inventory.siteid = (select defsite from maxuser where loginid = '" + params["userName"] + "') "	
 	+ " order by eom_inventory.WAREHOUSE, eom_inventory.ITEM " ;
	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);
	// Add a line for each output column
	row["DESCRIPTION"] = maximoDataSet.getString("DESCRIPTION");
	row["EOM_INVENTORYID"] = maximoDataSet.getFloat("EOM_INVENTORYID");
	row["FINANCIALPERIOD"] = maximoDataSet.getString("FINANCIALPERIOD");
	row["INVVALUE"] = maximoDataSet.getFloat("INVVALUE");
	row["ITEM"] = maximoDataSet.getString("ITEM");
	row["BIN"] = maximoDataSet.getString("BIN");
	row["LOCATION"] = maximoDataSet.getString("LOCATION");
	row["LOT"] = maximoDataSet.getString("LOT");
	row["ORGID"] = maximoDataSet.getString("ORGID");
	row["QUANTITY"] = maximoDataSet.getFloat("QUANTITY");
	row["SITEDESC"] = maximoDataSet.getString("SITEDESC");
	row["SITEID"] = maximoDataSet.getString("SITEID");
	row["STANDARDCOST"] = maximoDataSet.getFloat("STANDARDCOST");
	row["WAREHOUSE"] = maximoDataSet.getString("WAREHOUSE");
	return (true);]]></method>
