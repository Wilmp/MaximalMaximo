            <property name="queryText">SELECT   SPAREPART.QUANTITY 
, SPAREPART.DESCRIPTION sparedesc
 , SPAREPART.ITEMNUM 
 , NVL(ITEM.DESCRIPTION,' ') DESCRIPTION
 From Sparepart, Item 
 WHERE SPAREPART.ITEMNUM = ITEM.ITEMNUM  
 AND SPAREPART.itemsetid = ITEM.itemsetid
 AND sparepart.assetnum =  ?
and sparepart.siteid = ?</property>
