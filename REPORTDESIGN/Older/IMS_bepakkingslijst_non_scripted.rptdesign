            <property name="queryText">SELECT   asset.assetnum 
 , nvl(ASSET.DESCRIPTION,' ') assetdesc
 , asset.roepnummer 
 , asset.kenteken 
 , nvl(locations.description,' ') locdesc 
 , asset.INSTALLDATE 
 , nvl (asset.LOCATION,' ') LOCATION
 , asset.BEHEERDER 
 , asset.EIGENAAR 
 , asset.aangemaaktdoor 
 , asset.doorzettennaar 
 , NVL (asset.SERIALNUM,' ') SERIALNUM
 , asset.SITEID 
 from     asset 
 left outer join locations on  asset.location = locations.location and asset.siteid = locations.siteid  and asset.orgid = locations.orgid  
 where asset.type='ALGEMEEN'</property>
