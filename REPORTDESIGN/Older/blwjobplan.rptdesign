            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();
	// Add MM-Maximo query to sqlText variable.
	sqlText = " SELECT jobplan.jpnum jpnum, "
 + "   jobplan.jobplanid jobplanid, "
 + "   jobplan.description jpdesc, "
 + "   jobplan.orgid orgid, "
 + "   jobplan.siteid siteid, "
 + "   jobplan.status status, "
 + "   rfw.rfwnum jpRFW, "
 + "   jobplan.priority jpprio, "
 + "   jobplan.supervisor supervisor, "
 + "   jobplan.laborcode lead, "
 + "   laborperson.displayname leadname, "
 + "   dept.description departmentdesc, "
 + "   DECODE(jobplan.x_jp05, 1, 'Y', 0, 'N', 'N' ) assuranceact, "
 + "   jobplan.jpduration jpduration, "
 + "   jobplan.x_jp03 workpermit, "
 + "   jobplan.x_jp01 shutdown, "
 + "   REPLACE(longdesc.ldtext, chr(13), chr(32)) plh1, "
 + "   'placeholder2' plh2, "
 + "   'placeholder3' plh3 "
 + " FROM jobplan "
 + "     LEFT OUTER JOIN person laborperson "
 + "         ON jobplan.laborcode = laborperson.personid "
 + "             LEFT OUTER JOIN alndomain dept "
 + "                   ON dept.domainid           = 'DEPARTMENT' "
 + "                   AND laborperson.department = dept.value "
 + "     LEFT OUTER JOIN longdescription longdesc "
 + "		 ON jobplan.jobplanid = longdesc.ldkey "
 + "		 AND longdesc.ldownertable = 'JOBPLAN' "
 + "		 AND longdesc.ldownercol = 'DESCRIPTION' "
 + "     LEFT OUTER JOIN plusgrfw rfw "
 + "		 ON jobplan.plusgrfwid = rfw.plusgrfwid "
 	+ " where " + params["where"];
	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);
	// Add a line for each output column
	row["ASSURANCEACT"] = maximoDataSet.getString("ASSURANCEACT");
	row["DEPARTMENTDESC"] = maximoDataSet.getString("DEPARTMENTDESC");
	row["JOBPLANID"] = maximoDataSet.getInteger("JOBPLANID");
	row["JPDESC"] = maximoDataSet.getString("JPDESC");
	row["JPDURATION"] = maximoDataSet.getFloat("JPDURATION");
	row["JPNUM"] = maximoDataSet.getString("JPNUM");
	row["JPPRIO"] = maximoDataSet.getFloat("JPPRIO");
	row["JPRFW"] = maximoDataSet.getString("JPRFW");
	row["LEAD"] = maximoDataSet.getString("LEAD");
	row["LEADNAME"] = maximoDataSet.getString("LEADNAME");
	row["ORGID"] = maximoDataSet.getString("ORGID");
	row["PLH1"] = maximoDataSet.getString("PLH1");
	row["PLH2"] = maximoDataSet.getString("PLH2");
	row["PLH3"] = maximoDataSet.getString("PLH3");
	row["SHUTDOWN"] = maximoDataSet.getString("SHUTDOWN");
	row["SITEID"] = maximoDataSet.getString("SITEID");
	row["STATUS"] = maximoDataSet.getString("STATUS");
	row["SUPERVISOR"] = maximoDataSet.getString("SUPERVISOR");
	row["WORKPERMIT"] = maximoDataSet.getString("WORKPERMIT");
	return (true);]]></method>
