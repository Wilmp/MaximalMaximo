            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

sqlText = " "
+ " select distinct  "
+ "   asset.assetnum  "
+ " , asset.siteid "
+ " , asset.plusareg "  
+ " , asset.serialnum " 
+ " , asset.description "  
+ " , plusacacat.item "
+ " , plusaanbdistatus.status  "
+ " , case   "
+ "      when plusaanbdistatus.status  = 'INVALID' then 'R' "
+ "      when plusaanbdistatus.status  = 'ALERT'   then 'A' "
+ "      when plusaanbdistatus.status  = 'OK'      then 'G' "
+ "   else 'R'   "
+ "   end status_colour "
+ " , plusaanbdistatus.updatedtime   "
+ " , plusasaonoff.label "
+ " , plusasaonoff.ontime"
+ " , asset.parent "
+ " , (select distinct plusasaonoff.label from plusasaonoff where plusasaonoff.assetid = asset_parent.assetid)  description_parent "
+ " , plusasaonoff.lcn "
+ " , plusacmfleet.name "

+ " from asset "
+ " inner join plusacacat on asset.plusacatid = plusacacat.plusacacatid "
+ " left outer join plusasaonoff  on asset.assetid = plusasaonoff.assetid "
+ " inner join plusaanbdistatus on plusasaonoff.assetid  = plusaanbdistatus.assetid "
+ " and plusaanbdistatus.statusedtime <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["asofdate"]) 
+ " and (plusaanbdistatus.inactivetime is null or plusaanbdistatus.inactivetime > " + MXReportSqlFormat.getStartDayTimestampFunction(params["asofdate"]) + ") "

+ " left outer join plusasafleet on asset.assetid = plusasafleet.assetid "
+ " left outer join plusacmfleet on plusasafleet.fleetid = plusacmfleet.plusacmfleetid "
+ " left outer join asset asset_parent on asset.parent = asset_parent.assetnum and asset.siteid = asset_parent.siteid   "

+ " where " + params["where"] 

+ " and  plusaanbdistatus.currentconfig = 1 and plusaanbdistatus.inactivetime is null  "
+ " and  plusaanbdistatus.updatedtime between " + MXReportSqlFormat.getStartDayTimestampFunction(params["asofdate"]) + " and " + MXReportSqlFormat.getEndDayTimestampFunction(params["asofdate"]) 
 
+ " order by asset.assetnum, plusacmfleet.name " ;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["assetnum"]     = maximoDataSet.getString("assetnum");
row["siteid"]       = maximoDataSet.getString("siteid");
row["plusareg"]     = maximoDataSet.getString("plusareg");
row["serialnum"]    = maximoDataSet.getString("serialnum");
row["description"]  = maximoDataSet.getString("description");
row["item"]         = maximoDataSet.getString("item");
row["status"]       = maximoDataSet.getString("status");
row["status_colour"]= maximoDataSet.getString("status_colour");
row["updatedtime"]  = maximoDataSet.getTimestamp("updatedtime");
row["label"]        = maximoDataSet.getString("label");
row["ontime"]       = maximoDataSet.getTimestamp("ontime");
row["parent"]		= maximoDataSet.getString("parent");
row["description_parent"]  = maximoDataSet.getString("description_parent");
row["lcn"]          = maximoDataSet.getString("lcn");
row["name"]         = maximoDataSet.getString("name");

return (true);]]></method>
