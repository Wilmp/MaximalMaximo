            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

var currDate = new Date();
var firstDate = MXReportSqlFormat.getStartDayTimestamp(currDate);
var lastDate = MXReportSqlFormat.getEndDayTimestamp(DateTimeSpan.addDate(currDate, -1,0,0));

var request = reportContext.getHttpServletRequest();
if(request != null){
	var session = request.getSession();
	session.setAttribute(dskey, this.getDataSource().getName());
}

params["paramdelimiter"] = "||";
params["paramstring"] = attkey + params["paramdelimiter"] + dskey;

// Add query to sqlText variable.
sqlText = "select inventory.siteid, inventory.location, inventory.itemnum, inventory.itemsetid, item.description, inventory.orderunit, inventory.orderqty, inventory.issueunit, "  
+ " sum(invcost.stdcost) as stdcost, sum(invcost.avgcost) as avgcost, sum(invlifofifocost.unitcost) as unitcost, abs(sum(matusetrans.quantity)) as totalunits  "
+ " from item item, inventory inventory "
+ " left outer join invcost invcost on "
+ " inventory.itemnum = invcost.itemnum " 
+ " and inventory.itemsetid = invcost.itemsetid "  
+ " and inventory.location = invcost.location "
+ " and inventory.siteid = invcost.siteid "
+ " left outer join invlifofifocost invlifofifocost on "
+ " inventory.itemnum = invlifofifocost.itemnum "
+ " and inventory.itemsetid = invlifofifocost.itemsetid "  
+ " and inventory.location = invlifofifocost.location "
+ " and inventory.siteid = invlifofifocost.siteid "
+ " left outer join matusetrans on "
+ " matusetrans.transdate between " + MXReportSqlFormat.getTimestampFunction(firstDate) 
+ " and " + MXReportSqlFormat.getTimestampFunction(lastDate)
+ " and matusetrans.itemnum = inventory.itemnum  "
+ " and matusetrans.storeloc = inventory.location "
+ " and matusetrans.itemsetid = inventory.itemsetid "
+ " and matusetrans.siteid = inventory.siteid "
+ " where item.itemnum = inventory.itemnum "
+ " and item.itemsetid = inventory.itemsetid "
+ " and item.itemtype in (select value from synonymdomain where domainid = 'ITEMTYPE' and maxvalue = 'ITEM') "
+ " and (inventory.orderunit is not null or inventory.orderunit <> '') "
+ " and " + params["where"]
+ " group by inventory.siteid, inventory.location, inventory.itemnum, inventory.itemsetid, item.description, inventory.orderunit, inventory.orderqty, inventory.issueunit "
+ " order by inventory.siteid, inventory.location, inventory.itemnum, inventory.itemsetid, item.description, inventory.orderunit, inventory.orderqty, inventory.issueunit "
;

maximoDataSet.setQuery(sqlText);


//return (true);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.

row["location"] = maximoDataSet.getString("location");
row["itemnum"] = maximoDataSet.getString("itemnum");
row["description"] = maximoDataSet.getString("description");
//for defunitcost check below
row["stdcost"] = maximoDataSet.getDouble("stdcost");
row["avgcost"] = maximoDataSet.getDouble("avgcost");
row["orderunit"] = maximoDataSet.getString("orderunit");
row["orderqty"] = maximoDataSet.getDouble("orderqty");
row["issueunit"] = maximoDataSet.getString("issueunit");
row["itemsetid"] = maximoDataSet.getString("itemsetid");
row["totalunits"] = maximoDataSet.getDouble("totalunits");
row["unitcost"] = maximoDataSet.getDouble("unitcost");

//getting defissuecost
maxvarDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "maxvarDS");
maxvarDS.open();

var maxvarSQL = new String();
var defissuecost = new String();

// Add query to sqlText variable.
maxvarSQL = "select varvalue from maxvars "
// Include the Maximo where clause
+ " where varname = 'DEFISSUECOST' "
+ " and siteid=? "
;

maxvarDS.setQuery(maxvarSQL);
maxvarDS.setQueryParameterValue(1, params["siteid"]);

if (maxvarDS.fetch()) {
	defissuecost = maxvarDS.getString("varvalue");
}

maxvarDS.close();


if(row["totalunits"].value=null) {
	row["totalunits"]=0;
}
var defunitcost=0;

if(((maximoDataSet.getDouble("stdcost") == 0) || (maximoDataSet.getDouble("stdcost") == null)) &amp;&amp; 
((maximoDataSet.getDouble("avgcost") == 0) || (maximoDataSet.getDouble("avgcost") == null))) {
	defunitcost = maximoDataSet.getDouble("unitcost")
}
else if(defissuecost.value=="STDCOST") {
	defunitcost = maximoDataSet.getDouble("stdcost");
}
else {
	defunitcost = maximoDataSet.getDouble("avgcost");
}

defunitcost = (defunitcost * params["cryCost"].value)/100;

if( !(maximoDataSet.getString("orderunit").equals(maximoDataSet.getString("issueunit"))) ) {

	conversionDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "conversionDataSet");
	conversionDataSet.open();
	
	conversionSQL = "select conversion "
	+ " from conversion "
	+ " where frommeasureunit=? "
	+ " and tomeasureunit=? "
	+ " and itemnum=? "
	+ " and itemsetid=? "
	
	conversionDataSet.setQuery(conversionSQL);
	conversionDataSet.setQueryParameterValue(1, maximoDataSet.getString("orderunit"));
	conversionDataSet.setQueryParameterValue(2, maximoDataSet.getString("issueunit"));
	conversionDataSet.setQueryParameterValue(3, maximoDataSet.getString("itemnum"));
	conversionDataSet.setQueryParameterValue(4, maximoDataSet.getString("itemsetid"));
	
	if(conversionDataSet.fetch()) {
		if(conversionDataSet.getDouble("conversion") != 0) {
			row["conversion"] = 1/conversionDataSet.getDouble("conversion");
		}
		//gotta invert parameters
		else {
			REVconversionDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "REVconversionDataSet");
			REVconversionDataSet.open();
	
			REVconversionDataSet.setQuery(conversionSQL);
			REVconversionDataSet.setQueryParameterValue(1, maximoDataSet.getString("issueUnit"));
			REVconversionDataSet.setQueryParameterValue(2, maximoDataSet.getString("orderunit"));
			REVconversionDataSet.setQueryParameterValue(3, maximoDataSet.getString("itemnum"));
			REVconversionDataSet.setQueryParameterValue(4, maximoDataSet.getString("itemsetid"));
			if(conversionDataSet.fetch()) {
				if(conversionDataSet.getDouble("conversion") != 0) {
					row["conversion"] = REVconversionDataSet.getDouble("conversion");
				}
				else {
					LSTconversionDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), "LSTconversionDataSet");
					LSTconversionDataSet.open();
	
					LSTconversionDataSet.setQuery(conversionSQL);
					LSTconversionDataSet.setQueryParameterValue(1, maximoDataSet.getString("issueUnit"));
					LSTconversionDataSet.setQueryParameterValue(2, maximoDataSet.getString("orderunit"));
					LSTconversionDataSet.setQueryParameterValue(3, "");
					LSTconversionDataSet.setQueryParameterValue(4, maximoDataSet.getString("itemsetid"));
					row["conversion"] = LSTconversionDataSet.getDouble("conversion");
				}
			}
		}
	}
	
	conversionDataSet.close();
}
else {
	row["conversion"]=1;
}
var newEoq = row["totalunits"] * 2;

if(row["conversion"] == 0) {
	newEoq = 0;
}
else {
	newEoq = (newEoq / row["conversion"]*params["ordCost"]);
}

if(defunitcost == 0) {
	newEoq = 0;
}
else {
	newEoq = newEoq/defunitcost;
}

if(newEoq.value>0) {
	newEoq = Math.sqrt(newEoq.value);
}
else {
	newEoq = 1;
}
//last colum neweoq
row["neweoq"] = newEoq;


var request = reportContext.getHttpServletRequest();
if(request != null) {
	var session = request.getSession();
	var updStmtList = session.getAttribute(attkey);

	if(updStmtList==null){
		updStmtList = new Array;
	}

	var updateQuery = "update inventory set orderqty=" + newEoq 
	+ " where itemsetid='" +maximoDataSet.getString("itemsetid") + "' "
	+ " and siteid='" +maximoDataSet.getString("siteid")+ "' "
	+ " and itemnum='" + maximoDataSet.getString("itemnum") + "' "
	+ " and location='" + maximoDataSet.getString("location") + "'"
	;
	updStmtList.push(updateQuery);
	
	session.setAttribute(attkey, updStmtList);
}

return (true);]]></method>
