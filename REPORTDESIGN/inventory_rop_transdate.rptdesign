            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var request = reportContext.getHttpServletRequest();
if(request != null){
	var session = request.getSession();
	session.setAttribute(dskey, this.getDataSource().getName());
}

var mutDateSql = "1=1";
var mrtDateSql = "1=1";

params["paramdelimiter"] = "||";
params["paramstring"] = attkey + params["paramdelimiter"] + dskey;

var sqlText = new String();

if((BirtComp.notEqual(params["storeroom"],null))&amp;&amp;(BirtComp.notEqual(params["storeroom"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.location", params["storeroom"].toUpperCase());
}
if((BirtComp.notEqual(params["site"],null))&amp;&amp;(BirtComp.notEqual(params["site"],''))) {
	params["where"]+= " and " + MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"].toUpperCase());
}

if( BirtComp.notEqual(params["startdate"], null) &amp;&amp; BirtComp.notEqual(params["startdate"], "") ) {
	if( BirtComp.notEqual(params["enddate"], null) &amp;&amp; BirtComp.notEqual(params["enddate"], "") ) {
		mutDateSql = " matusetrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
		+ " and matusetrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
		
		mrtDateSql = " matrectrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]) 
		+ " and matrectrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
	} else {
		mutDateSql = " matusetrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
		
		mrtDateSql = " matrectrans.transdate >= " + MXReportSqlFormat.getStartDayTimestampFunction(params["startdate"]);
	}
} else {
	if( BirtComp.notEqual(params["enddate"], null) &amp;&amp; BirtComp.notEqual(params["enddate"], "") ) {
		mutDateSql = " matusetrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
		
		mrtDateSql = " matrectrans.transdate <= " + MXReportSqlFormat.getEndDayTimestampFunction(params["enddate"]);
	}
}
	
sqlText = "select inventory.siteid, inventory.itemsetid, inventory.location, inventory.itemnum, "
+ " item.description, inventory.orderunit, inventory.costtype, inventory.minlevel, inventory.deliverytime, inventory.sstock, "
+ " ( "
+ " select coalesce(sum(matusetrans.quantity),0) as mut_qty "
+ " from matusetrans "
+ " where " + mutDateSql
+ " and matusetrans.issuetype in (select value from synonymdomain where domainid='ISSUETYP' and maxvalue='ISSUE') "
+ " and matusetrans.siteid = inventory.siteid "
+ " and matusetrans.itemnum = inventory.itemnum "
+ " and matusetrans.itemsetid = inventory.itemsetid "
+ " and matusetrans.storeloc = inventory.location "
+ " and matusetrans.quantity is not null "
+ " group by matusetrans.itemnum, matusetrans.siteid, matusetrans.itemsetid, matusetrans.storeloc "
+ " ) as mut_qty, "
+ " ( "
+ " select coalesce(sum(matrectrans.quantity),0) as mrt_qty " 
+ " from matrectrans "
+ " where " + mrtDateSql
+ " and matrectrans.issuetype in (select value from synonymdomain where domainid='ISSUETYP' and maxvalue='TRANSFER') "
+ " and matrectrans.siteid = inventory.siteid " 
+ " and matrectrans.itemnum = inventory.itemnum "
+ " and matrectrans.itemsetid = inventory.itemsetid "
+ " and matrectrans.tostoreloc = inventory.location "
+ " and matrectrans.quantity is not null "
+ " group by matrectrans.itemnum, matrectrans.siteid, matrectrans.itemsetid, matrectrans.tostoreloc "
+ " ) as mrt_qty "
+ " from item, inventory "
+ " where inventory.itemsetid=item.itemsetid "
+ " and inventory.itemnum=item.itemnum "
+ " and item.itemtype in (select value from synonymdomain where domainid='ITEMTYPE' and maxvalue='ITEM') "
+ " and inventory.status not in (select value from synonymdomain where domainid='ITEMSTATUS' and maxvalue='OBSOLETE') "
+ " and item.status not in (select value from synonymdomain where domainid='ITEMSTATUS' and maxvalue='OBSOLETE') "
+ " and inventory.minlevel >= 0 "
+ " and " + params["where"]
+ " order by inventory.siteid, inventory.location, inventory.itemnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

var siteid = maximoDataSet.getString("siteid");
var itemsetid = maximoDataSet.getString("itemsetid");
var location = maximoDataSet.getString("location");
var itemnum = maximoDataSet.getString("itemnum");

row["itemnum"] = maximoDataSet.getString("itemnum");
row["description"] = maximoDataSet.getString("description");
row["orderunit"] = maximoDataSet.getString("orderunit");
row["minlevel"] = maximoDataSet.getString("minlevel");
row["sstock"] = maximoDataSet.getString("sstock");
row["costtype"] = maximoDataSet.getString("costtype");

var matusetransTotal = maximoDataSet.getInteger("mut_qty");
var matrectransTotal = maximoDataSet.getInteger("mrt_qty");
var deliverytime = maximoDataSet.getDouble("deliverytime");

//Caculations

// Lead Time = Average Monthly Usage * (INVENTORY.DELIVERYTIME / Average Days In a Month)
var avg_monthly_usage = Math.abs(matrectransTotal) + Math.abs(matusetransTotal); 
var avg_days_in_month = 30.44;
var leadtime = avg_monthly_usage * BirtMath.safeDivide(deliverytime, avg_days_in_month, 0);

// Safety Stock = (2  *  (Standard Deviation of Lead time)  *   (1.6 * Square Root of ( Lead time / Average Days In a Month)
var sstock = 2 * maximoDataSet.getDouble("deliverytime") * Math.pow( BirtMath.safeDivide(leadtime, avg_days_in_month, 0), (1/1.6) );

// Recommended ROP Value = Lead Time + Safety Stock
row["newrop"] = Math.ceil(leadtime + sstock);
row["newsstock"] = Math.ceil(sstock);

var diffInROP = row["newrop"] - row["minlevel"];
var diffInSStock = row["newsstock"] - row["sstock"]; 

if (diffInROP >= 0) {
	row["ropvalueinc"] = Math.abs(diffInROP);
} else {
	row["ropvaluedec"] = Math.abs(diffInROP);
}

if (diffInSStock >= 0) {
	row["ssvalueinc"] = Math.abs(diffInSStock);
} else {
	row["ssvaluedec"] = Math.abs(diffInSStock);
}

var request = reportContext.getHttpServletRequest();
if( BirtComp.notEqual(request,null) ) {
	var session = request.getSession();
	var updStmtList = session.getAttribute(attkey);

	if( BirtComp.equalTo(updStmtList,null) ){
		updStmtList = new Array;
	}
	
	var updateQuery = new String();
	
	var strSite = "";
	var strItem = "";
	var strItemset = "";
	var strLocation = "";
		
	if( BirtComp.notEqual(maximoDataSet.getString("siteid"),null)) {
		strSite = " = '" + maximoDataSet.getString("siteid") + "'";
	} else {
		strSite = " is null";
	}
		
	if( BirtComp.notEqual(maximoDataSet.getString("itemnum"),null)) {
		strItem = " = '" + maximoDataSet.getString("itemnum") + "'";
	} else {
		strItem = " is null";
	}
		
	if( BirtComp.notEqual(maximoDataSet.getString("itemsetid"),null)) {
		strItemset = " = '" + maximoDataSet.getString("itemsetid") + "'";
	} else {
		strItemset = " is null";
	}
	
	if( BirtComp.notEqual(maximoDataSet.getString("location"),null)) {
		strLocation = " = '" + maximoDataSet.getString("location") + "'";
	} else {
		strLocation = " is null";
	}
	
	if (BirtComp.equalTo(params["updatesafety"],mxReportScriptContext.getDisplayYesValue())) {
		updateQuery = "update inventory set minlevel=" + Math.ceil(leadtime + sstock)
		+ ", sstock=" +  Math.ceil(sstock)
		+ " where siteid " + strSite
		+ " and itemsetid " + strItemset
		+ " and location " + strLocation
		+ " and itemnum " + strItem
		;
	} else {
		updateQuery = "update inventory set minlevel=" + Math.ceil(leadtime + sstock)
		+ " where siteid " + strSite
		+ " and itemsetid " + strItemset
		+ " and location " + strLocation
		+ " and itemnum " + strItem
		;
	}
	
	if (scriptLogger.isDebugEnabled()) {
		scriptLogger.debug("["+maximoDataSet.getName() + "] >>> updateQuery: " + updateQuery);
	}
	updStmtList.push(updateQuery);
	
	session.setAttribute(attkey, updStmtList);
}

return (true);]]></method>
