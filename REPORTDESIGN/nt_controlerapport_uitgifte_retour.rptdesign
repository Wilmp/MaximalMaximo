row["iissuetype"] = maximoDataSet.getString("iissuetype");	
row["iquantity"] = maximoDataSet.getString("iquantity");	
row["istring"] = maximoDataSet.getString("istring");	
row["ritem"] = maximoDataSet.getString("ritem");	
