            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " "
+ " select " 
+ "   workorder.wonum, "
+ "   workorder.taskid, "
+ "   workorder.parent, "
+ "   workorder.status, "
+ "   workorder.worklocation, "
+ "   mu1.itemnum as iitem, "
+ "   mu1.storeloc as istoreloc, "
+ "   mu1.conditioncode as iconditioncode, "
+ "   mu1.issuetype as iissuetype, "
+ "   mu1.quantity as iquantity, "
+ "   workorder.wonum||workorder.taskid||mu1.itemnum as istring, "
+ "   mu2.itemnum as ritem, "
+ "   mu2.storeloc as rstoreloc, "
+ "   mu2.conditioncode as rconditioncode, "
+ "   mu2.issuetype as rissuetype, "
+ "   mu2.quantity as rquantity, "
+ "   workorder.wonum||workorder.taskid||mu2.itemnum as rstring, "
+ "   case  "
+ "     when  mu1.conditioncode = 'V' then 'Uitgifte van vuil deel' "
+ "     when  mu2.conditioncode = 'S' then 'Retour van schoon deel' "
+ "     when  mu1.itemnum <> mu2.itemnum then 'Verschil in artikelnummer' "
+ "     when  mu1.issuetype = 'ISSUE' and mu2.issuetype is null then 'Wel uitgifte, geen retour' " 
+ "     when  mu1.issuetype is null and mu2.issuetype ='RETURN' then 'Retour zonder uitgifte' "
+ "     when workorder.wonum||workorder.taskid||mu1.itemnum = workorder.wonum||workorder.taskid||mu2.itemnum and mu1.conditioncode = 'S' and mu2.conditioncode = 'V' then 'OK' "
//"     when workorder.wonum||mu1.itemnum = workorder.wonum||mu2.itemnum and mu1.conditioncode = 'S' and mu2.conditioncode = 'V' then 'OK' "
+ "     else  null "
+ "   end as remarks  "
+ " from workorder "
+ "   left outer join matusetrans mu1 on workorder.wonum = mu1.refwo and mu1.issuetype = 'ISSUE' "
//+ "   left outer join matusetrans mu2 on workorder.wonum = mu2.refwo and mu2.issuetype = 'RETURN' "
+ "   left outer join matusetrans mu2 on workorder.wonum = mu2.refwo and mu2.itemnum = mu1.itemnum and mu2.issuetype = 'RETURN' "
+ "   join item it on mu1.itemnum = it.itemnum or mu2.itemnum = it.itemnum"
// Include the Maximo where clause
+ " where " + params["where"]
+ " and it.conditionenabled = 1 "
+ " order by workorder.parent, workorder.wonum, workorder.taskid, it.itemnum "
;

maximoDataSet.setQuery(sqlText);]]></method>
