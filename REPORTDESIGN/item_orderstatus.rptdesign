            <method name="open"><![CDATA[maximoSubDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoSubDataSet.open();


if(!RegExp(/2=2/).test(params["paramstring"])){
	maximoSubDataSet.close();
}
else{

	var site = params["site"].replace(/[=!]/, "");
	var storeroom = params["storeroom"].replace(/[=!]/, "");
	
	// Add query to sqlText variable.
	var sqlText = " select distinct inventory.itemnum, item.description, prnum, prqty, prstatus, ponum, poqty, postatus "
	+ " from inventory  join item on inventory.itemnum = item.itemnum and inventory.itemsetid = item.itemsetid, ";
	if(maximoSubDataSet.isDB2()){
	sqlText += " ((select distinct prline.prnum prnum, prline.orderqty prqty, pr.status prstatus, cast(null as varchar(20)) ponum, cast(null as integer) poqty, cast(null as varchar(20)) postatus ";}
	else{
	sqlText += " ((select distinct prline.prnum prnum, prline.orderqty prqty, pr.status prstatus, null ponum, null poqty, null postatus ";}
	sqlText += " from prline, pr where prline.storeloc = ? and prline.itemnum = ? and prline.siteid = ?"
	+ " and pr.status in ('WAPPR', 'APPR') and prline.ponum is null and prline.prnum = pr.prnum and prline.siteid = pr.siteid ) union ";
	if(maximoSubDataSet.isDB2()){
	sqlText += " (select distinct cast(null as varchar(20)) prnum,  cast(null as integer) prqty,  cast(null as varchar(20)) prstatus, poline.ponum, poline.orderqty poqty, po.status postatus from poline, po ";} 
	else{
	sqlText += " (select distinct null, null, null, poline.ponum, poline.orderqty, po.status from poline, po ";} 
	sqlText += " where poline.storeloc = ?  and poline.itemnum = ? and poline.siteid = ?"
	+ " and po.status in ('WAPPR', 'APPR') and poline.ponum = po.ponum and poline.siteid = po.siteid )) ";
	if(!maximoSubDataSet.isOracle()){
				sqlText += " as insidetab ";}
	sqlText += " where inventory.location = ? and item.itemnum = ? and inventory.siteid = ? "
	+ " and " + params["where"]
	+ " order by inventory.itemnum ";

	maximoSubDataSet.setQuery(sqlText);
	
	maximoSubDataSet.setQueryParameterValue(1, storeroom);
	maximoSubDataSet.setQueryParameterValue(2, params["item"]);
	maximoSubDataSet.setQueryParameterValue(3, site);
	maximoSubDataSet.setQueryParameterValue(4, storeroom);
	maximoSubDataSet.setQueryParameterValue(5, params["item"]);
	maximoSubDataSet.setQueryParameterValue(6, site);	
	maximoSubDataSet.setQueryParameterValue(7, storeroom);
	maximoSubDataSet.setQueryParameterValue(8, params["item"]);
	maximoSubDataSet.setQueryParameterValue(9, site);	
	
	
	maximoSubDataSet.registerDataTranslation("description", "itemid", "ITEM", "DESCRIPTION");
}]]></method>
            <method name="fetch"><![CDATA[if (!maximoSubDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.

row["itemnum"] = maximoSubDataSet.getString("itemnum");
row["description"] = maximoSubDataSet.getString("description");
row["prnum"] = maximoSubDataSet.getString("prnum");
row["prqty"] = maximoSubDataSet.getString("prqty");
row["prstatus"] = maximoSubDataSet.getString("prstatus");	
row["ponum"] = maximoSubDataSet.getString("ponum");
row["poqty"] = maximoSubDataSet.getString("poqty");
row["postatus"] = maximoSubDataSet.getString("postatus");	

return (true);]]></method>
