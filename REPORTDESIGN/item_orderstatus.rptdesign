            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var newWhere = new String();
newWhere = params["where"];

if(params["site"].value){
	params["site"] = params["site"].toUpperCase();
	newWhere = newWhere + " and "
					+  MXReportSqlFormat.createParamWhereClause("inventory.siteid", params["site"]);
}

if(params["storeroom"].value){
	params["storeroom"] = params["storeroom"].toUpperCase();
	newWhere = newWhere + " and "
					+  MXReportSqlFormat.createParamWhereClause("inventory.location", params["storeroom"]);
}

var sqlText = new String();

// Add query to sqlText variable.
sqlText = " select inventory.itemnum, item.description, inventory.location, inventory.siteid from inventory "
+ " join item on inventory.itemnum = item.itemnum and inventory.itemsetid = item.itemsetid "
+ " where " + newWhere
+ " order by inventory.itemnum "
;

maximoDataSet.setQuery(sqlText);

maximoDataSet.registerDataTranslation("description", "itemid", "ITEM", "DESCRIPTION");]]></method>
            <method name="fetch"><![CDATA[var isValidRow = false;
while (isValidRow == false)
{
	if (!maximoDataSet.fetch())
		return (false);
	
	var itemnum = maximoDataSet.getString("itemnum");
	var site = maximoDataSet.getString("siteid");
	var storeroom = maximoDataSet.getString("location");
	
	row["itemnum"] = itemnum;
	row["description"] = maximoDataSet.getString("description");
	
	prDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "prDS");
	prDS.open();
	
	var sqlPrDS = " select count(distinct prline.prnum) prnum, sum(prline.orderqty) prqty from prline, pr "
	+ " where prline.storeloc = ? and prline.itemnum = ? and prline.siteid = ?"
	+ " and pr.status in ('WAPPR', 'APPR') and prline.ponum is null "
	+ " and prline.prnum = pr.prnum and prline.siteid = pr.siteid ";
	
	prDS.setQuery(sqlPrDS);
	prDS.setQueryParameterValue(1, storeroom);
	prDS.setQueryParameterValue(2, itemnum);
	prDS.setQueryParameterValue(3, site);
	
	if(prDS.fetch()) {
		row["prnum"] = prDS.getString("prnum");
		row["prqty"] = prDS.getString("prqty");
	}
	prDS.close();
	
	
	poDS = MXReportDataSetProvider.create(this.getDataSource().getName(), "poDS");
	poDS.open();
	
	var sqlPoDS = " select count(distinct poline.ponum) ponum, sum(poline.orderqty) poqty from poline, po "
	+ " where poline.storeloc = ? and poline.itemnum = ? and poline.siteid = ?"
	+ " and po.status in ('WAPPR', 'APPR') and poline.ponum = po.ponum and poline.siteid = po.siteid ";
	
	poDS.setQuery(sqlPoDS);
	poDS.setQueryParameterValue(1, storeroom);
	poDS.setQueryParameterValue(2, itemnum);
	poDS.setQueryParameterValue(3, site);
	
	if(poDS.fetch()) {
		row["ponum"] = poDS.getString("ponum");
		row["poqty"] = poDS.getString("poqty");
	}
	poDS.close();

	// Only include items that have outstanding orders, otherwise fetch the next row without writing this one
	if (row["poqty"]>0 || row["prqty"]>0)
		isValidRow = true;	
}

return (true);]]></method>
