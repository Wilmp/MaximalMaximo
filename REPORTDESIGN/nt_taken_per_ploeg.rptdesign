            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

// MPI 22-07-2013: ET # 3147 toevoegen kolom 'Reden toewijzingstatus' (wmassignment.plustinterptreason)
// MPI 12-08-2013: ET # 2332 toevoegen kolom 'spoor' (asset.ntloctrack) NB: niet altijd gevuld
// MPI 26-08-2013: ET # 3147 issue in rebound. Nothing changed in fact, only cosmetic  
// MPI 09-09-2013: ET # 3147 op verzoek Albert Janssen weer verwijderen kolom 'Reden toewijzingstatus' (wmassignment.plustinterptreason)
// MPI 18-09-2013: ET # 3640 indirecte uren inzichtelijk krijgen
// MPI 18-04-2014: ET # 5250 toevoeging kolommen dienstgroep en normtijd

var sqlText = new String();
var crewid    = new String();

params["crewid"]=params["crewid"].replace("=","");
crewid+= " labor.crewid = '" + params["crewid"] +"'"

// Add query to sqlText variable.
sqlText = " "
+ " select"
+ "   sub.ntloctrack as ntloctrack," 
+ "   wo.assetnum as assetnum,"
+ "   st.serialnum as serialnum," 
+ "   wa.parent as parent," 
+ "   wo.description as wodesc," 
+ "   wo.commoditygroup as commoditygroup," 
+ "   wm.status as status," 
+ "   wa.taskid as taskid," 
+ "   wa.description as taskdesc," 
+ "   wa.estdur as estdur," 
+ "   wm.assignstatus as assignstatus," 
+ "   wm.plustinterptreason as plustinterptreason,"
+ "   wm.laborcode as laborcode," 
+ "   null as assignmentstartdate, "
+ "   null as assignmenttranstype, "
+ "   labor.crewid as crewid, "
+ "   pe.displayname as displayname" 
+ " from "
+ "   labor  " 
+ "     join wmassignment wm  "
+ "       on  wm.laborcode = labor.laborcode " 
+ "     join person pe  "
+ "       on  pe.personid = wm.laborcode "
+ "     join woactivity wa  "
+ "       on  wa.wonum = wm.wonum "
+ "     join workorder wo  "
+ "       on  wo.wonum = wa.parent  "
+ "     join asset st "
+ "       on wo.assetnum = st.assetnum"
+ "     left join "  // added ET2332
+ "       ( "
+ "         select "  
+ "           sw.wonum, "   
+ "           sa.serialnum, " 
+ "           sa.description, "
+ "           sa.ntloctrack, "
+ "           sn.assetnum "  
+ "        from "  
+ "           asset sa, " 
+ "           workorder sw, " 
+ "           assetancestor sn "
+ "        where " 
+ "          sa.parent is null " 
+ "        and "  
+ "          sa.assetnum = sn.ancestor " 
+ "        and "
+ "          sn.assetnum = sw.assetnum " 
+ "        )" 
+ "        sub on st.assetnum = sub.assetnum "
+ "            and wo.wonum = sub.wonum " // added ET2332 
+ " where "
+   crewid
+ " and "
+ "   wm.laborcode is not null"
+ " and "
+ "   wm.assignstatus in('TOEGEWEZEN','GESTART','ONDERBROKEN') "
+ " union " 
+ " select"
+ "   null as ntloctrack," 
+ "   null as assetnum,"
+ "   null as serialnum," 
+ "   null as parent," 
+ "   null as wodesc," 
+ "   null as commoditygroup," 
+ "   null as status," 
+ "   null as taskid," 
+ "   'Indirecte taak' as taskdesc," 
+ "   null as estdur," 
+ "   'GESTART' as assignstatus," 
+ "   null as plustinterptreason,"
+ "   wm.laborcode as laborcode," 
+ "   labor.assignmentstartdate as assignmentstartdate, "
+ "   labor.assignmenttranstype as assignmenttranstype, "
+ "   null as crewid, "
+ "   pe.displayname as displayname" 
+ " from "
+ "   labor  " 
+ "     join wmassignment wm  "
+ "       on  wm.laborcode = labor.laborcode " 
+ "     join person pe  "
+ "       on  pe.personid = wm.laborcode "
+ "     join woactivity wa  "
+ "       on  wa.wonum = wm.wonum "
+ "     join workorder wo  "
+ "       on  wo.wonum = wa.parent  "
+ "     join asset st "
+ "       on wo.assetnum = st.assetnum"
+ "     left join "  // added ET2332
+ "       ( "
+ "         select "  
+ "           sw.wonum, "   
+ "           sa.serialnum, " 
+ "           sa.description, "
+ "           sa.ntloctrack, "
+ "           sn.assetnum "  
+ "        from "  
+ "           asset sa, " 
+ "           workorder sw, " 
+ "           assetancestor sn "
+ "        where " 
+ "          sa.parent is null " 
+ "        and "  
+ "          sa.assetnum = sn.ancestor " 
+ "        and "
+ "          sn.assetnum = sw.assetnum " 
+ "        )" 
+ "        sub on st.assetnum = sub.assetnum "
+ "            and wo.wonum = sub.wonum " // added ET2332 
+ " where "
+   crewid
+ " and "
+ "   wm.laborcode is not null"
+ " and "
+ "   labor.assignmentstartdate is not null "
+ " and "
+ "   wm.assignstatus in('TOEGEWEZEN','GESTART','ONDERBROKEN') "

//+ " order by wa.parent, wa.taskid " 
// Include the Maximo where clause
//+ " where " + params["where"]
;

maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

// Add a line for each output column
// The specific get method should match the data type of the output column.
row["ntloctrack"] = maximoDataSet.getString("ntloctrack");	
row["assetnum"] = maximoDataSet.getString("assetnum");	
row["serialnum"] = maximoDataSet.getString("serialnum");
row["parent"] = maximoDataSet.getString("parent");
row["wodesc"] = maximoDataSet.getString("wodesc");
row["status"] = maximoDataSet.getString("status");
row["taskid"] = maximoDataSet.getString("taskid");
row["taskdesc"] = maximoDataSet.getString("taskdesc");
row["assignstatus"] = maximoDataSet.getString("assignstatus");
row["plustinterptreason"] = maximoDataSet.getString("plustinterptreason");
row["laborcode"] = maximoDataSet.getString("laborcode");
row["assignmentstartdate"] = maximoDataSet.getTimestamp("assignmentstartdate");
row["assignmenttranstype"] = maximoDataSet.getString("assignmenttranstype");
row["crewid"] = maximoDataSet.getString("crewid");
row["displayname"] = maximoDataSet.getString("displayname");
row["commoditygroup"] = maximoDataSet.getString("commoditygroup");
row["estdur"] = maximoDataSet.getDuration("estdur");
//row["textfield"] = maximoDataSet.getString("textfield");

return (true);]]></method>
